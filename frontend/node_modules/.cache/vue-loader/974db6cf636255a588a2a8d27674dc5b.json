/script>\r\n\r\n<style scoped>\r\n.schedule-card {\r\n  margin-top: 10px;\r\n  max-width: 500px;\r\n  border-radius: 12px;\r\n  border: 1px solid #e0e0e0;\r\n}\r\n.card-header {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n  font-weight: bold;\r\n}\r\n.event-details p {\r\n  margin: 5px 0;\r\n}\r\n</style> "]}]}