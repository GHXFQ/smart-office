CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC;EAClB,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC;EAClB,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;EACrB,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;AACvB;;AAEA,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE;EAC7B,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;EACzB,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;AAChB;;AAEA,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE;EAC3B,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;EACzB,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;AAChB;;AAEA,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE;EACV,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC;AAClB;;AAEA,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE;EACb,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC;EACb,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC;EACR,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC;EAChB,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC;AACjB","file":"C:/Users/18083/Desktop/CCIT/office/smart-office/frontend/src/views/smartdoc/index.vue","sourceRoot":"","sourcesContent":["<template>\r\n  <div class=\"smartdoc-container\">\r\n    <div class=\"smartdoc-header\">\r\n      <h1 class=\"title\">智能文档系统</h1>\r\n      <div class=\"search-container\">\r\n        <el-input\r\n          v-model=\"searchQuery\"\r\n          placeholder=\"使用自然语言搜索您的文档...\"\r\n          prefix-icon=\"el-icon-search\"\r\n          clearable\r\n          @keyup.enter.native=\"searchDocuments\"\r\n        >\r\n          <template #append>\r\n            <el-button @click=\"searchDocuments\">搜索</el-button>\r\n          </template>\r\n        </el-input>\r\n      </div>\r\n      <div class=\"actions\">\r\n        <el-button type=\"primary\" @click=\"createDocument\">\r\n          <i class=\"el-icon-plus\"></i> 新建文档\r\n        </el-button>\r\n        <el-upload\r\n          class=\"upload-btn\"\r\n          action=\"#\"\r\n          :http-request=\"uploadDocument\"\r\n          :show-file-list=\"false\"\r\n          accept=\".docx,.pdf,.txt,.md\"\r\n        >\r\n          <el-button type=\"success\">\r\n            <i class=\"el-icon-upload\"></i> 上传文档\r\n          </el-button>\r\n        </el-upload>\r\n      </div>\r\n    </div>\r\n\r\n    <div class=\"smartdoc-content\">\r\n      <div class=\"sidebar\">\r\n        <el-menu\r\n          default-active=\"recent\"\r\n          class=\"doc-menu\"\r\n          @select=\"handleMenuSelect\"\r\n        >\r\n          <el-menu-item index=\"recent\">\r\n            <i class=\"el-icon-time\"></i>\r\n            <span>最近文档</span>\r\n          </el-menu-item>\r\n          <el-menu-item index=\"favorites\">\r\n            <i class=\"el-icon-star-on\"></i>\r\n            <span>收藏文档</span>\r\n          </el-menu-item>\r\n          <el-menu-item index=\"shared\">\r\n            <i class=\"el-icon-share\"></i>\r\n            <span>共享文档</span>\r\n          </el-menu-item>\r\n          <el-submenu index=\"categories\">\r\n            <template #title>\r\n              <i class=\"el-icon-folder\"></i>\r\n              <span>分类文档</span>\r\n            </template>\r\n            <el-menu-item v-for=\"category in categories\" :key=\"category.id\" :index=\"`category-${category.id}`\">\r\n              {{ category.name }}\r\n            </el-menu-item>\r\n          </el-submenu>\r\n          <el-menu-item index=\"trash\">\r\n            <i class=\"el-icon-delete\"></i>\r\n            <span>回收站</span>\r\n          </el-menu-item>\r\n        </el-menu>\r\n        \r\n        <div class=\"smart-features\">\r\n          <h3>智能助手</h3>\r\n          <el-button @click=\"openAssistant\" class=\"assistant-btn\">\r\n            <i class=\"el-icon-chat-dot-round\"></i> 智能问答\r\n          </el-button>\r\n          <div class=\"insight-section\">\r\n            <h4>知识洞察</h4>\r\n            <div v-for=\"(insight, index) in insights\" :key=\"index\" class=\"insight-item\">\r\n              <i class=\"el-icon-info\"></i>\r\n              <span>{{ insight.title }}</span>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <div class=\"main-content\">\r\n        <el-tabs v-model=\"activeTab\" class=\"view-tabs\">\r\n          <el-tab-pane label=\"所有文档\" name=\"all\">\r\n            <document-list \r\n              :documents=\"filteredDocuments\" \r\n              @view=\"viewDocument\"\r\n              @edit=\"editDocument\"\r\n              @delete=\"deleteDocument\"\r\n              @favorite=\"toggleFavorite\"\r\n            />\r\n          </el-tab-pane>\r\n          <el-tab-pane label=\"我创建的\" name=\"created\">\r\n            <document-list \r\n              :documents=\"createdDocuments\" \r\n              @view=\"viewDocument\"\r\n              @edit=\"editDocument\"\r\n              @delete=\"deleteDocument\"\r\n              @favorite=\"toggleFavorite\"\r\n            />\r\n          </el-tab-pane>\r\n          <el-tab-pane label=\"与我共享的\" name=\"shared\">\r\n            <document-list \r\n              :documents=\"sharedDocuments\" \r\n              @view=\"viewDocument\"\r\n              @edit=\"editDocument\"\r\n              @favorite=\"toggleFavorite\"\r\n            />\r\n          </el-tab-pane>\r\n        </el-tabs>\r\n\r\n        <div v-if=\"loading\" class=\"loading-container\">\r\n          <el-skeleton :rows=\"10\" animated />\r\n        </div>\r\n        \r\n        <div v-else-if=\"filteredDocuments.length === 0\" class=\"empty-state\">\r\n          <i class=\"el-icon-document\"></i>\r\n          <p>暂无文档，请创建或上传新文档</p>\r\n        </div>\r\n      </div>\r\n\r\n      <div class=\"knowledge-panel\" v-if=\"showKnowledgePanel\">\r\n        <div class=\"panel-header\">\r\n          <h3>知识关联</h3>\r\n          <el-button type=\"text\" @click=\"showKnowledgePanel = false\">\r\n            <i class=\"el-icon-close\"></i>\r\n          </el-button>\r\n        </div>\r\n        <div class=\"panel-content\">\r\n          <div v-if=\"relatedDocuments.length\">\r\n            <h4>相关文档</h4>\r\n            <ul class=\"related-list\">\r\n              <li v-for=\"doc in relatedDocuments\" :key=\"doc.id\" @click=\"viewDocument(doc)\">\r\n                <i :class=\"getDocumentIcon(doc.type)\"></i>\r\n                <span class=\"doc-title\">{{ doc.title }}</span>\r\n              </li>\r\n            </ul>\r\n          </div>\r\n          <div v-if=\"recentInsights.length\">\r\n            <h4>文档洞察</h4>\r\n            <div v-for=\"insight in recentInsights\" :key=\"insight.id\" class=\"insight-block\">\r\n              <p>{{ insight.content }}</p>\r\n              <el-tag size=\"small\">{{ insight.category }}</el-tag>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- 文档创建/编辑对话框 -->\r\n    <el-dialog\r\n      :title=\"dialogMode === 'create' ? '创建文档' : '编辑文档'\"\r\n      :visible.sync=\"dialogVisible\"\r\n      width=\"50%\"\r\n    >\r\n      <el-form :model=\"documentForm\" label-width=\"80px\">\r\n        <el-form-item label=\"标题\">\r\n          <el-input v-model=\"documentForm.title\" placeholder=\"请输入文档标题\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"分类\">\r\n          <el-select v-model=\"documentForm.categoryId\" placeholder=\"选择分类\">\r\n            <el-option\r\n              v-for=\"category in categories\"\r\n              :key=\"category.id\"\r\n              :label=\"category.name\"\r\n              :value=\"category.id\"\r\n            ></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item label=\"描述\">\r\n          <el-input\r\n            type=\"textarea\"\r\n            v-model=\"documentForm.description\"\r\n            rows=\"3\"\r\n            placeholder=\"请输入文档描述\"\r\n          ></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"权限\">\r\n          <el-radio-group v-model=\"documentForm.permission\">\r\n            <el-radio :label=\"'private'\">私有</el-radio>\r\n            <el-radio :label=\"'team'\">团队</el-radio>\r\n            <el-radio :label=\"'public'\">公开</el-radio>\r\n          </el-radio-group>\r\n        </el-form-item>\r\n        <el-form-item label=\"内容\" v-if=\"dialogMode === 'create'\">\r\n          <div class=\"editor-wrapper\">\r\n            <!-- 这里可以集成富文本编辑器 -->\r\n            <el-input\r\n              type=\"textarea\"\r\n              v-model=\"documentForm.content\"\r\n              rows=\"10\"\r\n              placeholder=\"编写文档内容或使用AI助手生成\"\r\n            ></el-input>\r\n          </div>\r\n        </el-form-item>\r\n      </el-form>\r\n      <span slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"dialogVisible = false\">取消</el-button>\r\n        <el-button type=\"primary\" @click=\"saveDocument\">保存</el-button>\r\n      </span>\r\n    </el-dialog>\r\n\r\n    <!-- AI助手对话框 -->\r\n    <el-dialog\r\n      title=\"智能文档助手\"\r\n      :visible.sync=\"assistantVisible\"\r\n      width=\"60%\"\r\n      class=\"assistant-dialog\"\r\n    >\r\n      <div class=\"assistant-container\">\r\n        <div class=\"chat-messages\">\r\n          <div v-for=\"(message, index) in assistantMessages\" :key=\"index\" \r\n            :class=\"['message', message.type === 'user' ? 'user-message' : 'ai-message']\">\r\n            <div class=\"message-content\">{{ message.content }}</div>\r\n          </div>\r\n        </div>\r\n        <div class=\"chat-input\">\r\n          <el-input\r\n            v-model=\"assistantQuery\"\r\n            placeholder=\"问些文档相关的问题，如'总结这份文档'或'生成一份报告提纲'...\"\r\n            @keyup.enter.native=\"askAssistant\"\r\n          >\r\n            <template #append>\r\n              <el-button @click=\"askAssistant\">发送</el-button>\r\n            </template>\r\n          </el-input>\r\n          <div class=\"quick-actions\">\r\n            <el-button size=\"mini\" @click=\"useQuickAction('summarize')\">总结文档</el-button>\r\n            <el-button size=\"mini\" @click=\"useQuickAction('extract')\">提取关键点</el-button>\r\n            <el-button size=\"mini\" @click=\"useQuickAction('generate')\">生成报告</el-button>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'SmartDocumentPage',\r\n  components: {\r\n    // 假设已有DocumentList组件\r\n    DocumentList: () => import('./components/DocumentList.vue')\r\n  },\r\n  data() {\r\n    return {\r\n      searchQuery: '',\r\n      documents: [],\r\n      filteredDocuments: [],\r\n      createdDocuments: [],\r\n      sharedDocuments: [],\r\n      categories: [\r\n        { id: 1, name: '会议记录' },\r\n        { id: 2, name: '项目计划' },\r\n        { id: 3, name: '技术文档' },\r\n        { id: 4, name: '合同协议' },\r\n        { id: 5, name: '研究报告' }\r\n      ],\r\n      loading: true,\r\n      activeTab: 'all',\r\n      showKnowledgePanel: true,\r\n      relatedDocuments: [],\r\n      recentInsights: [],\r\n      insights: [\r\n        { id: 1, title: '本周热门文档分析' },\r\n        { id: 2, title: '文档知识图谱更新' },\r\n        { id: 3, title: '重要内容变更提醒' }\r\n      ],\r\n      dialogVisible: false,\r\n      dialogMode: 'create', // create 或 edit\r\n      documentForm: {\r\n        title: '',\r\n        categoryId: null,\r\n        description: '',\r\n        content: '',\r\n        permission: 'private'\r\n      },\r\n      assistantVisible: false,\r\n      assistantQuery: '',\r\n      assistantMessages: [\r\n        { type: 'ai', content: '您好！我是智能文档助手，有什么可以帮助您的？您可以询问文档内容，请求总结，或者生成报告提纲等。' }\r\n      ],\r\n      selectedDocument: null\r\n    };\r\n  },\r\n  mounted() {\r\n    this.fetchDocuments();\r\n  },\r\n  methods: {\r\n    fetchDocuments() {\r\n      // 模拟获取文档数据\r\n      setTimeout(() => {\r\n        this.documents = [\r\n          {\r\n            id: 1,\r\n            title: '2023年度战略规划',\r\n            description: '公司年度战略目标与执行计划',\r\n            createdAt: '2023-01-15',\r\n            updatedAt: '2023-01-20',\r\n            owner: { id: 1, name: '张三' },\r\n            category: { id: 2, name: '项目计划' },\r\n            isFavorite: true,\r\n            type: 'docx',\r\n            permission: 'team'\r\n          },\r\n          {\r\n            id: 2,\r\n            title: '产品需求分析报告',\r\n            description: '基于用户反馈的产品迭代需求分析',\r\n            createdAt: '2023-02-05',\r\n            updatedAt: '2023-02-10',\r\n            owner: { id: 1, name: '张三' },\r\n            category: { id: 5, name: '研究报告' },\r\n            isFavorite: false,\r\n            type: 'pdf',\r\n            permission: 'private'\r\n          },\r\n          {\r\n            id: 3,\r\n            title: '技术架构设计文档',\r\n            description: '系统架构与技术选型说明',\r\n            createdAt: '2023-03-12',\r\n            updatedAt: '2023-03-15',\r\n            owner: { id: 2, name: '李四' },\r\n            category: { id: 3, name: '技术文档' },\r\n            isFavorite: false,\r\n            type: 'md',\r\n            permission: 'team'\r\n          }\r\n        ];\r\n        \r\n        this.filteredDocuments = [...this.documents];\r\n        this.createdDocuments = this.documents.filter(doc => doc.owner.id === 1);\r\n        this.sharedDocuments = this.documents.filter(doc => doc.permission !== 'private' && doc.owner.id !== 1);\r\n        \r\n        this.relatedDocuments = [\r\n          {\r\n            id: 4,\r\n            title: '上季度业绩报告',\r\n            type: 'pdf'\r\n          },\r\n          {\r\n            id: 5,\r\n            title: '竞品分析文档',\r\n            type: 'docx'\r\n          }\r\n        ];\r\n        \r\n        this.recentInsights = [\r\n          { id: 1, content: '该文档与公司季度目标相关度为 85%', category: '内容关联' },\r\n          { id: 2, content: '近期有3位同事查阅了此文档', category: '协作信息' }\r\n        ];\r\n        \r\n        this.loading = false;\r\n      }, 1000);\r\n    },\r\n    searchDocuments() {\r\n      if (!this.searchQuery.trim()) {\r\n        this.filteredDocuments = [...this.documents];\r\n        return;\r\n      }\r\n      \r\n      // 模拟自然语言搜索\r\n      this.loading = true;\r\n      setTimeout(() => {\r\n        // 简单模拟，实际应通过API调用大模型处理自然语言查询\r\n        this.filteredDocuments = this.documents.filter(doc => \r\n          doc.title.toLowerCase().includes(this.searchQuery.toLowerCase()) ||\r\n          doc.description.toLowerCase().includes(this.searchQuery.toLowerCase())\r\n        );\r\n        this.loading = false;\r\n      }, 800);\r\n    },\r\n    handleMenuSelect(key) {\r\n      this.loading = true;\r\n      setTimeout(() => {\r\n        if (key === 'recent') {\r\n          this.filteredDocuments = [...this.documents].sort((a, b) => \r\n            new Date(b.updatedAt) - new Date(a.updatedAt)\r\n          );\r\n        } else if (key === 'favorites') {\r\n          this.filteredDocuments = this.documents.filter(doc => doc.isFavorite);\r\n        } else if (key === 'shared') {\r\n          this.filteredDocuments = this.documents.filter(doc => \r\n            doc.permission !== 'private' && doc.owner.id !== 1\r\n          );\r\n        } else if (key.startsWith('category-')) {\r\n          const categoryId = parseInt(key.split('-')[1]);\r\n          this.filteredDocuments = this.documents.filter(doc => \r\n            doc.category.id === categoryId\r\n          );\r\n        } else if (key === 'trash') {\r\n          this.filteredDocuments = []; // 假设回收站为空\r\n        }\r\n        this.loading = false;\r\n      }, 500);\r\n    },\r\n    createDocument() {\r\n      this.dialogMode = 'create';\r\n      this.documentForm = {\r\n        title: '',\r\n        categoryId: null,\r\n        description: '',\r\n        content: '',\r\n        permission: 'private'\r\n      };\r\n      this.dialogVisible = true;\r\n    },\r\n    editDocument(document) {\r\n      this.dialogMode = 'edit';\r\n      this.documentForm = {\r\n        id: document.id,\r\n        title: document.title,\r\n        categoryId: document.category.id,\r\n        description: document.description,\r\n        permission: document.permission\r\n      };\r\n      this.dialogVisible = true;\r\n    },\r\n    saveDocument() {\r\n      // 模拟保存文档\r\n      if (this.dialogMode === 'create') {\r\n        const newDoc = {\r\n          id: Date.now(),\r\n          title: this.documentForm.title,\r\n          description: this.documentForm.description,\r\n          createdAt: new Date().toISOString().split('T')[0],\r\n          updatedAt: new Date().toISOString().split('T')[0],\r\n          owner: { id: 1, name: '张三' },\r\n          category: this.categories.find(c => c.id === this.documentForm.categoryId) || this.categories[0],\r\n          isFavorite: false,\r\n          type: 'docx',\r\n          permission: this.documentForm.permission\r\n        };\r\n        \r\n        this.documents.unshift(newDoc);\r\n        this.filteredDocuments = [...this.documents];\r\n      } else {\r\n        // 编辑现有文档\r\n        const index = this.documents.findIndex(doc => doc.id === this.documentForm.id);\r\n        if (index !== -1) {\r\n          this.documents[index] = {\r\n            ...this.documents[index],\r\n            title: this.documentForm.title,\r\n            description: this.documentForm.description,\r\n            updatedAt: new Date().toISOString().split('T')[0],\r\n            category: this.categories.find(c => c.id === this.documentForm.categoryId) || this.documents[index].category,\r\n            permission: this.documentForm.permission\r\n          };\r\n          this.filteredDocuments = [...this.documents];\r\n        }\r\n      }\r\n      \r\n      this.dialogVisible = false;\r\n    },\r\n    viewDocument(document) {\r\n      this.selectedDocument = document;\r\n      // 模拟文档查看逻辑\r\n      console.log('查看文档:', document.title);\r\n    },\r\n    deleteDocument(document) {\r\n      // 模拟删除文档\r\n      const index = this.documents.findIndex(doc => doc.id === document.id);\r\n      if (index !== -1) {\r\n        this.documents.splice(index, 1);\r\n        this.filteredDocuments = this.filteredDocuments.filter(doc => doc.id !== document.id);\r\n      }\r\n    },\r\n    toggleFavorite(document) {\r\n      // 模拟收藏/取消收藏\r\n      const index = this.documents.findIndex(doc => doc.id === document.id);\r\n      if (index !== -1) {\r\n        this.documents[index].isFavorite = !this.documents[index].isFavorite;\r\n        this.filteredDocuments = [...this.documents];\r\n      }\r\n    },\r\n    uploadDocument(options) {\r\n      // 模拟文档上传\r\n      const file = options.file;\r\n      console.log('上传文件:', file.name);\r\n      \r\n      // 模拟上传成功后添加到文档列表\r\n      setTimeout(() => {\r\n        const newDoc = {\r\n          id: Date.now(),\r\n          title: file.name.split('.')[0],\r\n          description: '上传的文档',\r\n          createdAt: new Date().toISOString().split('T')[0],\r\n          updatedAt: new Date().toISOString().split('T')[0],\r\n          owner: { id: 1, name: '张三' },\r\n          category: this.categories[0],\r\n          isFavorite: false,\r\n          type: file.name.split('.').pop(),\r\n          permission: 'private'\r\n        };\r\n        \r\n        this.documents.unshift(newDoc);\r\n        this.filteredDocuments = [...this.documents];\r\n      }, 1000);\r\n    },\r\n    openAssistant() {\r\n      this.assistantVisible = true;\r\n    },\r\n    askAssistant() {\r\n      if (!this.assistantQuery.trim()) return;\r\n      \r\n      // 添加用户消息\r\n      this.assistantMessages.push({\r\n        type: 'user',\r\n        content: this.assistantQuery\r\n      });\r\n      \r\n      // 模拟AI回复\r\n      setTimeout(() => {\r\n        let response = '';\r\n        \r\n        if (this.assistantQuery.includes('总结')) {\r\n          response = '根据文档内容分析，主要包含以下几个方面：\\n1. 项目背景与目标\\n2. 市场需求分析\\n3. 技术实施方案\\n4. 风险评估与应对策略\\n5. 时间线与里程碑';\r\n        } else if (this.assistantQuery.includes('提取')) {\r\n          response = '已为您提取关键信息：\\n• 项目预计在6月完成第一阶段\\n• 核心技术采用大模型与知识图谱结合\\n• 目标用户群体为企业中高层管理者\\n• 主要竞品有三家，我们的差异化优势在于...';\r\n        } else if (this.assistantQuery.includes('生成')) {\r\n          response = '已为您生成报告提纲：\\n\\n## 执行摘要\\n\\n## 项目背景\\n\\n## 需求分析\\n\\n## 解决方案\\n\\n## 实施计划\\n\\n## 预期成果\\n\\n## 风险管理';\r\n        } else {\r\n          response = '我已分析您的问题。这个文档主要讨论了智能办公系统的设计方案，包括技术架构、功能规划和实施路径。您可以询问更具体的内容，我会提供相关信息。';\r\n        }\r\n        \r\n        this.assistantMessages.push({\r\n          type: 'ai',\r\n          content: response\r\n        });\r\n        \r\n        this.assistantQuery = '';\r\n      }, 1000);\r\n    },\r\n    useQuickAction(action) {\r\n      let query = '';\r\n      \r\n      switch (action) {\r\n        case 'summarize':\r\n          query = '请总结这份文档的主要内容';\r\n          break;\r\n        case 'extract':\r\n          query = '提取这份文档的关键信息和数据';\r\n          break;\r\n        case 'generate':\r\n          query = '基于这份文档生成一份完整的报告提纲';\r\n          break;\r\n      }\r\n      \r\n      this.assistantQuery = query;\r\n      this.askAssistant();\r\n    },\r\n    getDocumentIcon(type) {\r\n      const iconMap = {\r\n        'pdf': 'el-icon-document',\r\n        'docx': 'el-icon-document-word',\r\n        'xlsx': 'el-icon-document-excel',\r\n        'pptx': 'el-icon-document-ppt',\r\n        'md': 'el-icon-document-text'\r\n      };\r\n      \r\n      return iconMap[type] || 'el-icon-document';\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.smartdoc-container {\r\n  height: 100%;\r\n  display: flex;\r\n  flex-direction: column;\r\n}\r\n\r\n.smartdoc-header {\r\n  padding: 16px 24px;\r\n  border-bottom: 1px solid #eaeaea;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: space-between;\r\n}\r\n\r\n.smartdoc-header .title {\r\n  font-size: 20px;\r\n  margin: 0;\r\n  color: #303133;\r\n  flex-shrink: 0;\r\n}\r\n\r\n.search-container {\r\n  flex-grow: 1;\r\n  max-width: 600px;\r\n  margin: 0 24px;\r\n}\r\n\r\n.actions {\r\n  display: flex;\r\n  gap: 12px;\r\n}\r\n\r\n.smartdoc-content {\r\n  display: flex;\r\n  flex-grow: 1;\r\n  height: calc(100vh - 120px);\r\n  overflow: hidden;\r\n}\r\n\r\n.sidebar {\r\n  width: 250px;\r\n  border-right: 1px solid #eaeaea;\r\n  display: flex;\r\n  flex-direction: column;\r\n  background-color: #f9f9f9;\r\n}\r\n\r\n.doc-menu {\r\n  border-right: none;\r\n}\r\n\r\n.smart-features {\r\n  padding: 16px;\r\n  flex-grow: 1;\r\n  border-top: 1px solid #eaeaea;\r\n}\r\n\r\n.smart-features h3 {\r\n  margin-top: 0;\r\n  color: #606266;\r\n  font-size: 14px;\r\n  margin-bottom: 12px;\r\n}\r\n\r\n.assistant-btn {\r\n  width: 100%;\r\n  margin-bottom: 16px;\r\n}\r\n\r\n.insight-section h4 {\r\n  font-size: 13px;\r\n  margin: 16px 0 8px;\r\n  color: #606266;\r\n}\r\n\r\n.insight-item {\r\n  padding: 8px;\r\n  border-radius: 4px;\r\n  background-color: #f2f6fc;\r\n  margin-bottom: 8px;\r\n  font-size: 12px;\r\n  cursor: pointer;\r\n  transition: background-color 0.2s;\r\n}\r\n\r\n.insight-item:hover {\r\n  background-color: #e6effd;\r\n}\r\n\r\n.main-content {\r\n  flex-grow: 1;\r\n  padding: 16px 24px;\r\n  overflow-y: auto;\r\n}\r\n\r\n.view-tabs {\r\n  margin-bottom: 16px;\r\n}\r\n\r\n.loading-container {\r\n  padding: 24px 0;\r\n}\r\n\r\n.empty-state {\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n  justify-content: center;\r\n  height: 300px;\r\n  color: #909399;\r\n}\r\n\r\n.empty-state i {\r\n  font-size: 48px;\r\n  margin-bottom: 16px;\r\n}\r\n\r\n.knowledge-panel {\r\n  width: 280px;\r\n  border-left