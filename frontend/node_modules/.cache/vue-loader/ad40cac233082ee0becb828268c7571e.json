\n          <el-option v-for=\"type in docTypes\" :key=\"type\" :label=\"type\" :value=\"type\" />\r\n        </el-select>\r\n        <el-select v-model=\"sortOption\" placeholder=\"排序方式\">\r\n          <el-option label=\"最近更新\" value=\"update\" />\r\n          <el-option label=\"最近创建\" value=\"create\" />\r\n          <el-option label=\"名称升序\" value=\"name-asc\" />\r\n          <el-option label=\"名称降序\" value=\"name-desc\" />\r\n        </el-select>\r\n        <el-select v-model=\"viewMode\" class=\"view-mode-select\">\r\n          <el-option label=\"网格视图\" value=\"grid\">\r\n            <el-icon><Grid /></el-icon> 网格视图\r\n          </el-option>\r\n          <el-option label=\"列表视图\" value=\"list\">\r\n            <el-icon><List /></el-icon> 列表视图\r\n          </el-option>\r\n        </el-select>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- 文档列表区域 - 网格视图 -->\r\n    <div v-if=\"viewMode === 'grid'\" class=\"document-grid\">\r\n      <el-row :gutter=\"16\">\r\n        <el-col :xs=\"24\" :sm=\"12\" :md=\"8\" :lg=\"6\" :xl=\"4\" \r\n          v-for=\"doc in filteredDocuments\" \r\n          :key=\"doc.id\"\r\n          class=\"grid-item-col\"\r\n        >\r\n          <div class=\"document-card\" @click=\"openDocument(doc)\">\r\n            <div class=\"card-thumbnail\" :style=\"{ backgroundColor: getRandomColor(doc.type) }\">\r\n              <el-icon><Document /></el-icon>\r\n            </div>\r\n            <div class=\"card-content\">\r\n              <h3 class=\"doc-title\" :title=\"doc.title\">{{ doc.title }}</h3>\r\n              <div class=\"doc-meta\">\r\n                <span class=\"doc-type\">{{ doc.type }}</span>\r\n                <span class=\"doc-date\">{{ formatDate(doc.updateTime) }}</span>\r\n              </div>\r\n              <div class=\"doc-preview\">{{ getDocumentPreview(doc) }}</div>\r\n            </div>\r\n            <div class=\"card-actions\">\r\n              <el-dropdown trigger=\"click\" @click.stop>\r\n                <el-button text circle class=\"action-btn\">\r\n                  <el-icon><More /></el-icon>\r\n                </el-button>\r\n                <template #dropdown>\r\n                  <el-dropdown-menu>\r\n                    <el-dropdown-item @click.stop=\"openDocument(doc)\">\r\n                      <el-icon><Edit /></el-icon> 编辑\r\n                    </el-dropdown-item>\r\n                    <el-dropdown-item @click.stop=\"duplicateDocument(doc)\">\r\n                      <el-icon><CopyDocument /></el-icon> 复制\r\n                    </el-dropdown-item>\r\n                    <el-dropdown-item @click.stop=\"shareDocument(doc)\">\r\n                      <el-icon><Share /></el-icon> 分享\r\n                    </el-dropdown-item>\r\n                    <el-dropdown-item @click.stop=\"exportDocument(doc)\">\r\n                      <el-icon><Download /></el-icon> 导出\r\n                    </el-dropdown-item>\r\n                    <el-dropdown-item divided @click.stop=\"deleteDocument(doc)\" class=\"danger-item\">\r\n                      <el-icon color=\"#F56C6C\"><Delete /></el-icon> 删除\r\n                    </el-dropdown-item>\r\n                  </el-dropdown-menu>\r\n                </template>\r\n              </el-dropdown>\r\n            </div>\r\n          </div>\r\n        </el-col>\r\n      </el-row>\r\n    </div>\r\n\r\n    <!-- 文档列表区域 - 列表视图 -->\r\n    <div v-else class=\"document-list\">\r\n      <el-table \r\n        :data=\"filteredDocuments\" \r\n        style=\"width: 100%\" \r\n        @row-click=\"openDocument\"\r\n      >\r\n        <el-table-column width=\"50\">\r\n          <template #default>\r\n            <el-icon><Document /></el-icon>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column prop=\"title\" label=\"文档标题\" min-width=\"250\" show-overflow-tooltip />\r\n        <el-table-column prop=\"type\" label=\"类型\" width=\"120\" />\r\n        <el-table-column label=\"创建时间\" width=\"180\">\r\n          <template #default=\"scope\">\r\n            {{ formatDate(scope.row.createTime) }}\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column label=\"更新时间\" width=\"180\">\r\n          <template #default=\"scope\">\r\n            {{ formatDate(scope.row.updateTime) }}\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column label=\"操作\" width=\"150\" fixed=\"right\">\r\n          <template #default=\"scope\">\r\n            <el-button-group>\r\n              <el-button size=\"small\" text @click.stop=\"openDocument(scope.row)\">\r\n                <el-icon><Edit /></el-icon>\r\n              </el-button>\r\n              <el-button size=\"small\" text @click.stop=\"shareDocument(scope.row)\">\r\n                <el-icon><Share /></el-icon>\r\n              </el-button>\r\n              <el-dropdown trigger=\"click\" @click.stop>\r\n                <el-button size=\"small\" text>\r\n                  <el-icon><More /></el-icon>\r\n                </el-button>\r\n                <template #dropdown>\r\n                  <el-dropdown-menu>\r\n                    <el-dropdown-item @click.stop=\"duplicateDocument(scope.row)\">复制</el-dropdown-item>\r\n                    <el-dropdown-item @click.stop=\"exportDocument(scope.row)\">导出</el-dropdown-item>\r\n                    <el-dropdown-item @click.stop=\"deleteDocument(scope.row)\" class=\"danger-item\">删除</el-dropdown-item>\r\n                  </el-dropdown-menu>\r\n                </template>\r\n              </el-dropdown>\r\n            </el-button-group>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n    </div>\r\n\r\n    <!-- 无文档时的提示 -->\r\n    <div v-if=\"filteredDocuments.length === 0\" class=\"empty-state\">\r\n      <el-empty description=\"暂无文档\">\r\n        <el-button type=\"primary\" @click=\"createNewDocument\">创建新文档</el-button>\r\n      </el-empty>\r\n    </div>\r\n\r\n    <!-- 创建新文档对话框 -->\r\n    <el-dialog \r\n      v-model=\"newDocumentDialogVisible\" \r\n      title=\"创建新文档\" \r\n      width=\"500px\"\r\n    >\r\n      <el-form :model=\"newDocumentForm\" label-width=\"80px\">\r\n        <el-form-item label=\"文档标题\" required>\r\n          <el-input v-model=\"newDocumentForm.title\" placeholder=\"请输入文档标题\" />\r\n        </el-form-item>\r\n        <el-form-item label=\"文档类型\">\r\n          <el-select v-model=\"newDocumentForm.type\" placeholder=\"选择或输入文档类型\" filterable allow-create>\r\n            <el-option v-for=\"type in docTypes\" :key=\"type\" :label=\"type\" :value=\"type\" />\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item label=\"文档模板\">\r\n          <el-select v-model=\"newDocumentForm.template\" placeholder=\"选择文档模板\" clearable>\r\n            <el-option label=\"空白文档\" value=\"blank\" />\r\n            <el-option label=\"会议记录\" value=\"meeting\" />\r\n            <el-option label=\"项目计划\" value=\"project\" />\r\n            <el-option label=\"周报模板\" value=\"weekly\" />\r\n          </el-select>\r\n        </el-form-item>\r\n      </el-form>\r\n      <template #footer>\r\n        <div class=\"dialog-footer\">\r\n          <el-button @click=\"newDocumentDialogVisible = false\">取消</el-button>\r\n          <el-button type=\"primary\" @click=\"confirmCreateDocument\">确认创建</el-button>\r\n        </div>\r\n      </template>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { ref, computed, onMounted } from 'vue'\r\nimport { useRouter } from 'vue-router'\r\nimport { \r\n  Document, Plus, Upload, Setting, ElIconSearch, \r\n  Grid, List, More, Edit, CopyDocument, Share, Download, Delete\r\n} from '@element-plus/icons-vue'\r\nimport { ElMessage, ElMessageBox } from 'element-plus'\r\n\r\nexport default {\r\n  name: 'SmartDocHome',\r\n  components: {\r\n    Document, Plus, Upload, Setting, ElIconSearch,\r\n    Grid, List, More, Edit, CopyDocument, Share, Download, Delete\r\n  },\r\n  setup() {\r\n    const router = useRouter()\r\n    \r\n    // 文档列表和筛选状态\r\n    const documents = ref([])\r\n    const searchQuery = ref('')\r\n    const docTypeFilter = ref('')\r\n    const sortOption = ref('update')\r\n    const viewMode = ref('grid')\r\n    \r\n    // 新建文档对话框\r\n    const newDocumentDialogVisible = ref(false)\r\n    const newDocumentForm = ref({\r\n      title: '',\r\n      type: '',\r\n      template: 'blank'\r\n    })\r\n    \r\n    // 文档类型列表\r\n    const docTypes = ref(['报告', '会议记录', '规划文档', '合同文档', '周报', '其他'])\r\n    \r\n    // 获取文档列表\r\n    const fetchDocuments = () => {\r\n      // 模拟API调用获取文档列表\r\n      setTimeout(() => {\r\n        documents.value = [\r\n          {\r\n            id: 1,\r\n            title: '项目需求分析报告',\r\n            type: '报告',\r\n            content: '本文档详细分析了项目的功能需求和非功能需求...',\r\n            createTime: '2023-11-15T09:00:00',\r\n            updateTime: '2023-11-16T14:30:00'\r\n          },\r\n          {\r\n            id: 2,\r\n            title: '产品发布会议记录',\r\n            type: '会议记录',\r\n            content: '与会人员：张三、李四、王五\\n会议议题：1. 产品发布时间表...',\r\n            createTime: '2023-11-10T13:00:00',\r\n            updateTime: '2023-11-10T15:45:00'\r\n          },\r\n          {\r\n            id: 3,\r\n            title: '2024年Q1营销规划',\r\n            type: '规划文档',\r\n            content: '一、市场分析\\n二、目标客户\\n三、推广策略...',\r\n            createTime: '2023-11-05T10:20:00',\r\n            updateTime: '2023-11-12T11:30:00'\r\n          },\r\n          {\r\n            id: 4,\r\n            title: '软件服务合同模板',\r\n            type: '合同文档',\r\n            content: '甲方：__________\\n乙方：__________\\n...',\r\n            createTime: '2023-10-20T09:15:00',\r\n            updateTime: '2023-10-25T16:40:00'\r\n          },\r\n          {\r\n            id: 5,\r\n            title: '研发部周报 (11.13-11.17)',\r\n            type: '周报',\r\n            content: '本周完成工作：\\n1. 功能A的开发\\n2. 功能B的测试...',\r\n            createTime: '2023-11-17T17:00:00',\r\n            updateTime: '2023-11-17T17:30:00'\r\n          },\r\n          {\r\n            id: 6,\r\n            title: '客户反馈收集与分析',\r\n            type: '报告',\r\n            content: '一、反馈渠道\\n二、主要问题分类\\n三、改进建议...',\r\n            createTime: '2023-11-08T14:20:00',\r\n            updateTime: '2023-11-09T10:15:00'\r\n          }\r\n        ]\r\n      }, 300)\r\n    }\r\n    \r\n    // 筛选后的文档列表\r\n    const filteredDocuments = computed(() => {\r\n      let result = [...documents.value]\r\n      \r\n      // 搜索过滤\r\n      if (searchQuery.value) {\r\n        const query = searchQuery.value.toLowerCase()\r\n        result = result.filter(doc => \r\n          doc.title.toLowerCase().includes(query) || \r\n          doc.content.toLowerCase().includes(query)\r\n        )\r\n      }\r\n      \r\n      // 类型过滤\r\n      if (docTypeFilter.value) {\r\n        result = result.filter(doc => doc.type === docTypeFilter.value)\r\n      }\r\n      \r\n      // 排序\r\n      switch (sortOption.value) {\r\n        case 'update':\r\n          result.sort((a, b) => new Date(b.updateTime) - new Date(a.updateTime))\r\n          break\r\n        case 'create':\r\n          result.sort((a, b) => new Date(b.createTime) - new Date(a.createTime))\r\n          break\r\n        case 'name-asc':\r\n          result.sort((a, b) => a.title.localeCompare(b.title))\r\n          break\r\n        case 'name-desc':\r\n          result.sort((a, b) => b.title.localeCompare(a.title))\r\n          break\r\n      }\r\n      \r\n      return result\r\n    })\r\n    \r\n    // 格式化日期\r\n    const formatDate = (dateString) => {\r\n      const date = new Date(dateString)\r\n      const now = new Date()\r\n      const diff = now - date\r\n      \r\n      // 如果是今天，显示时间\r\n      if (diff < 24 * 60 * 60 * 1000 && \r\n          date.getDate() === now.getDate() &&\r\n          date.getMonth() === now.getMonth() &&\r\n          date.getFullYear() === now.getFullYear()) {\r\n        return `今天 ${date.getHours().toString().padStart(2, '0')}:${date.getMinutes().toString().padStart(2, '0')}`\r\n      }\r\n      \r\n      // 如果是昨天\r\n      const yesterday = new Date(now)\r\n      yesterday.setDate(now.getDate() - 1)\r\n      if (date.getDate() === yesterday.getDate() &&\r\n          date.getMonth() === yesterday.getMonth() &&\r\n          date.getFullYear() === yesterday.getFullYear()) {\r\n        return `昨天 ${date.getHours().toString().padStart(2, '0')}:${date.getMinutes().toString().padStart(2, '0')}`\r\n      }\r\n      \r\n      // 如果是今年\r\n      if (date.getFullYear() === now.getFullYear()) {\r\n        return `${(date.getMonth() + 1).toString().padStart(2, '0')}-${date.getDate().toString().padStart(2, '0')} ${date.getHours().toString().padStart(2, '0')}:${date.getMinutes().toString().padStart(2, '0')}`\r\n      }\r\n      \r\n      // 其他情况\r\n      return `${date.getFullYear()}-${(date.getMonth() + 1).toString().padStart(2, '0')}-${date.getDate().toString().padStart(2, '0')}`\r\n    }\r\n    \r\n    // 获取文档预览内容\r\n    const getDocumentPreview = (doc) => {\r\n      if (!doc.content) return ''\r\n      // 去除HTML标签，只保留纯文本\r\n      const textContent = doc.content.replace(/<[^>]*>/g, '')\r\n      return textContent.length > 50 ? textContent.substring(0, 50) + '...' : textContent\r\n    }\r\n    \r\n    // 生成随机颜色（基于文档类型）\r\n    const getRandomColor = (type) => {\r\n      // 为每种类型分配固定的颜色\r\n      const colorMap = {\r\n        '报告': '#4A90E2',\r\n        '会议记录': '#50C060',\r\n        '规划文档': '#9B59B6',\r\n        '合同文档': '#E67E22',\r\n        '周报': '#3498DB',\r\n        '其他': '#95A5A6'\r\n      }\r\n      \r\n      return colorMap[type] || '#95A5A6'\r\n    }\r\n    \r\n    // 打开文档\r\n    const openDocument = (doc) => {\r\n      router.push(`/smartdoc/detail/${doc.id}`)\r\n    }\r\n    \r\n    // 显示创建新文档对话框\r\n    const createNewDocument = () => {\r\n      newDocumentForm.value = {\r\n        title: '',\r\n        type: '',\r\n        template: 'blank'\r\n      }\r\n      newDocumentDialogVisible.value = true\r\n    }\r\n    \r\n    // 确认创建文档\r\n    const confirmCreateDocument = () => {\r\n      if (!newDocumentForm.value.title) {\r\n        ElMessage.warning('请输入文档标题')\r\n        return\r\n      }\r\n      \r\n      // 模拟创建文档\r\n      const newDoc = {\r\n        id: Date.now(), // 使用时间戳作为临时ID\r\n        title: newDocumentForm.value.title,\r\n        type: newDocumentForm.value.type || '其他',\r\n        content: '',\r\n        createTime: new Date().toISOString(),\r\n        updateTime: new Date().toISOString()\r\n      }\r\n      \r\n      // 添加到文档列表\r\n      documents.value.unshift(newDoc)\r\n      \r\n      // 关闭对话框\r\n      newDocumentDialogVisible.value = false\r\n      \r\n      // 打开新创建的文档\r\n      router.push(`/smartdoc/detail/${newDoc.id}`)\r\n    }\r\n    \r\n    // 分享文档\r\n    const shareDocument = (doc) => {\r\n      ElMessage.success(`分享文档：\"${doc.title}\"`)\r\n    }\r\n    \r\n    // 复制文档\r\n    const duplicateDocument = (doc) => {\r\n      const newDoc = {\r\n        ...doc,\r\n        id: Date.now(),\r\n        title: `${doc.title} (副本)`,\r\n        createTime: new Date().toISOString(),\r\n        updateTime: new Date().toISOString()\r\n      }\r\n      documents.value.unshift(newDoc)\r\n      ElMessage.success('文档已复制')\r\n    }\r\n    \r\n    // 导出文档\r\n    const exportDocument = (doc) => {\r\n      ElMessage.success(`导出文档：\"${doc.title}\"`)\r\n    }\r\n    \r\n    // 删除文档\r\n    const deleteDocument = (doc) => {\r\n      ElMessageBox.confirm(`确定要删除文档\"${doc.title}\"吗？`, '提示', {\r\n        confirmButtonText: '确定',\r\n        cancelButtonText: '取消',\r\n        type: 'warning'\r\n      }).then(() => {\r\n        documents.value = documents.value.filter(item => item.id !== doc.id)\r\n        ElMessage.success('文档已删除')\r\n      }).catch(() => {})\r\n    }\r\n    \r\n    // 批量导出\r\n    const batchExport = () => {\r\n      ElMessage.info('批量导出功能开发中')\r\n    }\r\n    \r\n    // 批量共享\r\n    const batchShare = () => {\r\n      ElMessage.info('批量共享功能开发中')\r\n    }\r\n    \r\n    // 管理文件夹\r\n    const manageFolders = () => {\r\n      ElMessage.info('文件夹管理功能开发中')\r\n    }\r\n    \r\n    // 导入文档对话框\r\n    const showImportDialog = () => {\r\n      ElMessage.info('文档导入功能开发中')\r\n    }\r\n    \r\n    onMounted(() => {\r\n      fetchDocuments()\r\n    })\r\n    \r\n    return {\r\n      documents,\r\n      searchQuery,\r\n      docTypeFilter,\r\n      sortOption,\r\n      viewMode,\r\n      filteredDocuments,\r\n      docTypes,\r\n      newDocumentDialogVisible,\r\n      newDocumentForm,\r\n      \r\n      formatDate,\r\n      getDocumentPreview,\r\n      getRandomColor,\r\n      openDocument,\r\n      createNewDocument,\r\n      confirmCreateDocument,\r\n      shareDocument,\r\n      duplicateDocument,\r\n      exportDocument,\r\n      deleteDocument,\r\n      batchExport,\r\n      batchShare,\r\n      manageFolders,\r\n      showImportDialog\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.smartdoc-home-container {\r\n  padding: 24px;\r\n  height: 100%;\r\n  display: flex;\r\n  flex-direction: column;\r\n}\r\n\r\n.page-header {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n  margin-bottom: 24px;\r\n  \r\n  .header-title {\r\n    h2 {\r\n      margin: 0;\r\n      font-size: 24px;\r\n      font-weight: 600;\r\n    }\r\n    \r\n    .subtext {\r\n      font-size: 14px;\r\n      color: #909399;\r\n      margin-top: 4px;\r\n      display: block;\r\n    }\r\n  }\r\n  \r\n  .header-actions {\r\n    display: flex;\r\n    gap: 12px;\r\n  }\r\n}\r\n\r\n.filter-toolbar {\r\n  display: flex;\r\n  margin-bottom: 24px;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n  flex-wrap: wrap;\r\n  gap: 12px;\r\n  \r\n  .search-box {\r\n    flex: 1;\r\n    max-width: 400px;\r\n    \r\n    .search-input {\r\n      width: 100%;\r\n    }\r\n  }\r\n  \r\n  .filter-actions {\r\n    display: flex;\r\n    gap: 12px;\r\n    flex-wrap: wrap;\r\n  }\r\n  \r\n  .view-mode-select {\r\n    min-width: 120px;\r\n  }\r\n}\r\n\r\n.document-grid {\r\n  margin-bottom: 24px;\r\n  overflow-y: auto;\r\n  flex: 1;\r\n  \r\n  .grid-item-col {\r\n    margin-bottom: 20px;\r\n  }\r\n  \r\n  .document-card {\r\n    height: 250px;\r\n    border-radius: 8px;\r\n    overflow: hidden;\r\n    box-shadow: 0 2px 12px 0 rgba(0, 0, 0, 0.1);\r\n    transition: all 0.3s;\r\n    cursor: pointer;\r\n    display: flex;\r\n    flex-direction: column;\r\n    background-color: var(--el-bg-color);\r\n    position: relative;\r\n    \r\n    &:hover {\r\n      transform: translateY(-5px);\r\n      box-shadow: 0 5px 15px rgba(0, 0, 0, 0.2);\r\n      \r\n      .card-actions {\r\n        opacity: 1;\r\n      }\r\n    }\r\n    \r\n    .card-thumbnail {\r\n      height: 100px;\r\n      display: flex;\r\n      align-items: center;\r\n      justify-content: center;\r\n      \r\n      .el-icon {\r\n        font-size: 36px;\r\n        color: white;\r\n      }\r\n    }\r\n    \r\n    .card-content {\r\n      padding: 16px;\r\n      flex: 1;\r\n      overflow: hidden;\r\n      \r\n      .doc-title {\r\n        margin: 0 0 8px;\r\n        font-size: 16px;\r\n        font-weight: 600;\r\n        line-height: 1.4;\r\n        white-space: nowrap;\r\n        overflow: hidden;\r\n        text-overflow: ellipsis;\r\n      }\r\n      \r\n      .doc-meta {\r\n        display: flex;\r\n        align-items: center;\r\n        justify-content: space-between;\r\n        font-size: 12px;\r\n        color: #909399;\r\n        margin-bottom: 10px;\r\n        \r\n        .doc-type {\r\n          background-color: #f2f6fc;\r\n          padding: 2px 6px;\r\n          border-radius: 4px;\r\n        }\r\n      }\r\n      \r\n      .doc-preview {\r\n        font-size: 13px;\r\n        color: #606266;\r\n        overflow: hidden;\r\n        display: -webkit-box;\r\n        -webkit-line-clamp: 3;\r\n        -webkit-box-orient: vertical;\r\n        line-height: 1.5;\r\n      }\r\n    }\r\n    \r\n    .card-actions {\r\n      position: absolute;\r\n      top: 10px;\r\n      right: 10px;\r\n      opacity: 0;\r\n      transition: opacity 0.3s;\r\n      \r\n      .action-btn {\r\n        background-color: rgba(255, 255, 255, 0.9);\r\n        border: none;\r\n      }\r\n    }\r\n  }\r\n}\r\n\r\n.document-list {\r\n  margin-bottom: 24px;\r\n  overflow-y: auto;\r\n  flex: 1;\r\n  \r\n  .el-table {\r\n    border-radius: 8px;\r\n    overflow: hidden;\r\n    \r\n    ::v-deep(.el-table__row) {\r\n      cursor: pointer;\r\n    }\r\n  }\r\n  \r\n  .danger-item {\r\n    color: #F56C6C;\r\n  }\r\n}\r\n\r\n.empty-state {\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n  justify-content: center;\r\n  padding: 60px 0;\r\n}\r\n\r\n.danger-item {\r\n  color: #F56C6C;\r\n}\r\n</style> "]}]}