tween;\r\n    align-items: center;\r\n    \r\n    h2 {\r\n      color: var(--text-color);\r\n      margin: 0;\r\n    }\r\n  }\r\n  \r\n  .project-tabs {\r\n    margin-bottom: 20px;\r\n    \r\n    :deep(.el-tabs__item) {\r\n      color: var(--text-color-secondary);\r\n      \r\n      &.is-active {\r\n        color: var(--primary-color);\r\n      }\r\n    }\r\n    \r\n    :deep(.el-tabs__active-bar) {\r\n      background-color: var(--primary-color);\r\n    }\r\n    \r\n    :deep(.el-tabs__nav-wrap::after) {\r\n      background-color: var(--border-color);\r\n    }\r\n  }\r\n  \r\n  .kanban-board {\r\n    display: flex;\r\n    gap: 20px;\r\n    overflow-x: auto;\r\n    padding-bottom: 20px;\r\n    \r\n    .kanban-column {\r\n      min-width: 280px;\r\n      width: 280px;\r\n      background-color: var(--bg-color-secondary);\r\n      border-radius: 8px;\r\n      \r\n      .column-header {\r\n        padding: 12px 16px;\r\n        border-bottom: 1px solid var(--border-color);\r\n        display: flex;\r\n        justify-content: space-between;\r\n        align-items: center;\r\n        \r\n        .column-title {\r\n          font-size: 16px;\r\n          font-weight: 500;\r\n          color: var(--text-color);\r\n          display: flex;\r\n          align-items: center;\r\n          \r\n          .task-count {\r\n            margin-left: 8px;\r\n            background-color: var(--bg-color-tertiary);\r\n            color: var(--text-color-secondary);\r\n            border-radius: 10px;\r\n            padding: 2px 8px;\r\n            font-size: 12px;\r\n            font-weight: normal;\r\n          }\r\n        }\r\n      }\r\n      \r\n      .task-list {\r\n        padding: 12px;\r\n        min-height: 100px;\r\n        \r\n        .task-card {\r\n          background-color: var(--bg-color);\r\n          padding: 12px;\r\n          border-radius: 6px;\r\n          margin-bottom: 12px;\r\n          box-shadow: 0 2px 8px var(--shadow-color);\r\n          cursor: pointer;\r\n          \r\n          &:hover {\r\n            transform: translateY(-2px);\r\n            box-shadow: 0 4px 12px var(--shadow-color);\r\n          }\r\n          \r\n          .task-title {\r\n            color: var(--text-color);\r\n            font-weight: 500;\r\n            margin-bottom: 8px;\r\n          }\r\n          \r\n          .task-description {\r\n            color: var(--text-color-secondary);\r\n            font-size: 13px;\r\n            margin-bottom: 12px;\r\n            display: -webkit-box;\r\n            -webkit-line-clamp: 2;\r\n            -webkit-box-orient: vertical;\r\n            overflow: hidden;\r\n          }\r\n          \r\n          .task-meta {\r\n            display: flex;\r\n            justify-content: space-between;\r\n            align-items: center;\r\n            \r\n            .assignee {\r\n              display: flex;\r\n              align-items: center;\r\n              \r\n              .assignee-name {\r\n                color: var(--text-color-secondary);\r\n                font-size: 12px;\r\n                margin-left: 8px;\r\n              }\r\n            }\r\n            \r\n            .tags {\r\n              display: flex;\r\n              gap: 4px;\r\n              \r\n              .tag {\r\n                padding: 2px 6px;\r\n                border-radius: 4px;\r\n                font-size: 11px;\r\n                \r\n                &.priority-high {\r\n                  background-color: rgba(239, 68, 68, 0.1);\r\n                  color: #ef4444;\r\n                }\r\n                \r\n                &.priority-medium {\r\n                  background-color: rgba(245, 158, 11, 0.1);\r\n                  color: #f59e0b;\r\n                }\r\n                \r\n                &.priority-low {\r\n                  background-color: rgba(16, 185, 129, 0.1);\r\n                  color: #10b981;\r\n                }\r\n              }\r\n            }\r\n          }\r\n        }\r\n        \r\n        .add-task {\r\n          background-color: var(--bg-color-tertiary);\r\n          color: var(--text-color-secondary);\r\n          padding: 12px;\r\n          border-radius: 6px;\r\n          display: flex;\r\n          align-items: center;\r\n          justify-content: center;\r\n          cursor: pointer;\r\n          \r\n          &:hover {\r\n            background-color: var(--hover-color);\r\n          }\r\n          \r\n          i {\r\n            margin-right: 8px;\r\n          }\r\n        }\r\n      }\r\n    }\r\n  }\r\n}\r\n</style> "]}]}