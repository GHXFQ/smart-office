     <el-form-item label=\"标题\">\r\n          <el-input v-model=\"form.title\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"开始时间\">\r\n          <el-date-picker v-model=\"form.start\" type=\"datetime\" placeholder=\"选择开始时间\"></el-date-picker>\r\n        </el-form-item>\r\n        <el-form-item label=\"结束时间\">\r\n          <el-date-picker v-model=\"form.end\" type=\"datetime\" placeholder=\"选择结束时间\"></el-date-picker>\r\n        </el-form-item>\r\n        <el-form-item label=\"描述\">\r\n          <el-input v-model=\"form.description\" type=\"textarea\"></el-input>\r\n        </el-form-item>\r\n      </el-form>\r\n      <template #footer>\r\n        <span class=\"dialog-footer\">\r\n          <el-button @click=\"dialogVisible = false\">取消</el-button>\r\n          <el-button type=\"primary\" @click=\"saveEvent\">保存</el-button>\r\n        </span>\r\n      </template>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script setup>\r\nimport { ref } from 'vue';\r\n\r\nconst events = ref([\r\n  { id: 1, title: '团队会议', start: new Date(), end: new Date(new Date().getTime() + 3600 * 1000), description: '讨论项目进展' },\r\n  { id: 2, title: '与客户A会面', start: new Date(new Date().getTime() + 2 * 3600 * 1000), end: new Date(new Date().getTime() + 3 * 3600 * 1000), description: '商讨合同细节' }\r\n]);\r\n\r\nconst dialogVisible = ref(false);\r\nconst form = ref({\r\n  id: null,\r\n  title: '',\r\n  start: '',\r\n  end: '',\r\n  description: ''\r\n});\r\n\r\nconst formatTime = (date) => {\r\n  return new Date(date).toLocaleString();\r\n};\r\n\r\nconst addEvent = () => {\r\n  form.value = { id: null, title: '', start: '', end: '', description: '' };\r\n  dialogVisible.value = true;\r\n};\r\n\r\nconst editEvent = (event) => {\r\n  form.value = { ...event };\r\n  dialogVisible.value = true;\r\n};\r\n\r\nconst deleteEvent = (id) => {\r\n  events.value = events.value.filter(event => event.id !== id);\r\n};\r\n\r\nconst saveEvent = () => {\r\n  if (form.value.id) {\r\n    const index = events.value.findIndex(event => event.id === form.value.id);\r\n    if (index !== -1) {\r\n      events.value[index] = { ...form.value };\r\n    }\r\n  } else {\r\n    events.value.push({ ...form.value, id: Date.now() });\r\n  }\r\n  dialogVisible.value = false;\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.schedule-container {\r\n  padding: 20px;\r\n}\r\n.card-header {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n}\r\n.schedule-item {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n  padding: 10px 0;\r\n  border-bottom: 1px solid #ebeef5;\r\n}\r\n.event-title {\r\n  font-weight: bold;\r\n}\r\n.event-time {\r\n  color: #909399;\r\n}\r\n</style> "]}]}