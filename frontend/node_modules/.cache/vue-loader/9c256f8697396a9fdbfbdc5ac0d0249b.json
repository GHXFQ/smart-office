png\" alt=\"Logo\">\r\n      <h5 v-if=\"sidebarOpened\">智能办公系统</h5>\r\n    </div>\r\n\r\n    <!-- 侧边栏菜单 -->\r\n    <el-scrollbar class=\"sidebar-menu-container\">\r\n      <el-menu\r\n        :default-active=\"activeMenu\"\r\n        :collapse=\"!sidebarOpened\"\r\n        background-color=\"#fff\"\r\n        text-color=\"#495057\"\r\n        active-text-color=\"#007bff\"\r\n        :unique-opened=\"true\"\r\n        router\r\n      >\r\n        <el-menu-item v-for=\"route in routes\" :key=\"route.path\" :index=\"route.path\">\r\n          <i :class=\"route.meta.icon\"></i>\r\n          <template #title>{{ route.meta.title }}</template>\r\n        </el-menu-item>\r\n      </el-menu>\r\n    </el-scrollbar>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { computed } from 'vue'\r\nimport { useRouter, useRoute } from 'vue-router'\r\nimport { useStore } from 'vuex'\r\n\r\nexport default {\r\n  name: 'Sidebar',\r\n  setup() {\r\n    const store = useStore()\r\n    const router = useRouter()\r\n    const route = useRoute()\r\n\r\n    // 从Vuex中获取侧边栏状态\r\n    const sidebarOpened = computed(() => store.getters.sidebar.opened)\r\n\r\n    // 菜单路由\r\n    const routes = computed(() => {\r\n      return router.options.routes.find(r => r.path === '/').children\r\n        .filter(route => !route.hidden)\r\n    })\r\n\r\n    // 当前激活的菜单\r\n    const activeMenu = computed(() => {\r\n      return route.path\r\n    })\r\n\r\n    return {\r\n      sidebarOpened,\r\n      routes,\r\n      activeMenu\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.sidebar {\r\n  width: 220px;\r\n  height: 100%;\r\n  background-color: #fff;\r\n  border-right: 1px solid #e9ecef;\r\n  box-shadow: 0 0 10px rgba(0, 0, 0, 0.05);\r\n  z-index: 1000;\r\n  transition: width 0.3s;\r\n}\r\n\r\n.sidebar.is-collapsed {\r\n  width: 64px;\r\n}\r\n\r\n.sidebar-header {\r\n  padding: 20px;\r\n  display: flex;\r\n  align-items: center;\r\n  border-bottom: 1px solid #e9ecef;\r\n  height: 60px;\r\n}\r\n\r\n.sidebar-header img {\r\n  width: 24px;\r\n  height: 24px;\r\n}\r\n\r\n.sidebar-header h5 {\r\n  margin: 0 0 0 12px;\r\n  font-size: 16px;\r\n  font-weight: 600;\r\n  color: #333;\r\n  white-space: nowrap;\r\n}\r\n\r\n.sidebar-menu-container {\r\n  height: calc(100% - 60px);\r\n}\r\n\r\n.el-menu {\r\n  border-right: none;\r\n}\r\n\r\n.el-menu-item {\r\n  height: 50px;\r\n  line-height: 50px;\r\n}\r\n\r\n.el-menu-item i {\r\n  margin-right: 10px;\r\n  width: 24px;\r\n  text-align: center;\r\n  font-size: 18px;\r\n}\r\n</style> "]}]}