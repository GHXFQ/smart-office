,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;AACpC;;AAEA,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE;EACrB,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;AACxC;;AAEA,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE;EACxB,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;AACpC","file":"C:/Users/18083/Desktop/CCIT/office/smart-office/frontend/src/components/settings/ThemeSettings.vue","sourceRoot":"","sourcesContent":["<template>\n  <div class=\"settings-panel\">\n    <div class=\"settings-header\">\n      <h2>系统设置</h2>\n      <div class=\"settings-actions\">\n        <el-button @click=\"restoreDefaults\" type=\"text\">\n          <el-icon><Refresh /></el-icon> 恢复默认\n        </el-button>\n        <el-button @click=\"saveSettings\" type=\"primary\">\n          <el-icon><Check /></el-icon> 保存设置\n        </el-button>\n      </div>\n    </div>\n\n    <div class=\"settings-content\">\n      <el-menu class=\"settings-sidebar\" :default-active=\"activeSection\">\n        <el-menu-item index=\"ai\" @click=\"activeSection = 'ai'\">\n          <el-icon><Setting /></el-icon>\n          <span>个性化AI设置</span>\n        </el-menu-item>\n        <el-menu-item index=\"ui\" @click=\"activeSection = 'ui'\">\n          <el-icon><Brush /></el-icon>\n          <span>界面偏好</span>\n        </el-menu-item>\n      </el-menu>\n\n      <div class=\"settings-main\">\n        <!-- 主题设置 -->\n        <div v-if=\"activeSection === 'ui'\" class=\"settings-section\">\n          <h3>主题设置</h3>\n          \n          <div class=\"setting-item\">\n            <span class=\"setting-label\">主题模式</span>\n            <el-radio-group v-model=\"settings.theme\" size=\"large\">\n              <el-radio-button label=\"light\">\n                <el-icon><Sunny /></el-icon> 浅色\n              </el-radio-button>\n              <el-radio-button label=\"dark\">\n                <el-icon><Moon /></el-icon> 深色\n              </el-radio-button>\n            </el-radio-group>\n          </div>\n\n          <h3>布局设置</h3>\n          \n          <div class=\"setting-item\">\n            <span class=\"setting-label\">内容密度</span>\n            <el-radio-group v-model=\"settings.density\" size=\"large\">\n              <el-radio-button label=\"compact\">紧凑</el-radio-button>\n              <el-radio-button label=\"standard\">标准</el-radio-button>\n              <el-radio-button label=\"comfortable\">宽松</el-radio-button>\n            </el-radio-group>\n          </div>\n\n          <div class=\"setting-item\">\n            <span class=\"setting-label\">字体大小</span>\n            <div class=\"font-size-slider\">\n              <el-slider \n                v-model=\"settings.fontSize\" \n                :min=\"12\" \n                :max=\"20\" \n                :step=\"1\" \n                show-input\n                :format-tooltip=\"value => `${value}px`\"\n              ></el-slider>\n            </div>\n          </div>\n        </div>\n\n        <!-- AI设置部分 (简化版) -->\n        <div v-if=\"activeSection === 'ai'\" class=\"settings-section\">\n          <h3>个性化AI设置</h3>\n          <el-empty description=\"暂未实现\"></el-empty>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { Refresh, Check, Setting, Brush, Sunny, Moon } from '@element-plus/icons-vue';\n\nexport default {\n  name: 'ThemeSettings',\n  components: {\n    Refresh, \n    Check, \n    Setting, \n    Brush, \n    Sunny, \n    Moon\n  },\n  data() {\n    return {\n      activeSection: 'ui',\n      settings: {\n        theme: localStorage.getItem('theme') || 'light',\n        density: localStorage.getItem('density') || 'standard',\n        fontSize: parseInt(localStorage.getItem('fontSize')) || 14\n      },\n      defaultSettings: {\n        theme: 'light',\n        density: 'standard',\n        fontSize: 14\n      }\n    }\n  },\n  watch: {\n    'settings.theme': {\n      handler(newVal) {\n        this.applyTheme(newVal);\n      },\n      immediate: true\n    },\n    'settings.fontSize': {\n      handler(newVal) {\n        document.documentElement.style.setProperty('--font-size-base', `${newVal}px`);\n        localStorage.setItem('fontSize', newVal);\n      },\n      immediate: true\n    },\n    'settings.density': {\n      handler(newVal) {\n        document.body.setAttribute('data-density', newVal);\n        localStorage.setItem('density', newVal);\n      },\n      immediate: true\n    }\n  },\n  methods: {\n    applyTheme(theme) {\n      document.documentElement.setAttribute('data-theme', theme);\n      localStorage.setItem('theme', theme);\n      \n      // 更新meta主题色\n      const metaThemeColor = document.querySelector('meta[name=\"theme-color\"]');\n      if (metaThemeColor) {\n        metaThemeColor.setAttribute('content', theme === 'dark' ? '#1a1a1a' : '#ffffff');\n      }\n    },\n    saveSettings() {\n      // 保存设置到localStorage\n      localStorage.setItem('theme', this.settings.theme);\n      localStorage.setItem('density', this.settings.density);\n      localStorage.setItem('fontSize', this.settings.fontSize);\n      \n      // 应用设置\n      this.applyTheme(this.settings.theme);\n      document.body.setAttribute('data-density', this.settings.density);\n      document.documentElement.style.setProperty('--font-size-base', `${this.settings.fontSize}px`);\n      \n      // 通知用户\n      this.$message({\n        message: '设置已保存',\n        type: 'success'\n      });\n      \n      // 关闭设置面板\n      this.$emit('close');\n    },\n    restoreDefaults() {\n      this.settings = JSON.parse(JSON.stringify(this.defaultSettings));\n    }\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n.settings-panel {\n  width: 100%;\n  height: 100%;\n  display: flex;\n  flex-direction: column;\n  background-color: var(--bg-color);\n  color: var(--text-color);\n  \n  .settings-header {\n    display: flex;\n    justify-content: space-between;\n    align-items: center;\n    padding: 16px 20px;\n    border-bottom: 1px solid var(--border-color);\n    \n    h2 {\n      margin: 0;\n      font-size: 18px;\n      font-weight: 500;\n    }\n    \n    .settings-actions {\n      display: flex;\n      gap: 12px;\n    }\n  }\n  \n  .settings-content {\n    display: flex;\n    flex: 1;\n    overflow: hidden;\n    \n    .settings-sidebar {\n      width: 200px;\n      border-right: 1px solid var(--border-color);\n    }\n    \n    .settings-main {\n      flex: 1;\n      padding: 20px;\n      overflow-y: auto;\n      \n      .settings-section {\n        margin-bottom: 24px;\n        \n        h3 {\n          font-size: 16px;\n          font-weight: 500;\n          margin-top: 0;\n          margin-bottom: 16px;\n          padding-bottom: 8px;\n          border-bottom: 1px solid var(--border-color);\n        }\n      }\n      \n      .setting-item {\n        margin-bottom: 20px;\n        \n        .setting-label {\n          display: block;\n          font-size: 14px;\n          margin-bottom: 8px;\n        }\n        \n        .font-size-slider {\n          margin-top: 10px;\n        }\n      }\n    }\n  }\n}\n\n:deep(.el-menu) {\n  background-color: transparent;\n  border-right: none;\n}\n\n:deep(.el-menu-item) {\n  color: var(--text-color);\n  \n  &:hover {\n    background-color: var(--hover-color);\n  }\n  \n  &.is-active {\n    background-color: var(--hover-color);\n    color: var(--primary-color);\n  }\n}\n\n:deep(.el-radio-button__original-radio:checked + .el-radio-button__inner) {\n  background-color: var(--primary-color);\n  border-color: var(--primary-color);\n  box-shadow: -1px 0 0 0 var(--primary-color);\n}\n\n:deep(.el-slider__runway) {\n  background-color: var(--slider-bg);\n}\n\n:deep(.el-slider__bar) {\n  background-color: var(--primary-color);\n}\n\n:deep(.el-slider__button) {\n  border-color: var(--primary-color);\n}\n</style> "]}]}