\r\n  <div class=\"sidebar\" :class=\"{ 'is-collapsed': !sidebarOpened }\">\r\n    <!-- 侧边栏头部 -->\r\n    <div class=\"sidebar-header\">\r\n      <img src=\"https://cdn-icons-png.flaticon.com/512/2891/2891491.png\" alt=\"Logo\">\r\n      <h5 v-if=\"sidebarOpened\">智能办公系统</h5>\r\n    </div>\r\n\r\n    <!-- 侧边栏菜单 -->\r\n    <el-scrollbar class=\"sidebar-menu-container\">\r\n      <el-menu\r\n        :default-active=\"activeMenu\"\r\n        :collapse=\"!sidebarOpened\"\r\n        background-color=\"#fff\"\r\n        text-color=\"#495057\"\r\n        active-text-color=\"#007bff\"\r\n        :unique-opened=\"true\"\r\n        router\r\n      >\r\n        <el-menu-item v-for=\"route in routes\" :key=\"route.path\" :index=\"route.path\">\r\n          <el-icon>\r\n            <component :is=\"iconComponent(route.meta.icon)\"></component>\r\n          </el-icon>\r\n          <template #title>{{ route.meta.title }}</template>\r\n        </el-menu-item>\r\n      </el-menu>\r\n    </el-scrollbar>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { computed } from 'vue'\r\nimport { useRouter, useRoute } from 'vue-router'\r\nimport { useStore } from 'vuex'\r\n\r\nexport default {\r\n  name: 'Sidebar',\r\n  setup() {\r\n    const store = useStore()\r\n    const router = useRouter()\r\n    const route = useRoute()\r\n\r\n    // 从Vuex中获取侧边栏状态\r\n    const sidebarOpened = computed(() => store.getters.sidebar.opened)\r\n\r\n    // 菜单路由\r\n    const routes = computed(() => {\r\n      return router.options.routes.find(r => r.path === '/').children\r\n        .filter(route => !route.hidden)\r\n    })\r\n\r\n    // 当前激活的菜单\r\n    const activeMenu = computed(() => {\r\n      return route.path\r\n    })\r\n\r\n    // 将Bootstrap图标类名转换为Element Plus图标组件\r\n    const iconComponent = (icon) => {\r\n      // 如果是bootstrap图标类，转换为Element Plus图标\r\n      if (icon && icon.includes('bi-')) {\r\n        // 移除bi-前缀，转换为驼峰命名\r\n        const name = icon.replace('bi-', '')\r\n          .split('-')\r\n          .map((word, index) => \r\n            index === 0 ? word : word.charAt(0).toUpperCase() + word.slice(1)\r\n          )\r\n          .join('')\r\n        \r\n        // 检查是否存在对应的Element Plus图标\r\n        const iconMap = {\r\n          'home': 'House',\r\n          'grid': 'Grid',\r\n          'calendar': 'Calendar',\r\n          'book': 'Collection',\r\n          'chat': 'ChatDotRound',\r\n          'camera': 'VideoCamera',\r\n          'file': 'Document',\r\n          'file-earmark-text': 'Document',\r\n          'building': 'OfficeBuilding',\r\n          'envelope': 'Message',\r\n          'list': 'List',\r\n          'person': 'User',\r\n          'people': 'User',\r\n          'cart': 'ShoppingCart',\r\n          'gear': 'Setting',\r\n          'box': 'Box',\r\n          'graph-up': 'TrendCharts',\r\n          'code-slash': 'Terminal',\r\n          'headset': 'Service',\r\n          'send': 'Position',\r\n          'checkAll': 'CheckboxButton',\r\n          'clock': 'Clock',\r\n          'trash': 'Delete'\r\n        };\r\n        \r\n        return iconMap[name] || 'Document';\r\n      }\r\n      return 'Document'\r\n    }\r\n\r\n    return {\r\n      sidebarOpened,\r\n      routes,\r\n      activeMenu,\r\n      iconComponent\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.sidebar {\r\n  width: 220px;\r\n  height: 100%;\r\n  background-color: #fff;\r\n  border-right: 1px solid #e9ecef;\r\n  box-shadow: 0 0 10px rgba(0, 0, 0, 0.05);\r\n  z-index: 1000;\r\n  transition: width 0.3s;\r\n}\r\n\r\n.sidebar.is-collapsed {\r\n  width: 64px;\r\n}\r\n\r\n.sidebar-header {\r\n  padding: 20px;\r\n  display: flex;\r\n  align-items: center;\r\n  border-bottom: 1px solid #e9ecef;\r\n  height: 60px;\r\n}\r\n\r\n.sidebar-header img {\r\n  width: 24px;\r\n  height: 24px;\r\n}\r\n\r\n.sidebar-header h5 {\r\n  margin: 0 0 0 12px;\r\n  font-size: 16px;\r\n  font-weight: 600;\r\n  color: #333;\r\n  white-space: nowrap;\r\n}\r\n\r\n.sidebar-menu-container {\r\n  height: calc(100% - 60px);\r\n}\r\n\r\n.el-menu {\r\n  border-right: none;\r\n}\r\n\r\n.el-menu-item {\r\n  height: 50px;\r\n  line-height: 50px;\r\n}\r\n\r\n.el-menu-item .el-icon {\r\n  margin-right: 10px;\r\n  width: 24px;\r\n  text-align: center;\r\n  font-size: 18px;\r\n}\r\n</style> "]}]}