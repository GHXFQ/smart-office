      </el-button>\n\n            <div v-if=\"securityQuestionVisible\" class=\"security-question-section\">\n              <p>请回答您的安全问题:</p>\n              <div class=\"security-question-display\">\n                {{ securityQuestion }}\n              </div>\n\n              <el-form-item prop=\"securityAnswer\">\n                <el-input \n                  v-model=\"questionForm.securityAnswer\" \n                  placeholder=\"请输入安全问题的答案\"\n                >\n                </el-input>\n              </el-form-item>\n\n              <el-form-item prop=\"newPassword\">\n                <el-input \n                  v-model=\"questionForm.newPassword\" \n                  type=\"password\" \n                  placeholder=\"请输入新密码\"\n                  show-password\n                >\n                  <template #prefix>\n                    <el-icon><Lock /></el-icon>\n                  </template>\n                </el-input>\n              </el-form-item>\n\n              <el-form-item prop=\"confirmPassword\">\n                <el-input \n                  v-model=\"questionForm.confirmPassword\" \n                  type=\"password\" \n                  placeholder=\"请再次输入新密码\"\n                  show-password\n                >\n                  <template #prefix>\n                    <el-icon><Lock /></el-icon>\n                  </template>\n                </el-input>\n              </el-form-item>\n\n              <el-button \n                type=\"primary\" \n                class=\"submit-button\" \n                @click=\"resetByQuestion\" \n                :loading=\"resetLoading\"\n              >\n                重置密码\n              </el-button>\n            </div>\n          </el-form>\n        </el-tab-pane>\n\n        <el-tab-pane label=\"通过邮箱\" name=\"email\">\n          <el-form ref=\"emailFormRef\" :model=\"emailForm\" :rules=\"emailRules\">\n            <el-form-item prop=\"accountOrEmail\">\n              <el-input \n                v-model=\"emailForm.accountOrEmail\" \n                placeholder=\"请输入您的账号或注册邮箱\"\n              >\n                <template #prefix>\n                  <el-icon><User /></el-icon>\n                </template>\n              </el-input>\n            </el-form-item>\n\n            <div class=\"form-text\">\n              我们将向与此账户关联的邮箱发送密码重置链接。\n            </div>\n\n            <el-button \n              type=\"primary\" \n              class=\"submit-button\" \n              @click=\"resetByEmail\" \n              :loading=\"emailSendLoading\"\n            >\n              发送重置链接\n            </el-button>\n          </el-form>\n        </el-tab-pane>\n      </el-tabs>\n\n      <div class=\"back-to-login\">\n        <router-link to=\"/login\">\n          <el-icon><ArrowLeft /></el-icon> 返回登录\n        </router-link>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { ref, reactive } from 'vue';\nimport { useRouter } from 'vue-router';\nimport { ElMessage } from 'element-plus';\nimport { User, Lock, ArrowLeft } from '@element-plus/icons-vue';\n\nexport default {\n  name: 'ForgotPassword',\n  components: {\n    User,\n    Lock,\n    ArrowLeft\n  },\n  setup() {\n    const router = useRouter();\n    const activeTab = ref('question');\n    \n    // 安全问题相关\n    const questionFormRef = ref(null);\n    const securityQuestionLoading = ref(false);\n    const securityQuestionVisible = ref(false);\n    const securityQuestion = ref('');\n    const resetLoading = ref(false);\n    \n    const questionForm = reactive({\n      accountOrEmail: '',\n      securityAnswer: '',\n      newPassword: '',\n      confirmPassword: ''\n    });\n    \n    const questionRules = {\n      accountOrEmail: [\n        { required: true, message: '请输入账号或邮箱', trigger: 'blur' }\n      ],\n      securityAnswer: [\n        { required: true, message: '请输入安全问题答案', trigger: 'blur' }\n      ],\n      newPassword: [\n        { required: true, message: '请输入新密码', trigger: 'blur' },\n        { min: 8, message: '密码长度至少8个字符', trigger: 'blur' }\n      ],\n      confirmPassword: [\n        { required: true, message: '请确认密码', trigger: 'blur' },\n        { \n          validator: (rule, value, callback) => {\n            if (value !== questionForm.newPassword) {\n              callback(new Error('两次输入密码不一致'));\n            } else {\n              callback();\n            }\n          }, \n          trigger: 'blur' \n        }\n      ]\n    };\n    \n    // 邮箱找回相关\n    const emailFormRef = ref(null);\n    const emailSendLoading = ref(false);\n    \n    const emailForm = reactive({\n      accountOrEmail: ''\n    });\n    \n    const emailRules = {\n      accountOrEmail: [\n        { required: true, message: '请输入账号或邮箱', trigger: 'blur' }\n      ]\n    };\n    \n    // 获取安全问题\n    const fetchSecurityQuestion = async () => {\n      if (!questionForm.accountOrEmail.trim()) {\n        ElMessage.warning('请输入账号或邮箱以获取安全问题');\n        return;\n      }\n      \n      securityQuestionLoading.value = true;\n      \n      try {\n        // 模拟获取安全问题\n        await new Promise(resolve => setTimeout(resolve, 1000));\n        securityQuestion.value = '您最喜欢的颜色是？'; // 示例问题\n        securityQuestionVisible.value = true;\n        ElMessage.success('已获取安全问题');\n      } catch (error) {\n        ElMessage.error('获取安全问题失败：' + (error.message || '未找到与该账号关联的安全问题'));\n      } finally {\n        securityQuestionLoading.value = false;\n      }\n    };\n    \n    // 通过安全问题重置密码\n    const resetByQuestion = async () => {\n      if (!questionFormRef.value) return;\n      \n      try {\n        await questionFormRef.value.validate();\n      } catch (error) {\n        return;\n      }\n      \n      resetLoading.value = true;\n      \n      try {\n        // 模拟重置密码\n        await new Promise(resolve => setTimeout(resolve, 1500));\n        ElMessage.success('密码重置成功！');\n        router.push('/login');\n      } catch (error) {\n        ElMessage.error('密码重置失败：' + (error.message || '未知错误'));\n      } finally {\n        resetLoading.value = false;\n      }\n    };\n    \n    // 通过邮箱重置密码\n    const resetByEmail = async () => {\n      if (!emailFormRef.value) return;\n      \n      try {\n        await emailFormRef.value.validate();\n      } catch (error) {\n        return;\n      }\n      \n      emailSendLoading.value = true;\n      \n      try {\n        // 模拟发送邮件\n        await new Promise(resolve => setTimeout(resolve, 1500));\n        ElMessage.success('重置链接已发送至您的邮箱，请查收！');\n      } catch (error) {\n        ElMessage.error('发送重置链接失败：' + (error.message || '未知错误'));\n      } finally {\n        emailSendLoading.value = false;\n      }\n    };\n    \n    return {\n      router,\n      activeTab,\n      questionFormRef,\n      questionForm,\n      questionRules,\n      securityQuestionLoading,\n      securityQuestionVisible,\n      securityQuestion,\n      resetLoading,\n      fetchSecurityQuestion,\n      resetByQuestion,\n      emailFormRef,\n      emailForm,\n      emailRules,\n      emailSendLoading,\n      resetByEmail,\n      User,\n      Lock,\n      ArrowLeft\n    };\n  }\n}\n</script>\n\n<style scoped>\n.forgot-password-container {\n  font-family: 'Microsoft YaHei', sans-serif;\n  min-height: 100vh;\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  background: linear-gradient(135deg, #9333ea 0%, #6366f1 100%);\n  \n  /* 覆盖深色模式变量，强制使用浅色模式 */\n  --bg-color: #ffffff;\n  --bg-color-secondary: #f9fafb;\n  --bg-color-tertiary: #f3f4f6;\n  --text-color: #1f2937;\n  --text-color-secondary: #4b5563;\n  --text-color-tertiary: #6b7280;\n  --border-color: #e5e7eb;\n  --border-color-light: #f3f4f6;\n  --shadow-color: rgba(0, 0, 0, 0.1);\n  --hover-color: #f3f4f6;\n  --active-color: #e5e7eb;\n  \n  /* 字体和内容密度相关设置 */\n  --font-size-base: 16px;\n  --font-size-sm: 14px;\n  --font-size-lg: 18px;\n  --font-weight-normal: 400;\n  --font-weight-medium: 500;\n  --font-weight-bold: 600;\n  --line-height-base: 1.5;\n  --spacing-base: 16px;\n  --spacing-sm: 8px;\n  --spacing-lg: 24px;\n  --content-max-width: 1200px;\n  --form-element-height: 40px;\n  --border-radius: 8px;\n}\n\n.forgot-password-box {\n  width: 500px;\n  max-width: 90%;\n  background-color: white;\n  border-radius: calc(var(--border-radius) * 2);\n  box-shadow: 0 10px 30px rgba(0, 0, 0, 0.15);\n  padding: var(--spacing-lg) var(--spacing-lg);\n  color: #333;\n}\n\n.forgot-password-header {\n  text-align: center;\n  margin-bottom: var(--spacing-lg);\n}\n\n.forgot-password-header img {\n  width: 60px;\n  margin-bottom: 15px;\n}\n\n.forgot-password-header h4 {\n  font-size: var(--font-size-lg);\n  font-weight: var(--font-weight-bold);\n  margin: 1rem 0;\n  color: #333;\n}\n\n.forgot-password-header p {\n  font-size: var(--font-size-base);\n  color: #666;\n}\n\n.recovery-tabs {\n  margin-bottom: var(--spacing-base);\n}\n\n.fetch-button {\n  margin-top: calc(-1 * var(--spacing-sm));\n  margin-bottom: var(--spacing-base);\n}\n\n.security-question-section {\n  margin-top: var(--spacing-base);\n}\n\n.security-question-display {\n  background-color: #f4f6f9;\n  border-radius: var(--border-radius);\n  padding: var(--spacing-sm);\n  margin: var(--spacing-sm) 0 var(--spacing-base);\n  color: #333;\n}\n\n.submit-button {\n  width: 100%;\n  margin-top: var(--spacing-sm);\n}\n\n.back-to-login {\n  margin-top: var(--spacing-lg);\n  text-align: center;\n}\n\n.back-to-login a {\n  color: #409EFF;\n  display: inline-flex;\n  align-items: center;\n  text-decoration: none;\n}\n\n.form-text {\n  color: #666;\n  font-size: var(--font-size-sm);\n  margin-bottom: var(--spacing-base);\n}\n\n/* 修改表单组件样式 */\n:deep(.el-tabs__item) {\n  color: #606266;\n}\n\n:deep(.el-tabs__item.is-active) {\n  color: #409EFF;\n}\n\n:deep(.el-tabs__active-bar) {\n  background-color: #409EFF;\n}\n\n:deep(.el-input__wrapper) {\n  background-color: white;\n  box-shadow: 0 0 0 1px #dcdfe6 inset;\n}\n\n:deep(.el-input__inner) {\n  color: #333;\n}\n\n:deep(.el-button--primary) {\n  background-color: #409EFF;\n  color: white;\n}\n\n:deep(.el-button--info) {\n  background-color: #909399;\n  color: white;\n}\n</style> "]}]}