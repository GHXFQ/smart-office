CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;IAClE;EACF;AACF","file":"C:/Users/18083/Desktop/CCIT/office/smart-office/frontend/src/components/settings/ThemeSettings.vue","sourceRoot":"","sourcesContent":["<template>\r\n  <div class=\"settings-panel\">\r\n    <div class=\"settings-header\">\r\n      <h2>系统设置</h2>\r\n      <div class=\"settings-actions\">\r\n        <el-button @click=\"restoreDefaults\" type=\"text\">\r\n          <el-icon><Refresh /></el-icon> 恢复默认\r\n        </el-button>\r\n        <el-button @click=\"saveSettings\" type=\"primary\">\r\n          <el-icon><Check /></el-icon> 保存设置\r\n        </el-button>\r\n      </div>\r\n    </div>\r\n\r\n    <div class=\"settings-content\">\r\n      <el-menu class=\"settings-sidebar\" :default-active=\"activeSection\">\r\n        <el-menu-item index=\"ai\" @click=\"activeSection = 'ai'\">\r\n          <el-icon><Setting /></el-icon>\r\n          <span>个性化AI设置</span>\r\n        </el-menu-item>\r\n        <el-menu-item index=\"ui\" @click=\"activeSection = 'ui'\">\r\n          <el-icon><Brush /></el-icon>\r\n          <span>界面偏好</span>\r\n        </el-menu-item>\r\n      </el-menu>\r\n\r\n      <div class=\"settings-main\">\r\n        <!-- 主题设置 -->\r\n        <div v-if=\"activeSection === 'ui'\" class=\"settings-section\">\r\n          <h3>主题设置</h3>\r\n          \r\n          <div class=\"setting-item\">\r\n            <span class=\"setting-label\">主题模式</span>\r\n            <el-radio-group v-model=\"settings.theme\" size=\"large\">\r\n              <el-radio-button label=\"light\">\r\n                <el-icon><Sunny /></el-icon> 浅色\r\n              </el-radio-button>\r\n              <el-radio-button label=\"dark\">\r\n                <el-icon><Moon /></el-icon> 深色\r\n              </el-radio-button>\r\n            </el-radio-group>\r\n          </div>\r\n\r\n          <h3>布局设置</h3>\r\n          \r\n          <div class=\"setting-item\">\r\n            <span class=\"setting-label\">内容密度</span>\r\n            <el-radio-group v-model=\"settings.density\" size=\"large\">\r\n              <el-radio-button label=\"compact\">紧凑</el-radio-button>\r\n              <el-radio-button label=\"standard\">标准</el-radio-button>\r\n              <el-radio-button label=\"comfortable\">宽松</el-radio-button>\r\n            </el-radio-group>\r\n          </div>\r\n\r\n          <div class=\"setting-item\">\r\n            <span class=\"setting-label\">字体大小</span>\r\n            <div class=\"font-size-slider\">\r\n              <el-slider \r\n                v-model=\"settings.fontSize\" \r\n                :min=\"12\" \r\n                :max=\"20\" \r\n                :step=\"1\" \r\n                show-input\r\n                :format-tooltip=\"value => `${value}px`\"\r\n              ></el-slider>\r\n            </div>\r\n          </div>\r\n        </div>\r\n\r\n        <!-- AI设置部分 (简化版) -->\r\n        <div v-if=\"activeSection === 'ai'\" class=\"settings-section\">\r\n          <h3>个性化AI设置</h3>\r\n          <el-empty description=\"暂未实现\"></el-empty>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { Refresh, Check, Setting, Brush, Sunny, Moon } from '@element-plus/icons-vue';\r\n\r\nexport default {\r\n  name: 'ThemeSettings',\r\n  components: {\r\n    Refresh, \r\n    Check, \r\n    Setting, \r\n    Brush, \r\n    Sunny, \r\n    Moon\r\n  },\r\n  data() {\r\n    return {\r\n      activeSection: 'ui',\r\n      settings: {\r\n        theme: localStorage.getItem('theme') || 'light',\r\n        density: localStorage.getItem('density') || 'standard',\r\n        fontSize: parseInt(localStorage.getItem('fontSize')) || 14\r\n      },\r\n      defaultSettings: {\r\n        theme: 'light',\r\n        density: 'standard',\r\n        fontSize: 14\r\n      }\r\n    }\r\n  },\r\n  watch: {\r\n    'settings.theme': {\r\n      handler(newVal) {\r\n        this.applyTheme(newVal);\r\n      },\r\n      immediate: true\r\n    },\r\n    'settings.fontSize': {\r\n      handler(newVal) {\r\n        document.documentElement.style.setProperty('--font-size-base', `${newVal}px`);\r\n        localStorage.setItem('fontSize', newVal);\r\n      },\r\n      immediate: true\r\n    },\r\n    'settings.density': {\r\n      handler(newVal) {\r\n        document.body.setAttribute('data-density', newVal);\r\n        localStorage.setItem('density', newVal);\r\n      },\r\n      immediate: true\r\n    }\r\n  },\r\n  methods: {\r\n    applyTheme(theme) {\r\n      document.documentElement.setAttribute('data-theme', theme);\r\n      localStorage.setItem('theme', theme);\r\n      \r\n      // 更新meta主题色\r\n      const metaThemeColor = document.querySelector('meta[name=\"theme-color\"]');\r\n      if (metaThemeColor) {\r\n        metaThemeColor.setAttribute('content', theme === 'dark' ? '#1a1a1a' : '#ffffff');\r\n      }\r\n    },\r\n    saveSettings() {\r\n      // 保存设置到localStorage\r\n      localStorage.setItem('theme', this.settings.theme);\r\n      localStorage.setItem('density', this.settings.density);\r\n      localStorage.setItem('fontSize', this.settings.fontSize);\r\n      \r\n      // 应用设置\r\n      this.applyTheme(this.settings.theme);\r\n      document.body.setAttribute('data-density', this.settings.density);\r\n      document.documentElement.style.setProperty('--font-size-base', `${this.settings.fontSize}px`);\r\n      \r\n      // 通知用户\r\n      this.$message({\r\n        message: '设置已保存',\r\n        type: 'success'\r\n      });\r\n      \r\n      // 关闭设置面板\r\n      this.$emit('close');\r\n      // 关闭对话框\r\n      this.$store.commit('SET_SETTINGS_VISIBLE', false);\r\n    },\r\n    restoreDefaults() {\r\n      this.settings = JSON.parse(JSON.stringify(this.defaultSettings));\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.settings-panel {\r\n  width: 100%;\r\n  height: 100%;\r\n  display: flex;\r\n  flex-direction: column;\r\n  background-color: var(--bg-color);\r\n  color: var(--text-color);\r\n  \r\n  .settings-header {\r\n    display: flex;\r\n    justify-content: space-between;\r\n    align-items: center;\r\n    padding: 16px 20px;\r\n    border-bottom: 1px solid var(--border-color);\r\n    \r\n    h2 {\r\n      margin: 0;\r\n      font-size: 18px;\r\n      font-weight: 500;\r\n    }\r\n    \r\n    .settings-actions {\r\n      display: flex;\r\n      gap: 12px;\r\n    }\r\n  }\r\n  \r\n  .settings-content {\r\n    display: flex;\r\n    flex: 1;\r\n    overflow: hidden;\r\n    \r\n    .settings-sidebar {\r\n      width: 200px;\r\n      border-right: 1px solid var(--border-color);\r\n    }\r\n    \r\n    .settings-main {\r\n      flex: 1;\r\n      padding: 20px;\r\n      overflow-y: auto;\r\n      \r\n      .settings-section {\r\n        margin-bottom: 24px;\r\n        \r\n        h3 {\r\n          font-size: 16px;\r\n          font-weight: 500;\r\n          margin-top: 0;\r\n          margin-bottom: 16px;\r\n          padding-bottom: 8px;\r\n          border-bottom: 1px solid var(--border-color);\r\n        }\r\n      }\r\n      \r\n      .setting-item {\r\n        margin-bottom: 20px;\r\n        \r\n        .setting-label {\r\n          display: block;\r\n          font-size: 14px;\r\n          margin-bottom: 8px;\r\n        }\r\n        \r\n        .font-size-slider {\r\n          margin-top: 10px;\r\n        }\r\n      }\r\n    }\r\n  }\r\n}\r\n\r\n:deep(.el-menu) {\r\n  background-color: transparent;\r\n  border-right: none;\r\n}\r\n\r\n:deep(.el-menu-item) {\r\n  color: var(--text-color);\r\n  \r\n  &:hover {\r\n    background-color: var(--hover-color);\r\n  }\r\n  \r\n  &.is-active {\r\n    background-color: var(--hover-color);\r\n    color: var(--primary-color);\r\n  }\r\n}\r\n\r\n:deep(.el-radio-button__original-radio:checked + .el-radio-button__inner) {\r\n  background-color: var(--primary-color);\r\n  border-color: var(--primary-color);\r\n  box-shadow: -1px 0 0 0 var(--primary-color);\r\n}\r\n\r\n:deep(.el-slider__runway) {\r\n  background-color: var(--slider-bg);\r\n}\r\n\r\n:deep(.el-slider__bar) {\r\n  background-color: var(--primary-color);\r\n}\r\n\r\n:deep(.el-slider__button) {\r\n  border-color: var(--primary-color);\r\n}\r\n</style> "]}]}