\"/\">\n        <el-breadcrumb-item :to=\"{ path: '/' }\">首页</el-breadcrumb-item>\n        <el-breadcrumb-item>{{ currentRoute }}</el-breadcrumb-item>\n      </el-breadcrumb>\n    </div>\n\n    <!-- 右侧菜单 -->\n    <div class=\"right-menu\">\n      <!-- 全局搜索 -->\n      <div class=\"search-container\">\n        <el-input\n          v-model=\"searchInput\"\n          placeholder=\"全局搜索...\"\n          clearable\n          :prefix-icon=\"Search\"\n          class=\"search-input\"\n        />\n      </div>\n\n      <!-- 通知菜单 -->\n      <el-dropdown class=\"notification-container\" trigger=\"click\" :teleported=\"true\" @visible-change=\"handleDropdownChange\">\n        <div class=\"notification-icon-container\">\n          <el-badge :value=\"totalUnread\" class=\"item\" :hidden=\"totalUnread === 0\">\n            <el-icon size=\"18\"><Bell /></el-icon>\n          </el-badge>\n        </div>\n        <template #dropdown>\n          <el-dropdown-menu>\n            <el-dropdown-item>\n              <el-icon><ChatDotRound /></el-icon> {{ unread.messages }} 条未读消息\n            </el-dropdown-item>\n            <el-dropdown-item>\n              <el-icon><Bell /></el-icon> {{ unread.notifications }} 条未读通知\n            </el-dropdown-item>\n            <el-dropdown-item>\n              <el-icon><CheckboxButton /></el-icon> {{ unread.tasks }} 个未完成任务\n            </el-dropdown-item>\n            <el-dropdown-item>\n              <el-icon><List /></el-icon> {{ unread.approvals }} 个待审批\n            </el-dropdown-item>\n            <el-dropdown-item divided>\n              <span style=\"display: block; text-align: center;\">查看全部通知</span>\n            </el-dropdown-item>\n          </el-dropdown-menu>\n        </template>\n      </el-dropdown>\n\n      <!-- 用户头像菜单 -->\n      <el-dropdown class=\"avatar-container\" trigger=\"click\" :teleported=\"true\" @visible-change=\"handleDropdownChange\">\n        <div class=\"avatar-wrapper\">\n          <img :src=\"userAvatar\" class=\"user-avatar\">\n          <span v-if=\"username\" class=\"username\">{{ username }}</span>\n          <el-icon><ArrowDown /></el-icon>\n        </div>\n        <template #dropdown>\n          <el-dropdown-menu>\n            <el-dropdown-item>\n              <router-link to=\"/profile\" class=\"dropdown-link\">\n                <el-icon><User /></el-icon> 个人中心\n              </router-link>\n            </el-dropdown-item>\n            <el-dropdown-item>\n              <span @click=\"openSettings\" style=\"display: block;\">\n                <el-icon><Setting /></el-icon> 系统设置\n              </span>\n            </el-dropdown-item>\n            <el-dropdown-item divided>\n              <span @click=\"logout\" style=\"display: block;\">\n                <el-icon><SwitchButton /></el-icon> 退出登录\n              </span>\n            </el-dropdown-item>\n          </el-dropdown-menu>\n        </template>\n      </el-dropdown>\n    </div>\n    \n    <!-- 设置弹窗组件 -->\n    <settings-dialog v-model:visible=\"settingsVisible\" />\n  </div>\n</template>\n\n<script>\nimport { computed, ref } from 'vue'\nimport { useStore } from 'vuex'\nimport { useRoute, useRouter } from 'vue-router'\nimport { ElMessage } from 'element-plus'\nimport { \n  Menu, \n  Search, \n  Bell, \n  ChatDotRound, \n  List, \n  User, \n  Setting, \n  SwitchButton, \n  ArrowDown\n} from '@element-plus/icons-vue'\nimport CheckboxButton from '@/components/icons/CheckboxButton.vue'\nimport SettingsDialog from '@/components/settings/SettingsDialog.vue'\n\nexport default {\n  name: 'Navbar',\n  components: {\n    SettingsDialog\n  },\n  setup() {\n    const store = useStore()\n    const route = useRoute()\n    const router = useRouter()\n\n    const searchInput = ref('')\n    \n    // 从Vuex获取侧边栏状态\n    const isActive = computed(() => store.getters.sidebar.opened)\n    \n    // 从Vuex获取用户信息和未读消息\n    const userAvatar = computed(() => {\n      const avatar = store.getters.user && store.getters.user.avatar;\n      if (!avatar) {\n        return '/img/default-avatar.svg'; // 更新默认头像路径\n      }\n      // 检查是否是完整URL\n      if (avatar.startsWith('http://') || avatar.startsWith('https://')) {\n        return avatar;\n      }\n      // 添加服务器地址前缀，确保不会重复添加前缀\n      if (avatar.startsWith('/')) {\n        return `http://localhost:8000${avatar}`;\n      } else {\n        return `http://localhost:8000/${avatar}`;\n      }\n    });\n    const username = computed(() => {\n      // 从store中获取用户信息\n      const user = store.getters.user;\n      // 优先获取name，如果没有则尝试获取username\n      if (user) {\n        return user.name || user.username || '';\n      }\n      return '';\n    });\n    const unread = computed(() => store.getters.unread || { messages: 0, notifications: 0, tasks: 0, approvals: 0 })\n    \n    // 计算未读消息总数\n    const totalUnread = computed(() => \n      unread.value.messages + \n      unread.value.notifications + \n      unread.value.tasks + \n      unread.value.approvals\n    )\n\n    // 获取当前路由标题\n    const currentRoute = computed(() => {\n      return route.meta.title || '工作台'\n    })\n\n    // 切换侧边栏\n    const toggleSidebar = () => {\n      store.dispatch('toggleSidebar')\n    }\n\n    // 退出登录\n    const logout = async () => {\n      try {\n        await store.dispatch('user/logout')\n        // 成功退出后，明确重置loading状态\n        store.commit('user/AUTH_REQUEST', false)\n        ElMessage.success('退出登录成功')\n        router.push('/login')\n      } catch (error) {\n        ElMessage.error('退出登录失败，请重试')\n        console.error('登出失败:', error)\n        // 失败时也确保重置loading状态\n        store.commit('user/AUTH_REQUEST', false)\n      }\n    }\n    \n    // 处理下拉菜单变化，防止事件冒泡\n    const handleDropdownChange = () => {\n      // 下拉菜单在Element Plus中已经处理了自身的事件冒泡，\n      // 所以我们不需要额外添加事件监听器了\n    }\n\n    // 打开系统设置\n    const openSettings = () => {\n      // 显示设置对话框\n      store.commit('SET_SETTINGS_VISIBLE', true);\n    }\n\n    // 从Vuex获取设置对话框可见状态\n    const settingsVisible = computed({\n      get: () => store.state.settingsVisible || false,\n      set: (value) => store.commit('SET_SETTINGS_VISIBLE', value)\n    })\n\n    return {\n      // 图标组件\n      Menu,\n      Search,\n      Bell,\n      ChatDotRound,\n      List,\n      User,\n      Setting,\n      SwitchButton,\n      ArrowDown,\n      CheckboxButton,\n      // 其他数据\n      searchInput,\n      isActive,\n      userAvatar,\n      username,\n      unread,\n      totalUnread,\n      currentRoute,\n      toggleSidebar,\n      logout,\n      handleDropdownChange,\n      openSettings,\n      settingsVisible\n    }\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n.navbar {\n  height: 60px;\n  position: fixed;\n  top: 0;\n  left: 0;\n  right: 0;\n  z-index: 1000;\n  overflow: hidden;\n  display: flex;\n  align-items: center;\n  background-color: var(--navbar-bg);\n  box-shadow: 0 1px 4px var(--shadow-color);\n  padding: 0 15px;\n  transition: all 0.3s;\n  color: var(--navbar-text);\n}\n\n.hamburger-container {\n  padding: 0 15px;\n  height: 100%;\n  display: flex;\n  align-items: center;\n  cursor: pointer;\n  transition: background-color 0.3s;\n  \n  &:hover {\n    background-color: var(--hover-color);\n  }\n  \n  .el-icon {\n    color: var(--text-color);\n  }\n}\n\n.breadcrumb-container {\n  margin-left: 10px;\n  \n  :deep(.el-breadcrumb__item) {\n    .el-breadcrumb__inner {\n      color: var(--text-color-secondary);\n      \n      &.is-link {\n        color: var(--text-color-tertiary);\n        \n        &:hover {\n          color: var(--primary-color);\n        }\n      }\n    }\n    \n    &:last-child .el-breadcrumb__inner {\n      color: var(--text-color);\n    }\n  }\n}\n\n.right-menu {\n  margin-left: auto;\n  display: flex;\n  align-items: center;\n  \n  .search-container {\n    margin-right: 15px;\n    \n    .search-input {\n      width: 250px;\n      transition: width 0.3s;\n      \n      :deep(.el-input__wrapper) {\n        background-color: var(--input-bg);\n        border-color: var(--input-border);\n        \n        .el-input__inner {\n          color: var(--input-text);\n          \n          &::placeholder {\n            color: var(--input-placeholder);\n          }\n        }\n      }\n      \n      @media (max-width: 767px) {\n        width: 150px;\n      }\n    }\n  }\n  \n  .notification-container, .avatar-container {\n    cursor: pointer;\n    padding: 0 12px;\n    height: 100%;\n    display: flex;\n    align-items: center;\n    \n    &:hover {\n      background-color: var(--hover-color);\n    }\n  }\n  \n  .notification-icon-container {\n    font-size: 18px;\n    color: var(--text-color);\n    display: flex;\n    align-items: center;\n  }\n  \n  .avatar-wrapper {\n    display: flex;\n    align-items: center;\n    \n    .user-avatar {\n      width: 30px;\n      height: 30px;\n      border-radius: 8px;\n      margin-right: 8px;\n    }\n    \n    .username {\n      margin-right: 8px;\n      font-size: 14px;\n      color: var(--text-color);\n      white-space: nowrap;\n      max-width: 80px;\n      overflow: hidden;\n      text-overflow: ellipsis;\n      \n      @media (max-width: 767px) {\n        display: none;\n      }\n    }\n  }\n}\n\n:deep(.el-dropdown-menu) {\n  background-color: var(--dropdown-bg);\n  border-color: var(--border-color);\n  \n  .el-dropdown-menu__item {\n    color: var(--dropdown-text);\n    \n    &:hover {\n      background-color: var(--dropdown-hover-bg);\n    }\n    \n    .dropdown-link {\n      display: block;\n      text-decoration: none;\n      color: inherit;\n    }\n  }\n  \n  .el-dropdown-menu__item--divided::before {\n    background-color: var(--border-color);\n  }\n}\n</style> "]}]}