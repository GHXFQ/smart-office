:lg=\"24\" :xl=\"24\">\r\n        <div class=\"welcome-card\">\r\n          <div class=\"welcome-content\">\r\n            <h4>欢迎回来，{{ username }}</h4>\r\n            <p>{{ greeting }}，祝您工作愉快！</p>\r\n            <el-button type=\"primary\" plain size=\"small\" class=\"quick-start-btn\">\r\n              <i class=\"bi bi-lightning\"></i> 快速开始\r\n            </el-button>\r\n          </div>\r\n          <i class=\"bi bi-person-workspace welcome-icon\"></i>\r\n        </div>\r\n      </el-col>\r\n    </el-row>\r\n\r\n    <el-row :gutter=\"20\" class=\"mt-20\">\r\n      <!-- 统计卡片 -->\r\n      <el-col :xs=\"24\" :sm=\"12\" :md=\"6\" :lg=\"6\" :xl=\"6\">\r\n        <el-card class=\"stats-card\">\r\n          <div class=\"stats-icon icon-blue\">\r\n            <i class=\"bi bi-envelope\"></i>\r\n          </div>\r\n          <div class=\"stats-content\">\r\n            <div class=\"stats-title\">未读邮件</div>\r\n            <div class=\"stats-value\">{{ unread.messages }}</div>\r\n          </div>\r\n        </el-card>\r\n      </el-col>\r\n      \r\n      <el-col :xs=\"24\" :sm=\"12\" :md=\"6\" :lg=\"6\" :xl=\"6\">\r\n        <el-card class=\"stats-card\">\r\n          <div class=\"stats-icon icon-green\">\r\n            <i class=\"bi bi-check2-all\"></i>\r\n          </div>\r\n          <div class=\"stats-content\">\r\n            <div class=\"stats-title\">待办任务</div>\r\n            <div class=\"stats-value\">{{ unread.tasks }}</div>\r\n          </div>\r\n        </el-card>\r\n      </el-col>\r\n      \r\n      <el-col :xs=\"24\" :sm=\"12\" :md=\"6\" :lg=\"6\" :xl=\"6\">\r\n        <el-card class=\"stats-card\">\r\n          <div class=\"stats-icon icon-orange\">\r\n            <i class=\"bi bi-list-check\"></i>\r\n          </div>\r\n          <div class=\"stats-content\">\r\n            <div class=\"stats-title\">待审批</div>\r\n            <div class=\"stats-value\">{{ unread.approvals }}</div>\r\n          </div>\r\n        </el-card>\r\n      </el-col>\r\n      \r\n      <el-col :xs=\"24\" :sm=\"12\" :md=\"6\" :lg=\"6\" :xl=\"6\">\r\n        <el-card class=\"stats-card\">\r\n          <div class=\"stats-icon icon-red\">\r\n            <i class=\"bi bi-calendar-event\"></i>\r\n          </div>\r\n          <div class=\"stats-content\">\r\n            <div class=\"stats-title\">今日日程</div>\r\n            <div class=\"stats-value\">3</div>\r\n          </div>\r\n        </el-card>\r\n      </el-col>\r\n    </el-row>\r\n\r\n    <el-row :gutter=\"20\" class=\"mt-20\">\r\n      <!-- 快捷功能 -->\r\n      <el-col :xs=\"24\" :sm=\"24\" :md=\"12\" :lg=\"8\" :xl=\"8\">\r\n        <el-card class=\"box-card\">\r\n          <template #header>\r\n            <div class=\"card-header\">\r\n              <span>常用功能</span>\r\n              <el-button type=\"text\" class=\"more-link\">自定义</el-button>\r\n            </div>\r\n          </template>\r\n          <div class=\"card-body\">\r\n            <el-row :gutter=\"15\">\r\n              <el-col :span=\"6\" v-for=\"(item, index) in shortcutItems\" :key=\"index\">\r\n                <router-link :to=\"item.link\" class=\"shortcut-item\">\r\n                  <div class=\"shortcut-icon\" :class=\"item.iconClass\">\r\n                    <i :class=\"item.icon\"></i>\r\n                  </div>\r\n                  <div class=\"shortcut-title\">{{ item.title }}</div>\r\n                </router-link>\r\n              </el-col>\r\n            </el-row>\r\n          </div>\r\n        </el-card>\r\n      </el-col>\r\n\r\n      <!-- 最近任务 -->\r\n      <el-col :xs=\"24\" :sm=\"24\" :md=\"12\" :lg=\"8\" :xl=\"8\">\r\n        <el-card class=\"box-card\">\r\n          <template #header>\r\n            <div class=\"card-header\">\r\n              <span>最近任务</span>\r\n              <el-button type=\"text\" class=\"more-link\">查看全部</el-button>\r\n            </div>\r\n          </template>\r\n          <div class=\"card-body recent-tasks\">\r\n            <div class=\"recent-task-item\" v-for=\"(task, index) in recentTasks\" :key=\"index\">\r\n              <div class=\"task-icon\" :class=\"task.iconClass\">\r\n                <i :class=\"task.icon\"></i>\r\n              </div>\r\n              <div class=\"task-content\">\r\n                <div class=\"task-title\">{{ task.title }}</div>\r\n                <div class=\"task-info\">{{ task.deadline }} · {{ task.creator }}</div>\r\n              </div>\r\n              <div class=\"task-status\" :class=\"'status-' + task.status\">\r\n                {{ task.statusText }}\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </el-card>\r\n      </el-col>\r\n\r\n      <!-- 通知消息 -->\r\n      <el-col :xs=\"24\" :sm=\"24\" :md=\"12\" :lg=\"8\" :xl=\"8\">\r\n        <el-card class=\"box-card\">\r\n          <template #header>\r\n            <div class=\"card-header\">\r\n              <span>通知消息</span>\r\n              <el-button type=\"text\" class=\"more-link\">查看全部</el-button>\r\n            </div>\r\n          </template>\r\n          <div class=\"card-body notifications\">\r\n            <div class=\"notification-item\" v-for=\"(notification, index) in notifications\" :key=\"index\">\r\n              <div class=\"notification-content\">\r\n                <div class=\"notification-title\">{{ notification.title }}</div>\r\n                <div class=\"notification-time\">{{ notification.time }}</div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </el-card>\r\n      </el-col>\r\n    </el-row>\r\n\r\n    <el-row :gutter=\"20\" class=\"mt-20\">\r\n      <!-- 日程安排 -->\r\n      <el-col :xs=\"24\" :sm=\"24\" :md=\"12\" :lg=\"12\" :xl=\"12\">\r\n        <el-card class=\"box-card\">\r\n          <template #header>\r\n            <div class=\"card-header\">\r\n              <span>近期日程</span>\r\n              <el-button type=\"text\" class=\"more-link\">查看日历</el-button>\r\n            </div>\r\n          </template>\r\n          <div class=\"card-body schedule\">\r\n            <div class=\"calendar-event\" v-for=\"(event, index) in calendarEvents\" :key=\"index\">\r\n              <div class=\"calendar-date\">\r\n                <span class=\"calendar-month\">{{ event.month }}</span>\r\n                <span class=\"calendar-day\">{{ event.day }}</span>\r\n              </div>\r\n              <div class=\"event-content\">\r\n                <div class=\"event-title\">{{ event.title }}</div>\r\n                <div class=\"event-time\">{{ event.time }} · {{ event.location }}</div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </el-card>\r\n      </el-col>\r\n\r\n      <!-- 工作动态 -->\r\n      <el-col :xs=\"24\" :sm=\"24\" :md=\"12\" :lg=\"12\" :xl=\"12\">\r\n        <el-card class=\"box-card\">\r\n          <template #header>\r\n            <div class=\"card-header\">\r\n              <span>团队动态</span>\r\n              <el-button type=\"text\" class=\"more-link\">查看全部</el-button>\r\n            </div>\r\n          </template>\r\n          <div class=\"card-body activities\">\r\n            <el-timeline>\r\n              <el-timeline-item\r\n                v-for=\"(activity, index) in activities\"\r\n                :key=\"index\"\r\n                :type=\"activity.type\"\r\n                :color=\"activity.color\"\r\n                :timestamp=\"activity.time\"\r\n                :hollow=\"activity.hollow\"\r\n              >\r\n                {{ activity.content }}\r\n              </el-timeline-item>\r\n            </el-timeline>\r\n          </div>\r\n        </el-card>\r\n      </el-col>\r\n    </el-row>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { computed, ref } from 'vue'\r\nimport { useStore } from 'vuex'\r\n\r\nexport default {\r\n  name: 'Dashboard',\r\n  setup() {\r\n    const store = useStore()\r\n    \r\n    // 从Vuex获取用户信息和未读消息\r\n    const username = computed(() => store.getters.user && store.getters.user.name || '用户')\r\n    const unread = computed(() => store.getters.unread)\r\n    \r\n    // 根据时间生成问候语\r\n    const greeting = computed(() => {\r\n      const hour = new Date().getHours()\r\n      if (hour < 6) return '凌晨好'\r\n      if (hour < 9) return '早上好'\r\n      if (hour < 12) return '上午好'\r\n      if (hour < 14) return '中午好'\r\n      if (hour < 17) return '下午好'\r\n      if (hour < 19) return '傍晚好'\r\n      if (hour < 22) return '晚上好'\r\n      return '夜深了'\r\n    })\r\n\r\n    // 快捷功能项\r\n    const shortcutItems = ref([\r\n      { title: '发起审批', icon: 'bi bi-list-check', iconClass: 'icon-blue', link: '/approval' },\r\n      { title: '日程安排', icon: 'bi bi-calendar-week', iconClass: 'icon-green', link: '/calendar' },\r\n      { title: '会议预约', icon: 'bi bi-camera-video', iconClass: 'icon-orange', link: '/meeting' },\r\n      { title: '新建文档', icon: 'bi bi-file-earmark-text', iconClass: 'icon-purple', link: '/document' },\r\n      { title: '邮件发送', icon: 'bi bi-envelope', iconClass: 'icon-red', link: '/email' },\r\n      { title: '物资申领', icon: 'bi bi-box-seam', iconClass: 'icon-blue', link: '/supplies' },\r\n      { title: '创建任务', icon: 'bi bi-check2-all', iconClass: 'icon-green', link: '/tasks' },\r\n      { title: '通讯录', icon: 'bi bi-person-lines-fill', iconClass: 'icon-orange', link: '/contacts' }\r\n    ])\r\n\r\n    // 最近任务\r\n    const recentTasks = ref([\r\n      { \r\n        title: '完成Q2季度财务报表分析', \r\n        deadline: '今天 14:30', \r\n        creator: '李经理',\r\n        icon: 'bi bi-file-earmark-spreadsheet', \r\n        iconClass: 'icon-orange',\r\n        status: 'pending',\r\n        statusText: '待处理'\r\n      },\r\n      { \r\n        title: '客户需求调研会议', \r\n        deadline: '明天 10:00', \r\n        creator: '张总',\r\n        icon: 'bi bi-people', \r\n        iconClass: 'icon-blue',\r\n        status: 'processing',\r\n        statusText: '进行中'\r\n      },\r\n      { \r\n        title: '产品原型评审', \r\n        deadline: '已超时 1 天', \r\n        creator: '王设计',\r\n        icon: 'bi bi-layout-text-window', \r\n        iconClass: 'icon-red',\r\n        status: 'overdue',\r\n        statusText: '已超时'\r\n      },\r\n      { \r\n        title: '新员工入职培训', \r\n        deadline: '已完成', \r\n        creator: '人事部',\r\n        icon: 'bi bi-person-plus', \r\n        iconClass: 'icon-green',\r\n        status: 'completed',\r\n        statusText: '已完成'\r\n      }\r\n    ])\r\n\r\n    // 通知消息\r\n    const notifications = ref([\r\n      { title: '您有一个新的审批待处理', time: '10分钟前' },\r\n      { title: '张经理邀请您参加项目启动会', time: '30分钟前' },\r\n      { title: '系统维护通知：本周六凌晨2点-4点进行系统升级', time: '1小时前' },\r\n      { title: '您的请假申请已通过审批', time: '昨天' },\r\n      { title: '财务部发布了新的报销制度', time: '2天前' }\r\n    ])\r\n\r\n    // 日程安排\r\n    const calendarEvents = ref([\r\n      { month: '5月', day: '6', title: '产品季度规划会', time: '10:00-12:00', location: '三楼会议室' },\r\n      { month: '5月', day: '8', title: '客户需求沟通', time: '14:30-16:00', location: '线上会议' },\r\n      { month: '5月', day: '10', title: '团队建设活动', time: '全天', location: '市郊森林公园' }\r\n    ])\r\n\r\n    // 团队动态\r\n    const activities = ref([\r\n      { content: '王工程师 提交了新版本代码', time: '刚刚', type: 'primary', color: '#0bbd87' },\r\n      { content: '张设计 上传了产品原型设计文件', time: '1小时前', type: 'success', color: '#409EFF' },\r\n      { content: '李经理 批准了张三的请假申请', time: '2小时前', type: 'info', hollow: true },\r\n      { content: '系统 自动生成了周工作报告', time: '昨天 18:30', type: 'warning', hollow: true },\r\n      { content: '赵总 创建了新项目 \"智能办公优化\"', time: '前天 09:10', type: 'danger', hollow: true }\r\n    ])\r\n\r\n    return {\r\n      username,\r\n      greeting,\r\n      unread,\r\n      shortcutItems,\r\n      recentTasks,\r\n      notifications,\r\n      calendarEvents,\r\n      activities\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.dashboard-container {\r\n  padding: 0 10px;\r\n}\r\n\r\n.mt-20 {\r\n  margin-top: 20px;\r\n}\r\n\r\n.welcome-card {\r\n  background: linear-gradient(135deg, #0061f2 0%, #6610f2 100%);\r\n  color: white;\r\n  padding: 30px;\r\n  border-radius: 10px;\r\n  position: relative;\r\