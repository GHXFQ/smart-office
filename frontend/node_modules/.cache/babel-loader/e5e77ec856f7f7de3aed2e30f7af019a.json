    // 处理其他HTML相关替换\n    else if (suggestion.original && suggestion.suggested) {\n      const escapedOriginal = suggestion.original.replace(/[.*+?^${}()|[\\]\\\\]/g, \"\\\\$&\");\n      const regex = new RegExp(escapedOriginal, \"g\");\n      \n      if (htmlContent.match(regex)) {\n        return htmlContent.replace(regex, suggestion.suggested);\n      }\n    }\n  }\n  \n  // 处理甲乙方表述\n  if (suggestion.explanation) {\n    // 处理甲方表述\n    if (suggestion.explanation.includes(\"甲方\")) {\n      const regex = /甲\\s*方[^。，；,;.]{0,30}/g;\n      if (htmlContent.match(regex)) {\n        return htmlContent.replace(regex, suggestion.suggested);\n      }\n    }\n    // 处理乙方表述\n    else if (suggestion.explanation.includes(\"乙方\")) {\n      const regex = /乙\\s*方[^。，；,;.]{0,30}/g;\n      if (htmlContent.match(regex)) {\n        return htmlContent.replace(regex, suggestion.suggested);\n      }\n    }\n  }\n  \n  // 如果没有特殊处理，返回null表示需要继续尝试其他方法\n  return null;\n}\r\n"],"mappings":"AAAA;AACA,OAAO,SAASA,sBAAsBA,CAACC,UAAU,EAAEC,WAAW,EAAE;EAC9D;EACA,IAAID,UAAU,CAACE,WAAW,KACtBF,UAAU,CAACE,WAAW,CAACC,QAAQ,CAAC,IAAI,CAAC,IACrCH,UAAU,CAACE,WAAW,CAACC,QAAQ,CAAC,MAAM,CAAC,IACvCH,UAAU,CAACE,WAAW,CAACC,QAAQ,CAAC,KAAK,CAAC,CACvC,EAAE;IACH;IACA,IAAIH,UAAU,CAACE,WAAW,CAACC,QAAQ,CAAC,KAAK,CAAC,IAAIH,UAAU,CAACE,WAAW,CAACC,QAAQ,CAAC,IAAI,CAAC,EAAE;MACnF,MAAMC,KAAK,GAAG,yBAAyB;MACvC,IAAIH,WAAW,CAACI,KAAK,CAACD,KAAK,CAAC,EAAE;QAC5B,OAAOH,WAAW,CAACK,OAAO,CAACF,KAAK,EAAE,MAAM,CAAC;MAC3C;IACF;IACA;IAAA,KACK,IAAIJ,UAAU,CAACO,QAAQ,IAAIP,UAAU,CAACQ,SAAS,EAAE;MACpD,MAAMC,eAAe,GAAGT,UAAU,CAACO,QAAQ,CAACD,OAAO,CAAC,qBAAqB,EAAE,MAAM,CAAC;MAClF,MAAMF,KAAK,GAAG,IAAIM,MAAM,CAACD,eAAe,EAAE,GAAG,CAAC;MAE9C,IAAIR,WAAW,CAACI,KAAK,CAACD,KAAK,CAAC,EAAE;QAC5B,OAAOH,WAAW,CAACK,OAAO,CAACF,KAAK,EAAEJ,UAAU,CAACQ,SAAS,CAAC;MACzD;IACF;EACF;;EAEA;EACA,IAAIR,UAAU,CAACE,WAAW,EAAE;IAC1B;IACA,IAAIF,UAAU,CAACE,WAAW,CAACC,QAAQ,CAAC,IAAI,CAAC,EAAE;MACzC,MAAMC,KAAK,GAAG,uBAAuB;MACrC,IAAIH,WAAW,CAACI,KAAK,CAACD,KAAK,CAAC,EAAE;QAC5B,OAAOH,