n        if (iframe.contentDocument) {\r\n          iframe.contentDocument.documentElement.setAttribute('data-theme', 'dark')\r\n          iframe.contentDocument.body.classList.add('dark-theme')\r\n        }\r\n      } catch (e) {\r\n        console.log('无法访问iframe内容')\r\n      }\r\n    })\r\n  } catch (e) {\r\n    console.error('处理iframe时出错', e)\r\n  }\r\n  \r\n  // 处理特殊元素\r\n  try {\r\n    // 导航和菜单\r\n    const menuElements = document.querySelectorAll('.el-menu, .navbar, nav, .el-menu-item, .el-submenu__title')\r\n    menuElements.forEach(el => {\r\n      el.style.backgroundColor = '#000000'\r\n      el.style.color = '#ffffff'\r\n    })\r\n    \r\n    // 处理所有白色背景容器\r\n    const whiteContainers = document.querySelectorAll('.white-bg, .bg-white, [class*=\"bg-white\"], [class*=\"white-bg\"]')\r\n    whiteContainers.forEach(el => {\r\n      el.style.backgroundColor = '#000000'\r\n      el.style.color = '#ffffff'\r\n    })\r\n  } catch (e) {\r\n    console.error('处理特殊元素时出错', e)\r\n  }\r\n}\r\n\r\n// 移除深色主题\r\nfunction removeDarkTheme() {\r\n  // 移除根元素属性\r\n  document.documentElement.removeAttribute('data-theme')\r\n  document.documentElement.classList.remove('dark-theme')\r\n  document.body.classList.remove('dark-theme')\r\n  \r\n  // 处理iframe\r\n  try {\r\n    const iframes = document.querySelectorAll('iframe')\r\n    iframes.forEach(iframe => {\r\n      try {\r\n        if (iframe.contentDocument) {\r\n          iframe.contentDocument.documentElement.removeAttribute('data-theme')\r\n          iframe.contentDocument.body.classList.remove('dark-theme')\r\n        }\r\n      } catch (e) {\r\n        console.log('无法访问iframe内容')\r\n      }\r\n    })\r\n  } catch (e) {\r\n    console.error('处理iframe时出错', e)\r\n  }\r\n  \r\n  // 恢复特殊元素\r\n  try {\r\n    // 恢复导航和菜单\r\n    const menuElements = document.querySelectorAll('.el-menu, .navbar, nav, .el-menu-item, .el-submenu__title')\r\n    menuElements.forEach(el => {\r\n      el.style.backgroundColor = ''\r\n      el.style.color = ''\r\n    })\r\n    \r\n    // 恢复白色背景容器\r\n    const whiteContainers = document.querySelectorAll('.white-bg, .bg-white, [class*=\"bg-white\"], [class*=\"white-bg\"]')\r\n    whiteContainers.forEach(el => {\r\n      el.style.backgroundColor = ''\r\n      el.style.color = ''\r\n    })\r\n  } catch (e) {\r\n    console.error('恢复特殊元素时出错', e)\r\n  }\r\n}\r\n\r\n// 切换主题\r\nfunction toggleTheme(theme) {\r\n  if (theme === 'dark') {\r\n    applyDarkTheme()\r\n  } else {\r\n    removeDarkTheme()\r\n  }\r\n}\r\n\r\n// 初始化主题\r\nfunction initTheme() {\r\n  const savedTheme = localStorage.getItem('app_theme') || 'light'\r\n  toggleTheme(savedTheme)\r\n}\r\n\r\nexport {\r\n  applyDarkTheme,\r\n  removeDarkTheme,\r\n  toggleTheme,\r\n  initTheme\r\n} "],"mappings":";;AAAA;AACA;AACA;;AAEA;AACA,SAASA,cAAcA,CAAA,EAAG;EACxB;EACAC,QAAQ,CAACC,eAAe,CAACC,YAAY,CAAC,YAAY,EAAE,MAAM,CAAC;EAC3DF,QAAQ,CAACC,eAAe,CAACE,SAAS,CAACC,GAAG,CAAC,YAAY,CAAC;EACpDJ,QAAQ,CAACK,IAAI,CAACF,SAAS,CAACC,GAAG,CAAC,YAAY,CAAC;;EAEzC;EACA,IAAI;IACF,MAAME,OAAO,GAAGN,QAAQ,CAACO,gBAAgB,CAAC,QAAQ,CAAC;IACnDD,OAAO,CAACE,OAAO,CAACC,MAAM,IAAI;MACxB,IAAI;QACF,IAAIA,MAAM,CAACC,eAAe,EAAE;UAC1BD,MAAM,CAACC,eAAe,CAACT,eAAe,CAACC,YAAY,CAAC,YAAY,EAAE,MAAM,CAAC;UACzEO,MAAM,CAACC,eAAe,CAACL,IAAI,CAACF,SAAS,CAACC,GAAG,CAAC,YAAY,CAAC;QACzD;MACF,CAAC,CAAC,OAAOO,CAAC,EAAE;QACVC,OAAO,CAACC,GAAG,CAAC,cAAc,CAAC;MAC7B;IACF,CAAC,CAAC;EACJ,CAAC,CAAC,OAAOF,CAAC,EAAE;IACVC,OAAO,CAACE,KAAK,CAAC,aAAa,EAAEH,CAAC,CAAC;EACjC;;EAEA;EACA,IAAI;IACF;IACA,MAAMI,YAAY,GAAGf,QAAQ,CAACO,gBAAgB,CAAC,2DAA2D,CAAC;IAC3GQ,YAAY,CAACP,OAAO,CAACQ,EAAE,IAAI;MACzBA,EAAE,CAACC,KAAK,CAACC,eAAe,GAAG,SAAS;MACpCF,EAAE,CAACC,KAAK,CAACE,KAAK,GAAG,SAAS;IAC5B,CAAC,CAAC;;IAEF;IACA,MAAMC,eAAe,GAAGpB,QAAQ,CAACO,gBAAgB,CAAC,gEAAgE,CAAC;IACnHa,eAAe,CAACZ,OAAO,CAACQ,EAAE,IAAI;MAC5BA,EAAE,CAACC,KAAK,CAACC,eAAe,GAAG,SAAS;MACpCF,EAAE,CAACC,KAAK,CAACE,KAAK,GAAG,SAAS;IAC5B,CAAC,CAAC;EACJ,CAAC,CAAC,OAAOR,CAAC,EAAE;IACVC,OAAO,CAACE,KAAK,CAAC,WAAW,EAAEH,CAAC,CAAC;EAC/B;AACF;;AAEA;AAC