mg src=\"https://cdn-icons-png.flaticon.com/512/2891/2891491.png\" alt=\"Logo\">\r\n      <h5 v-if=\"sidebarOpened\">智能办公系统</h5>\r\n    </div>\r\n\r\n    <!-- 侧边栏菜单 -->\r\n    <el-scrollbar class=\"sidebar-menu-container\">\r\n      <el-menu\r\n        :default-active=\"activeMenu\"\r\n        :collapse=\"!sidebarOpened\"\r\n        background-color=\"#fff\"\r\n        text-color=\"#495057\"\r\n        active-text-color=\"#007bff\"\r\n        :unique-opened=\"true\"\r\n        router\r\n      >\r\n        <el-menu-item v-for=\"route in routes\" :key=\"route.path\" :index=\"route.path\">\r\n          <el-icon>\r\n            <component :is=\"iconComponent(route.meta.icon)\"></component>\r\n          </el-icon>\r\n          <template #title>{{ route.meta.title }}</template>\r\n        </el-menu-item>\r\n      </el-menu>\r\n    </el-scrollbar>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { computed } from 'vue'\r\nimport { useRouter, useRoute } from 'vue-router'\r\nimport { useStore } from 'vuex'\r\n\r\nexport default {\r\n  name: 'Sidebar',\r\n  setup() {\r\n    const store = useStore()\r\n    const router = useRouter()\r\n    const route = useRoute()\r\n\r\n    // 从Vuex中获取侧边栏状态\r\n    const sidebarOpened = computed(() => store.getters.sidebar.opened)\r\n\r\n    // 菜单路由\r\n    const routes = computed(() => {\r\n      return router.options.routes.find(r => r.path === '/').children\r\n        .filter(route => !route.hidden)\r\n    })\r\n\r\n    // 当前激活的菜单\r\n    const activeMenu = computed(() => {\r\n      return route.path\r\n    })\r\n\r\n    // 将Bootstrap图标类名转换为Element Plus图标组件\r\n    const iconComponent = (icon) => {\r\n      // 如果是bootstrap图标类，转换为Element Plus图标\r\n      if (icon && icon.includes('bi-')) {\r\n        // 移除bi-前缀，转换为驼峰命名\r\n        const name = icon.replace('bi-', '')\r\n          .split('-')\r\n          .map((word, index) => \r\n            index === 0 ? word : word.charAt(0).toUpperCase() + word.slice(1)\r\n          )\r\n          .join(''