"category.icon\"></i>\r\n              <span>{{ category.name }}</span>\r\n              <span class=\"badge\">{{ category.count }}</span>\r\n            </div>\r\n          </div>\r\n        </el-card>\r\n      </div>\r\n\r\n      <!-- 中间物品列表 -->\r\n      <div class=\"col-md-4\">\r\n        <el-card class=\"supplies-list-card\">\r\n          <template #header>\r\n            <div class=\"card-header-flex\">\r\n              <span>{{ categories[selectedCategory].name }}</span>\r\n              <el-button type=\"primary\" size=\"small\" @click.stop>\r\n                <i class=\"bi bi-plus\"></i> 添加物品\r\n              </el-button>\r\n            </div>\r\n          </template>\r\n          <div class=\"supplies-list\">\r\n            <div \r\n              v-for=\"(supply, index) in supplies\" \r\n              :key=\"index\" \r\n              class=\"supply-item\" \r\n              :class=\"{'active': selectedSupply === index}\"\r\n              @click.stop=\"selectSupply(index)\"\r\n            >\r\n              <div class=\"supply-title\">\r\n                <span>{{ supply.name }}</span>\r\n                <span class=\"supply-status\" :class=\"'status-' + supply.status\">{{ getStatusText(supply.status) }}</span>\r\n              </div>\r\n              <div class=\"supply-info\">\r\n                <span>库存: {{ supply.stock }} {{ supply.unit }}</span>\r\n                <span>{{ supply.category }}</span>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </el-card>\r\n      </div>\r\n\r\n      <!-- 右侧物品详情 -->\r\n      <div class=\"col-md-6\">\r\n        <el-card v-if=\"selectedSupply !== null\">\r\n          <div class=\"detail-header\">\r\n            <div class=\"detail-image\">\r\n              <i class=\"bi bi-box-seam\" style=\"font-size: 48px; color: #6c757d;\"></i>\r\n            </div>\r\n            <div class=\"detail-info\">\r\n              <div class=\"detail-name\">{{ supplies[selectedSupply].name }}</div>\r\n              <div class=\"detail-category\">{{ supplies[selectedSupply].category }}</div>\r\n              <div class=\"detail-actions\">\r\n                <el-button type=\"primary\" size=\"small\">\r\n                  <i class=\"bi bi-cart-plus\"></i> 申请领用\r\n                </el-button>\r\n                <el-button size=\"small\">\r\n                  <i class=\"bi bi-pencil\"></i> 编辑信息\r\n                </el-button>\r\n                <el-button size=\"small\">\r\n                  <i class=\"bi bi-plus-circle\"></i> 更新库存\r\n                </el-button>\r\n              </div>\r\n            </div>\r\n          </div>\r\n\r\n          <el-tabs v-model=\"activeTab\">\r\n            <el-tab-pane label=\"基本信息\" name=\"info\">\r\n              <div class=\"detail-section\">\r\n                <div class=\"section-title\">库存信息</div>\r\n                <el-descriptions :column=\"2\" border>\r\n                  <el-descriptions-item label=\"当前库存\">{{ supplies[selectedSupply].stock }} {{ supplies[selectedSupply].unit }}</el-descriptions-item>\r\n                  <el-descriptions-item label=\"预警阈值\">{{ supplies[selectedSupply].threshold }} {{ supplies[selectedSupply].unit }}</el-descriptions-item>\r\n                  <el-descriptions-item label=\"状态\">\r\n                    <span class=\"supply-status\" :class=\"'status-' + supplies[selectedSupply].status\">{{ getStatusText(supplies[selectedSupply].status) }}</span>\r\n                  </el-descriptions-item>\r\n                  <el-descriptions-item label=\"供应商\">{{ supplies[selectedSupply].supplier }}</el-descriptions-item>\r\n                  <el-descriptions-item label=\"单价\">¥{{ supplies[selectedSupply].price }} / {{ supplies[selectedSupply].unit }}</el-descriptions-item>\r\n                  <el-descriptions-item label=\"规格\">{{ supplies[selectedSupply].specification }}</el-descriptions-item>\r\n                  <el-descriptions-item label=\"品牌\">{{ supplies[selectedSupply].brand }}</el-descriptions-item>\r\n                  <el-descriptions-item label=\"入库日期\">{{ supplies[selectedSupply].date }}</el-descriptions-item>\r\n                </el-descriptions>\r\n              </div>\r\n\r\n              <div class=\"detail-section\">\r\n                <div class=\"section-title\">物品描述</div>\r\n                <p>{{ supplies[selectedSupply].description || '暂无描述' }}</p>\r\n              </div>\r\n\r\n              <div class=\"detail-section\">\r\n                <div class=\"section-title\">使用记录</div>\r\n                <el-table :data=\"supplies[selectedSupply].records || []\" style=\"width: 100%\">\r\n                  <el-table-column prop=\"date\" label=\"日期\" width=\"180\"></el-table-column>\r\n                  <el-table-column prop=\"user\" label=\"领用人\"></el-table-column>\r\n                  <el-table-column prop=\"department\" label=\"部门\"></el-table-column>\r\n                  <el-table-column prop=\"quantity\" label=\"数量\"></el-table-column>\r\n                  <el-table-column prop=\"status\" label=\"状态\">\r\n                    <template #default=\"scope\">\r\n                      <el-tag \r\n                        :type=\"scope.row.status === '已审批' ? 'success' : scope.row.status === '待审批' ? 'warning' : 'info'\"\r\n                      >\r\n                        {{ scope.row.status }}\r\n                      </el-tag>\r\n                    </template>\r\n                  </el-table-column>\r\n                </el-table>\r\n              </div>\r\n            </el-tab-pane>\r\n            <el-tab-pane label=\"历史记录\" name=\"history\">\r\n              <div class=\"timeline-container\">\r\n                <el-timeline>\r\n                  <el-timeline-item\r\n                    v-for=\"(activity, index) in supplies[selectedSupply].activities || []\"\r\n                    :key=\"index\"\r\n                    :timestamp=\"activity.time\"\r\n                    :type=\"getTimelineType(activity.type)\"\r\n                  >\r\n                    {{ activity.content }}\r\n                  </el-timeline-item>\r\n                </el-timeline>\r\n              </div>\r\n            </el-tab-pane>\r\n          </el-tabs>\r\n        </el-card>\r\n        \r\n        <el-empty v-else description=\"请选择一个办公用品\"></el-empty>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'Supplies',\r\n  data() {\r\n    return {\r\n      searchQuery: '',\r\n      selectedCategory: 0,\r\n      selectedSupply: 0,\r\n      activeTab: 'info',\r\n      categories: [\r\n        { name: '全部分类', icon: 'bi bi-grid', count: 120 },\r\n        { name: '办公文具', icon: 'bi bi-pencil', count: 45 },\r\n        { name: '纸张纸板', icon: 'bi bi-file-earmark', count: 20 },\r\n        { name: '电脑配件', icon: 'bi bi-pc-display', count: 15 },\r\n        { name: '办公设备', icon: 'bi bi-printer', count: 12 },\r\n        { name: '清洁用品', icon: 'bi bi-droplet', count: 18 },\r\n        { name: '食品饮料', icon: 'bi bi-cup-hot', count: 10 }\r\n      ],\r\n      supplies: [\r\n        {\r\n          name: 'A4复印纸',\r\n          category: '纸张纸板',\r\n          stock: 48,\r\n          threshold: 20,\r\n          unit: '包',\r\n          status: 'available',\r\n          supplier: '得力文具有限公司',\r\n          price: 38.5,\r\n          specification: '210mm×297mm，80g/m²，500张/包',\r\n          brand: '得力(deli)',\r\n          date: '2023-05-01',\r\n          description: '优质A4复印纸，适用于复印机、打印机、传真机等办公设备，纸张洁白平整，不卡纸，打印效果清晰。',\r\n          records: [\r\n            { date: '2023-05-15', user: '张三', department: '市场部', quantity: '2包', status: '已审批' },\r\n            { date: '2023-05-10', user: '李四', department: '技术部', quantity: '5包', status: '已审批' },\r\n            { date: '2023-05-05', user: '王五', department: '行政部', quantity: '10包', status: '已审批' }\r\n          ],\r\n          activities: [\r\n            { time: '2023-05-15 10:30', type: 'out', content: '张三领用了2包A4复印纸' },\r\n            { time: '2023-05-10 14:20', type: 'out', content: '李四领用了5包A4复印纸' },\r\n            { time: '2023-05-05 09:15', type: 'out', content: '王五领用了10包A4复印纸' },\r\n            { time: '2023-05-01 11:00', type: 'in', content: '系统管理员入库了65包A4复印纸' }\r\n          ]\r\n        },\r\n        {\r\n          name: '签字笔',\r\n          category: '办公文具',\r\n          stock: 108,\r\n          threshold: 50,\r\n          unit: '支',\r\n          status: 'available',\r\n          supplier: '晨光文具股份有限公司',\r\n          price: 2.5,\r\n          specification: '0.5mm，黑色',\r\n          brand: '晨光(M&G)',\r\n          date: '2023-04-25',\r\n          description: '中性笔，书写流畅，不断墨，不漏墨，适合日常办公和学习使用。',\r\n          records: [\r\n            { date: '2023-05-12', user: '赵六', department: '人事部', quantity: '12支', status: '已审批' },\r\n            { date: '2023-05-08', user: '钱七', department: '财务部', quantity: '20支', status: '已审批' }\r\n          ],\r\n          activities: [\r\n            { time: '2023-05-12 15:45', type: 'out', content: '赵六领用了12支签字笔' },\r\n            { time: '2023-05-08 11:30', type: 'out', content: '钱七领用了20支签字笔' },\r\n            { time: '2023-04-25 14:00', type: 'in', content: '系统管理员入库了140支签字笔' }\r\n          ]\r\n        },\r\n        {\r\n          name: '鼠标垫',\r\n          category: '电脑配件',\r\n          stock: 15,\r\n          threshold: 10,\r\n          unit: '个',\r\n          status: 'low',\r\n          supplier: '罗技科技有限公司',\r\n          price: 18.8,\r\n          specification: '300mm×240mm×3mm',\r\n          brand: '罗技(Logitech)',\r\n          date: '2023-04-20',\r\n          description: '优质鼠标垫，表面平滑，底部防滑，边缘防磨损，提供舒适的鼠标使用体验。',\r\n          records: []\r\n        },\r\n        {\r\n          name: '订书机',\r\n          category: '办公文具',\r\n          stock: 5,\r\n          threshold: 5,\r\n          unit: '台',\r\n          status: 'low',\r\n          supplier: '得力文具有限公司',\r\n          price: 15.5,\r\n          specification: '12号，最大订书厚度20页',\r\n          brand: '得力(deli)',\r\n          date: '2023-04-15',\r\n          description: '金属机身，坚固耐用，操作简便，一次可装载1