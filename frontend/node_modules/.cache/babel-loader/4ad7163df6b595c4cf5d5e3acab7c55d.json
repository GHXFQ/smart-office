            <el-form-item label=\"合同名称\">\n                    <el-input v-model=\"contractDetails.title\"></el-input>\n                  </el-form-item>\n                  <el-form-item label=\"合同编号\">\n                    <el-input v-model=\"contractDetails.number\" disabled></el-input>\n                  </el-form-item>\n                  <el-form-item label=\"合同类型\">\n                    <el-select v-model=\"contractDetails.type\" style=\"width: 100%;\">\n                      <el-option label=\"采购合同\" value=\"采购合同\"></el-option>\n                      <el-option label=\"销售合同\" value=\"销售合同\"></el-option>\n                      <el-option label=\"服务合同\" value=\"服务合同\"></el-option>\n                      <el-option label=\"合作协议\" value=\"合作协议\"></el-option>\n                      <el-option label=\"租赁合同\" value=\"租赁合同\"></el-option>\n                      <el-option label=\"其他\" value=\"其他\"></el-option>\n                    </el-select>\n                  </el-form-item>\n                  <el-form-item label=\"合同金额\">\n                    <el-input v-model=\"contractDetails.amount\" prefix=\"¥\"></el-input>\n                  </el-form-item>\n                </el-form>\n              </div>\n              <div class=\"col-md-6\">\n                <el-form label-position=\"top\">\n                  <el-form-item label=\"签约对方\">\n                    <el-input v-model=\"contractDetails.company\"></el-input>\n                  </el-form-item>\n                  <el-form-item label=\"签约日期\">\n                    <el-date-picker \n                      v-model=\"contractDetails.signDate\" \n                      type=\"date\" \n                      value-format=\"YYYY-MM-DD\"\n                      style=\"width: 100%;\">\n                    </el-date-picker>\n                  </el-form-item>\n                  <el-form-item label=\"生效日期\">\n                    <el-date-picker \n                      v-model=\"contractDetails.startDate\" \n                      type=\"date\" \n                      value-format=\"YYYY-MM-DD\"\n                      style=\"width: 100%;\">\n                    </el-date-picker>\n                  </el-form-item>\n                  <el-form-item label=\"到期日期\">\n                    <el-date-picker \n                      v-model=\"contractDetails.expireDate\" \n                      type=\"date\" \n                      value-format=\"YYYY-MM-DD\"\n                      style=\"width: 100%;\">\n                    </el-date-picker>\n                  </el-form-item>\n                </el-form>\n              </div>\n            </div>\n          </el-tab-pane>\n\n          <el-tab-pane label=\"合同文本\" name=\"contractText\">\n             <QuillEditor\n                v-model:content=\"contractDetails.content\"\n                ref=\"quillEditor\"\n                :toolbar=\"editorOption.modules.toolbar\"\n                contentType=\"html\"\n                class=\"contract-text-editor\"\n                theme=\"snow\"\n                @textChange=\"onContractContentChange\"\n              />\n          </el-tab-pane>\n\n        </el-tabs>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script setup>\nimport { ref, reactive, onMounted, watch, computed } from 'vue';\nimport { useRoute, useRouter } from 'vue-router';\nimport { getContractDetail, updateContract } from '@/api/contract';\nimport { ElMessage, ElLoading } from 'element-plus';\nimport { QuillEditor } from '@vueup/vue-quill';\nimport '@vueup/vue-quill/dist/vue-quill.snow.css';\nimport { \n  Download, DocumentAdd, MagicStick, Timer, Position, Setting, \n  DocumentChecked, Notebook, Plus, Check, WarningFilled, Document, \n  Warning, InfoFilled, Loading \n} from '@element-plus/icons-vue';\n\nconst route = useRoute();\nconst router = useRouter();\n\nconst isLoading = ref(true);\nconst isSaving = ref(false);\nconst activeTab = ref('basicInfo');\nconst contractId = ref(route.params.id);\n\nconst contractDetails = reactive({\n  title: '',\n  number: '',\n  type: '',\n  amount: '',\n  company: '',\n  signDate: '',\n  startDate: '',\n  expireDate: '',\n  content: '',\n});\n\nconst contractTitle = computed(() => contractDetails.title || '合同详情');\n\n// Dummy data and stubs for functions used in the template\nconst quillEditor = ref(null);\nconst editorOption = reactive({\n    modules: {\n        toolbar: [\n            ['bold', 'italic', 'underline', 'strike'],\n            ['blockquote', 'code-block'],\n            [{ 'header': 1 }, { 'header': 2 }],\n            [{ 'list': 'ordered'}, { 'list': 'bullet' }],\n            [{ 'script': 'sub'}, { 'script': 'super' }],\n            [{ 'indent': '-1'}, { 'indent': '+1' }],\n            [{ 'direction': 'rtl' }],\n            [{ 'size': ['small', false, 'large', 'huge'] }],\n            [{ 'header': [1, 2, 3, 4, 5, 6, false] }],\n            [{ 'color': [] }, { 'background': [] }],\n            [{ 'font': [] }],\n            [{ 'align': [] }],\n            ['clean'],\n            ['link', 'image', 'video']\n        ]\n    }\n});\nconst onContractContentChange = () => {};\nconst downloadContract = () => { console.log('Download triggered'); };\n\nconst fetchContractDetail = async (id) => {\n  if (!id) return;\n  isLoading.value = true;\n  try {\n    const response = await getContractDetail(id);\n    const data = response.data;\n    contractDetails.title = data.title;\n    contractDetails.number = data.number;\n    contractDetails.type = data.type;\n    contractDetails.amount = data.amount;\n    contractDetails.company = data.company;\n    contractDetails.signDate = data.sign_date;\n    contractDetails.startDate = data.start_date;\n    contractDetails.expireDate = data.expire_date;\n    contractDetails.content = data.content;\n  } catch (error) {\n    ElMessage.error('加载合同详情失败');\n    console.error(error);\n  } finally {\n    isLoading.value = false;\n  }\n};\n\nconst saveChanges = async () => {\n  isSaving.value = true;\n  const loading = ElLoading.service({ lock: true, text: '正在保存...' });\n  try {\n    const payload = {\n      title: contractDetails.title,\n      type: contractDetails.type,\n      amount: contractDetails.amount,\n      company: contractDetails.company,\n      sign_date: contractDetails.signDate,\n      start_date: contractDetails.startDate,\n      expire_date: contractDetails.expireDate,\n      content: contractDetails.content,\n    };\n    await updateContract(contractId.value, payload);\n    ElMessage.success('合同保存成功');\n  } catch (error) {\n    ElMessage.error('保存失败，请重试');\n    console.error(error);\n  } finally {\n    isSaving.value = false;\n    loading.close();\n  }\n};\n\n// Watch for route changes to refetch data\nwatch(\n  () => [route.params.id, route.query._refresh],\n  ([newId, refresh]) => {\n    if (newId) {\n      contractId.value = newId;\n      fetchContractDetail(newId);\n    }\n  },\n  { immediate: true }\n);\n\nonMounted(() => {\n  // The watcher with immediate: true handles the initial fetch\n});\n\n// Dummy stubs for other template features\nconst realTimePolishLoading = ref(false);\nconst enableInlineSuggestions = ref(false);\nconst onInlineSuggestionsToggle = () => {};\nconst manualAnalyzeText = () => {};\nconst estimatedTime = ref('1-2分钟');\nconst polishPreferences = reactive({ style: 'neutral', contractType: 'general', focus: ['grammar'] });\nconst newTerminology = reactive({ term: '' });\nconst customTerminologies = ref([]);\nconst addCustomTerminology = () => {};\nconst removeCustomTerminology = () => {};\nconst isChecking = ref(false);\nconst startContractCheck = () => {};\nconst checkPreferences = reactive({ legalCompliance: true, completeness: true, riskAlert: true, contractType: 'general', checkDepth: 'standard' });\nconst aiCheckFinished = ref(false);\nconst checkResult = reactive({\n  totalIssues: 0,\n  criticalIssues: 0,\n  warningIssues: 0,\n  suggestions: 0,\n  legalComplianceIssues: [],\n  completenessIssues: [],\n  riskAlerts: [],\n});\nconst activeCollapse = ref(['legalCompliance']);\nconst viewRegulation = () => {};\n\n</script>\n\n<style scoped>\n/* Basic styling to make the component look reasonable */\n.contract-detail-container {\n  padding: 20px;\n}\n.card-header {\n  font-size: 1.25rem;\n  font-weight: 500;\n}\n.contract-text-editor {\n  height: 500px;\n}\n</style>\n"],"mappings":"AAoGA,SAASA,GAAG,EAAEC,QAAQ,EAAEC,SAAS,EAAEC,KAAK,EAAEC,QAAQ,QAAQ,KAAK;AAC/D,SAASC,QAAQ,EAAEC,SAAS,QAAQ,YAAY;AAChD,SAASC,iBAAiB,EAAEC,cAAc,QAAQ,gBAAgB;AAClE,SAASC,SAAS,EAAEC,SAAS,QAAQ,cAAc;AACnD,SAASC,WAAW,QAAQ,kBAAkB;AAC9C,OAAO,0CAA0C;AACjD,SACEC,QAAQ,EAAEC,WAAW,EAAEC,UAAU,EAAEC,KAAK,EAAEC,QAAQ,EAAEC,OAAO,EAC3DC,eAAe,EAAEC,QAAQ,EAAEC,IAAI,EAAEC,KAAK,EAAEC,aAAa,EAAEC,QAAQ,EAC/DC,OAAO,EAAEC,UAAU,EAAEC,OAAO,QACvB,yBAAyB;;;;;;;IAEhC,MAAMC,KAAK,GAAGtB,QAAQ,CAAC,CAAC;IACxB,MAAMuB,MAAM,GAAGtB,SAAS,CAAC,CAAC;IAE1B,MAAMuB,SAAS,GAAG7B,GAAG,CAAC,IAAI,CAAC;IAC3B,MAAM8B,QAAQ,GAAG9B,GAAG,CAAC,KAAK,CAAC;IAC3B,MAAM+B,SAAS,GAAG/B,GAAG,CAAC,WAAW,CAAC;IAClC,MAAMgC,UAAU,GAAGhC,GAAG,CAAC2B,KAAK,CAACM,MAAM,CAACC,EAAE,CAAC;IAEvC,MAAMC,eAAe,GAAGlC,QAAQ,CAAC;MAC/BmC,KAAK,EAAE,EAAE;MACTC,MAAM,EAAE,EAAE;MACVC,IAAI,EAAE,EAAE;MACRC,MAAM,EAAE,EAAE;MACVC,OAAO,EAAE,EAAE;MACXC,QAAQ,EAAE,EAAE;MACZC,SAAS,EAAE,EAAE;MACbC,UAAU,EAAE,EAAE;MACdC,OAAO,EAAE;IACX,CAAC,CAAC;IAEF,MAAMC,aAAa,GAAGzC,QAAQ,CAAC,MAAM+B,eAAe,CAACC,KAAK,IAAI,MAAM,CAAC;;IAErE;IACA,MAAMU,WAAW,GAAG9C,GAAG,CAAC,IAAI,CAAC;IAC7B,MAAM+C,YAAY,GAAG9C,QAAQ,CAAC;MAC1B+C,OAAO,EAAE;QACLC,OAAO,EAAE,CACL,CAAC,MAAM,EAAE,QAAQ,EAAE,WAAW,EAAE,QAAQ,CAAC,EACzC,CAAC,YAAY,EAAE,YAAY,CAAC,EAC5B,CAAC;UAAE,QAAQ,EAAE;QAAE,CAAC,EAAE;UAAE,QAAQ,EAAE;QAAE,CAAC,CAAC,EAClC,CAAC;UAAE,MAAM,EAAE;QAAS,CAAC,EAAE;UAAE,MAAM,EAAE;QAAS,CAAC,CAAC,EAC5C,CAAC;UAAE,QAAQ,EAAE;QAAK,CAAC,EAAE;UAAE,QAAQ,EAAE;QAAQ,CAAC,CAAC,EAC3C,CAAC;UAAE,QAAQ,EAAE;QAAI,CAAC,EAAE;UAAE,QAAQ,EAAE;QAAK,CAAC,CAAC,EACvC,CAAC;UAAE,WAAW,EAAE;QAAM,CAAC,CAAC,EACxB,CAAC;UAAE,MAAM,EAAE,CAAC,OAAO,EAAE,KAAK,EAAE,OAAO,EAAE,MAAM;QAAE,CAAC,CAAC,EAC/C,CAAC;UAAE,QAAQ,EAAE,CAAC,CAAC,EAAE,CAAC,