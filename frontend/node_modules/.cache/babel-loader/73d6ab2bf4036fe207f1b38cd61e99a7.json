\n          ElMessage.error(error.value);\n        } else if (err.response && err.response.data && err.response.data.message) {\n          ElMessage.error(err.response.data.message);\n        } else {\n          ElMessage.error(err.message || '登录失败，请稍后重试');\n        }\n      }\n    }\n    \n    return {\n      loginFormRef,\n      loginForm,\n      loginRules,\n      loading,\n      handleLogin,\n      User,\n      Lock\n    }\n  }\n}\n</script>\n\n<style scoped>\n.login-container {\n  height: 100vh;\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  background: linear-gradient(135deg, #667eea 0%, #764ba2 100%);\n}\n\n.login-box {\n  width: 400px;\n  background-color: #ffffff;\n  border-radius: 10px;\n  box-shadow: 0 0 20px rgba(0, 0, 0, 0.1);\n  padding: 40px;\n}\n\n.login-header {\n  text-align: center;\n  margin-bottom: 30px;\n}\n\n.login-header img {\n  width: 60px;\n  margin-bottom: 15px;\n}\n\n.login-header h4 {\n  font-weight: 600;\n  color: #333;\n  margin-bottom: 8px;\n  font-size: 20px;\n}\n\n.login-header p {\n  color: #6c757d;\n  font-size: 14px;\n}\n\n.login-form {\n  margin-bottom: 20px;\n}\n\n.login-options {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  margin-bottom: 20px;\n}\n\n.forgot-link {\n  color: #1890ff;\n  text-decoration: none;\n  font-size: 14px;\n}\n\n.forgot-link:hover {\n  text-decoration: underline;\n}\n\n.login-button {\n  width: 100%;\n  margin-bottom: 15px;\n  height: 40px;\n  font-size: 16px;\n}\n\n.login-footer {\n  text-align: center;\n  margin-bottom: 20px;\n  font-size: 14px;\n  color: #6c757d;\n}\n\n.login-footer a {\n  color: #1890ff;\n  text-decoration: none;\n}\n\n.login-footer a:hover {\n  text-decoration: underline;\n}\n</style> "],"mappings":";;EACOA,KAAK,EAAC;AAAiB;;EACrBA,KAAK,EAAC;AAAW;;EAmCbA,KAAK,EAAC;AAAe;;EAUvBA,KAAK,EAAC;AAAc;;;;;;;;;;;uBA9C7BC,mBAAA,CAkDM,OAlDNC,UAkDM,GAjDJC,mBAAA,CAgDM,OAhDNC,UAgDM,G,0BA/CJD,mBAAA,CAIM;IAJDH,KAAK,EAAC;EAAc,IACvBG,mBAAA,CA