ebHistory } from 'vue-router'\n\n// 路由懒加载处理，添加错误处理\nconst lazyLoad = (componentPath) => {\n  return () => {\n    return new Promise((resolve) => {\n      try {\n        const component = import(`../views/${componentPath}.vue`);\n        resolve(component);\n      } catch (error) {\n        // 避免使用console.error，改为兼容ESLint的写法\n        // eslint-disable-next-line no-console\n        console.error(`路由加载失败: ${componentPath}`, error);\n        // 返回一个错误页面组件\n        import('../views/error/NotFound.vue').then(resolve).catch(() => {\n          // 如果错误页面也加载失败，返回一个基本的组件\n          resolve({\n            template: `<div class=\"error-page\">加载失败: ${componentPath}</div>`\n          });\n        });\n      }\n    });\n  };\n};\n\n// 路由懒加载 - 只保留已实现的组件\nconst Login = lazyLoad('auth/Login')\nconst Register = lazyLoad('auth/Register')\nconst ForgotPassword = lazyLoad('auth/ForgotPassword')\nconst Layout = lazyLoad('layout/Index')\nconst Dashboard = lazyLoad('dashboard/Index')\nconst Calendar = lazyLoad('calendar/Index')\nconst Knowledge = lazyLoad('knowledge/Index')\nconst Chat = lazyLoad('chat/Index')\nconst Contract = lazyLoad('contract/Index')\nconst ContractDetail = lazyLoad('contract/Detail')\nconst Contact = lazyLoad('contact/Index')\nconst Project = lazyLoad('project/Index')\nconst Company = lazyLoad('company/Index')\nconst Profile = lazyLoad('profile/Index')\nconst SmartDoc = lazyLoad('smartdoc/Index')\nconst SmartDocDetail = lazyLoad('smartdoc/Detail')\n\nconst routes = [\n  {\n    path: '/login',\n    name: 'Login',\n    component: Login,\n    meta: { title: '登录', requiresAuth: false }\n  },\n  {\n    path: '/register',\n    name: 'Register',\n    component: Register,\n    meta: { title: '注册', requiresAuth: false }\n  },\n  {\n    path: '/forgot-password',\n    name: 'ForgotPassword',\n    component: ForgotPassword,\n    meta: { title: '找回密码', requiresAuth: false }\n  },\n  {\n    path: '/',\n    component: Layout,\n    redirect: '/dashboard',\n    meta: { requiresAuth: true },\n    children: [\n      {\n        path: 'dashboard',\n        name: 'Dashboard',\n        component: Dashboard,\n        meta: { title: '工作台', icon: 'bi-grid-1x2' }\n      },\n      {\n        path: 'calendar',\n        name: 'Calendar',\n        component: Calendar,\n        meta: { title: '日程管理', icon: 'bi-calendar-week' }\n      },\n      {\n        path: 'knowledge',\n        name: 'Knowledge',\n        component: Knowledge,\n        meta: { title: '知识库', icon: 'bi-book' }\n      },\n      {\n        path: 'chat',\n        name: 'Chat',\n        component: Chat,\n        meta: { title: '即时通讯', icon: 'bi-chat-dots' }\n      },\n      {\n        path: 'smartdoc',\n        name: 'SmartDoc',\n        component: SmartDoc,\n        meta: { title: '智能文档', icon: 'bi-file-earmark-text' },\n      },\n      {\n        path: 'smartdoc/:id',\n        name: 'SmartDocDetail',\n        component: SmartDocDetail,\n        meta: { title: '文档详情', icon: 'bi-file-earmark-text' },\n        hidden: true,\n        props: true,\n        beforeEnter: (to, from, next) => {\n          // 验证ID是否有效\n          const id = to.params.id\n          if (!id || isNaN(id)) {\n            next({ name: 'SmartDoc' })\n          } else {\n            next()\n          }\n        }\n      },\n      {\n        path: 'contract',\n        name: 'Contract',\n        component: Contract,\n        meta: { title: '合同管理', icon: 'bi-file-earmark-ruled' }\n      },\n      {\n        path: 'contract/:id',\n        name: 'ContractDetail',\n        component: ContractDetail,\n        meta: { title: '合同详情', icon: 'bi-file-earmark-ruled' },\n        hidden: true,\n        beforeEnter: (to, from, next) => {\n          // 验证ID是否为有效数字\n          const id = to.params.id\n          if (!id || id === 'contract' || isNaN(id)) {\n            next({ name: 'Contract' })\n          } else {\n            next()\n          }\n        }\n      },\n      {\n        path: 'contact',\n        name: 'Contact',\n        component: Contact,\n        meta: { title: '企业通讯录', icon: 'bi-person-lines-fill' }\n      },\n      {\n        path: 'project',\n        name: 'Project',\n        component: Project,\n        meta: { title: '项目管理', icon: 'bi-kanban' }\n      },\n      {\n        path: 'company',\n        name: 'Company',\n        component: Company,\n        meta: { title: '企业公司', icon: 'bi-building' }\n      },\n      {\n        path: 'profile',\n        name: 'Profile',\n        component: Profile,\n        meta: { title: '个人中心', icon: 'bi-person' }\n      }\n    ]\n  },\n  {\n    path: '/:pathMatch(.*)*',\n    redirect: '/login'\n  }\n]\n\nconst router = createRouter({\n  history: createWebHistory(),\n  routes\n})\n\n// 全局前置守卫，处理页面标题和身份验证\nrouter.beforeEach((to, from, next) => {\n  // 设置标题\n  document.title = to.meta.title ? `${to.meta.title} - 智能办公系统` : '智能办公系统'\n  \n  console.log('路由导航:', from.path, '=>', to.path);\n  \n  // 检查用户是否已登录\n  const isAuthenticated = localStorage.getItem('token') !== null\n  \n  // 如果从智能文档详情页进行导航，但导航目标不是智能文档相关页面\n  // 且导航路径看起来像是相对路径（如/smartdoc/contact）\n  if (from.path.startsWith('/smartdoc/')) {\n    // 检查要导航到的路径是否形如 /smartdoc/xxx，但xxx不是数字（非有效的文档ID）\n    const pathSegments = to.path.split('/');\n    if (pathSegments.length > 2 && pathSegments[1] === 'smartdoc' && isNaN(pathSegments[2])) {\n      // 这是一个可能的错误路径，重新导航到正确路径\n      const correctPath = '/' + pathSegments.slice(2).join('/');\n      console.log('检测到错误路径，重定向至:', correctPath);\n      // 使用路由导航而不是硬刷新，以保持平滑过渡\n      next({ path: correctPath, replace: true });\n      return;\n    }\n    \n    // 不是文档详情或首页，且路径没有错误，则正常清理DOM元素\n    if (!(to.path === '/smartdoc' || to.path.startsWith('/smartdoc/'))) {\n      // 异步清理DOM元素，避免干扰动画过渡\n      setTimeout(() => {\n        // 清理可能存在的遗留DOM元素\n        try {\n          // 清理QuillEditor相关DOM元素和事件监听器\n          const elements = [\n            '.ql-container', \n            '.ql-toolbar', \n            '.ql-editor',\n            '.ql-tooltip',\n            '.quill-editor'\n          ];\n          \n          elements.forEach(selector => {\n            document.querySelectorAll(selector).forEach(el => {\n              if (el && el.parentNode) {\n                try {\n                  el.parentNode.removeChild(el);\n                } catch (e) {\n                  console.error('移除元素失败:', selector, e);\n                }\n              }\n            });\n          });\n          \n          // 强制移除可能存在的遮罩层\n          const overlays = document.querySelectorAll('.el-overlay, .el-message, .el-message-box');\n          overlays.forEach(overlay => {\n            if (overlay && overlay.parentNode) {\n              overlay.parentNode.removeChild(overlay);\n            }\n          });\n        } catch (error) {\n          console.error('清理DOM元素失败:', error);\n        }\n      }, 300); // 延迟执行，等待过渡动画完成\n    }\n  }\n  \n  // 修复：检查是否是从其他页面直接跳转到智能文档详情页\n  if (to.name === 'SmartDocDetail' && !from.path.startsWith('/smartdoc')) {\n    // 验证ID是否有效\n    const id = to.params.id;\n    if (!id || isNaN(id)) {\n      next({ name: 'SmartDoc' });\n      return;\n    }\n    \n    // 如果ID有效，允许直接访问详情页\n    next();\n    return;\n  }\n  \n  // 如果路由需要认证且用户未登录，则重定向到登录页\n  if (to.meta.requiresAuth && !isAuthenticated && to.path !== '/login') {\n    next({ path: '/login' })\n  } else {\n    next()\n  }\n})\n\nexport default router "],"mappings":";;AAAA,SAASA,YAAY,EAAEC,gBAAgB,QAAQ,YAAY;;AAE3D;AACA,MAAMC,QAAQ,GAAIC,aAAa,IAAK;EAClC,OAAO,MAAM;IACX,OAAO,IAAIC,OAAO,CAAEC,OAAO,IAAK;MAC9B,IAAI;QACF,MAAMC,SAAS,GAAG,MAAM,CAAC,YAAYH,aAAa,MAAM,CAAC;QACzDE,OAAO,CAACC,SAAS,CAAC;MACpB,CAAC,CAAC,OAAOC,KAAK,EAAE;QACd;QACA;QACAC,OAAO,CAACD,KAAK,CAAC,WAAWJ,aAAa,EAAE,EAAEI,KAAK,CAAC;QAChD;QACA,MAAM,CAAC,6BAA6B,CAAC,CAACE,IAAI,CAACJ,OAAO,CAAC,CAACK,KAAK,CAAC,MAAM;UAC9D;UACAL,OAAO,CAAC;YACNM,QAAQ,EAAE,iCAAiCR,aAAa;UAC1D,CAAC,CAAC;QACJ,CAAC,CAAC;MACJ;IACF,CAAC,CAAC;EACJ,CAAC;AACH,CAAC;;AAED;AACA,MAAMS,KAAK,GAAGV,QAAQ,CAAC,YAAY,CAAC;AACpC,MAAMW,QAAQ,GAAGX,QAAQ,CAAC,eAAe,CAAC;AAC1C,MAAM