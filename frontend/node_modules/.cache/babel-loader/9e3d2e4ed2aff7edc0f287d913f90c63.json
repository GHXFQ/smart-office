ull,\r\n    loading: false,\r\n    error: null\r\n  }),\r\n  \r\n  getters: {\r\n    isAuthenticated: (state) => !!state.token,\r\n    userProfile: (state) => state.user,\r\n    userError: (state) => state.error\r\n  },\r\n  \r\n  actions: {\r\n    async login(credentials) {\r\n      this.loading = true\r\n      this.error = null\r\n      \r\n      try {\r\n        const response = await authApi.login(credentials)\r\n        \r\n        if (response.success) {\r\n          const { token, userId, username } = response.data\r\n          \r\n          // 保存认证信息\r\n          localStorage.setItem('token', token)\r\n          \r\n          // 创建简单用户对象\r\n          const user = {\r\n            id: userId,\r\n            username: username\r\n          }\r\n          \r\n          // 保存用户信息\r\n          localStorage.setItem('user', JSON.stringify(user))\r\n          \r\n          this.token = token\r\n          this.user = user\r\n          this.loading = false\r\n          \r\n          // 登录成功后获取完整用户信息\r\n          this.getUserProfile()\r\n          \r\n          return Promise.resolve(response)\r\n        } else {\r\n          this.error = response.message || '登录失败'\r\n          this.loading = false\r\n          return Promise.reject(response)\r\n        }\r\n      } catch (error) {\r\n        let errorMsg = '登录失败，请稍后重试'\r\n        \r\n        // 尝试从不同位置获取错误信息\r\n        if (error.response) {\r\n          if (error.response.data) {\r\n            if (error.response.data.message) {\r\n              errorMsg = error.response.data.message\r\n            } else if (error.response.data.detail) {\r\n              errorMsg = error.response.data.detail\r\n            } else if (typeof error.response.data === 'string') {\r\n              errorMsg = error.response.data\r\n            }\r\n          } else if (error.response.statusText) {\r\n            errorMsg = `服务器错误 (${error.response.status}): ${error.response.statusText}`\r\n          }\r\n        } else if (error.message) {\r\n          errorMsg = error.message\r\n        }\r\n        \r\n        this.error = errorMsg\r\n        this.loading = false\r\n        return Promise.reject({ ...error, message: errorMsg })\r\n      }\r\n    },\r\n    \r\n    async register(userData) {\r\n      this.loading = true\r\n      this.error = null\r\n      \r\n      try {\r\n        const response = await authApi.register(userData)\r\n        \r\n        if (response.success) {\r\n          this.loading = false\r\n          return Promise.resolve(response)\r\n        } else {\r\n          this.error = response.message\r\n          this.loading = false\r\n          return Promise.reject(response)\r\n        }\r\n      } catch (error) {\r\n        const errorMsg = error.response?.data?.message || '注册失败，请稍后重试'\r\n        this.error = errorMsg\r\n        this.loading = false\r\n        return Promise.reject(error)\r\n      }\r\n    },\r\n    \r\n    async logout() {\r\n      try {\r\n        await authApi.logout()\r\n      } catch (error) {\r\n        console.error('登出请求失败，但会继续清除本地会话', error)\r\n      }\r\n      \r\n      // 清除本地存储\r\n      localStorage.removeItem('token')\r\n      localStorage.removeItem('user')\r\n      \r\n      // 更新状态\r\n      this.token = ''\r\n      this.user = null\r\n      \r\n      return Promise.resolve()\r\n    },\r\n    \r\n    async getUserProfile() {\r\n      this.loading = true\r\n      \r\n      try {\r\n        const response = await userApi.getUserProfile()\r\n        \r\n        // 检查不同响应格式\r\n        let userData = null\r\n        \r\n        if (response && response.success && response.data) {\r\n          // 标准格式 {success: true, data: {...}}\r\n          userData = response.data\r\n        } else if (response && response.code === 200 && response.data) {\r\n          // 格式 {code: 200, data: {...}}\r\n          userData = response.data\r\n        } else if (response && typeof response === 'object' && response.id) {\r\n          // 直接返回数据对象 {id: ..., username: ...}\r\n          userData = response\r\n        }\r\n        \r\n        if (userData) {\r\n          this.user = { ...this.user, ...userData }\r\n          localStorage.setItem('user', JSON.stringify(this.user))\r\n          this.loading = false\r\n          return Promise.resolve(userData)\r\n        } else {\r\n          console.error('无法解析用户数据', response)\r\n          this.error = '获取个人信息失败，无效的数据格式'\r\n          this.loading = false\r\n          return Promise.reject(new Error('无效的数据格式'))\r\n        }\r\n      } catch (error) {\r\n        console.error('获取用户信息失败', error)\r\n        this.error = '获取个人信息失败'\r\n        this.loading = false\r\n        return Promise.reject(error)\r\n      }\r\n    },\r\n    \r\n    clearError() {\r\n      this.error = null\r\n    }\r\n  }\r\n}) "],"mappings":";AAAA,SAASA,WAAW,QAAQ,OAAO;AACnC,OAAOC,OAAO,MAAM,YAAY;AAChC,OAAOC,OAAO,MAAM,YAAY;AAEhC,OAAO,MAAMC,YAAY,GAAGH,WAAW,CAAC,MAAM,EAAE;EAC9CI,KAAK,EAAEA,CAAA,MAAO;IACZC,KAAK,EAAEC,YAAY,CAACC,OAAO,CAAC,OAAO,CAAC,IAAI,EAAE;IAC1CC,IAAI,EAAEC,IAAI,CAACC,KAAK,CAACJ,YAAY,CAACC,OAAO,CAAC,MAAM,CAAC,CAAC,IAAI,IAAI;IACtDI,OAAO,EAAE,KAAK;IACdC,KAAK,EAAE;EACT,CAAC,CAAC;EAEFC,OAAO,EAAE;IACPC,eAAe,EAAGV,KAAK,IAAK,CAAC,CAACA,KAAK,CAACC,KAAK;IACzCU,WAAW,EAAGX,KAAK,IAAKA,KAAK,CAACI,IAAI;IAClCQ,SAAS,EAAGZ,KAAK,IAAKA,KAAK,CAACQ;EAC9B,CAAC;EAEDK,OAAO,EAAE;IACP,MAAMC,KAAKA,CAACC,WAAW,EAAE;MACvB,IAAI,CAACR,OAAO,GAAG,IAAI;MACnB,IAAI,CAACC,KAAK,GAAG,IAAI;MAEjB,IAAI;QACF,MAAMQ,QAAQ,GAAG,MAAMnB,OAAO,CAACiB,KAAK,CAACC,WAAW,CAAC;QAEjD,IAAIC,QAAQ,CAACC,OAAO,EAAE;UACpB,MAAM;YAAEhB,KAAK;YAAEiB,MAAM;YAAEC;UAAS,CAAC,GAAGH,QAAQ,CAACI,IAAI;;UAEjD;UACAlB,YAAY,CAACmB,OAAO,CAAC,OAAO,EAAEpB,KAAK,CAAC;;UAEpC;UACA,MAAMG,IAAI,GAAG;YACXkB,EAAE,EAAEJ,MAAM;YACVC,QAAQ,EAAEA;UACZ,CAAC;;UAED;UACAjB,YAAY,CAACmB,OAAO,CAAC,MAAM,EAAEhB,IAAI,CAACkB,SAAS,CAACnB,IAAI,CAAC,CAAC;UAElD,IAAI,CAACH,KAAK,GAAGA,KAAK;UAClB,IAAI,CAACG,IAAI,GAAGA,IAAI;UAChB,IAAI,CAACG,OAAO,GAAG,KAAK;;UAEpB;UACA,IAAI,CAACiB,cAAc,CAAC,CAAC;UAErB,OAAOC,OAAO,CAACC,OAAO,CAACV