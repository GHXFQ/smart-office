=\"admin-login-container\">\r\n    <div class=\"main-container\">\r\n      <!-- 左侧区域 -->\r\n      <div class=\"left-section\">\r\n        <div class=\"animation-container\" id=\"lottieAnimation\"></div>\r\n        <div class=\"left-content\">\r\n          <h2>后台管理系统</h2>\r\n          <p>高效、智能、便捷</p>\r\n        </div>\r\n      </div>\r\n      \r\n      <!-- 右侧登录表单 -->\r\n      <div class=\"login-box\">\r\n        <div class=\"login-header\">\r\n          <i class=\"bi bi-shield-lock\"></i>\r\n          <h4>管理员登录</h4>\r\n        </div>\r\n\r\n        <el-form ref=\"loginFormRef\" :model=\"loginForm\" :rules=\"loginRules\" class=\"login-form\" @submit.prevent>\r\n          <el-form-item prop=\"username\">\r\n            <el-input \r\n              v-model=\"loginForm.username\" \r\n              placeholder=\"请输入管理员账号\" \r\n              autocomplete=\"off\"\r\n              size=\"large\"\r\n            >\r\n              <template #prefix>\r\n                <el-icon><User /></el-icon>\r\n              </template>\r\n            </el-input>\r\n          </el-form-item>\r\n          \r\n          <el-form-item prop=\"password\">\r\n            <el-input \r\n              v-model=\"loginForm.password\" \r\n              type=\"password\" \r\n              placeholder=\"请输入密码\" \r\n              show-password\r\n              autocomplete=\"off\"\r\n              size=\"large\"\r\n              @keyup.enter=\"handleLogin\"\r\n            >\r\n              <template #prefix>\r\n                <el-icon><Lock /></el-icon>\r\n              </template>\r\n            </el-input>\r\n          </el-form-item>\r\n          \r\n          <el-button \r\n            type=\"primary\" \r\n            class=\"login-button\" \r\n            :loading=\"loading\" \r\n            @click=\"handleLogin\"\r\n            size=\"large\"\r\n          >\r\n            登 录\r\n          </el-button>\r\n        </el-form>\r\n\r\n        <div class=\"login-footer\">\r\n          <router-link to=\"/login\" class=\"back-link\">\r\n            <el-icon><ArrowLeftBold /></el-icon> 返回用户登录\r\n          </router-link>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { ref, reactive, onMounted } from 'vue';\r\nimport { useRouter } from 'vue-router';\r\nimport { useStore } from 'vuex';\r\nimport { ElMessage } from 'element-plus';\r\nimport { User, Lock, ArrowLeftBold } from '@element-plus/icons-vue';\r\nimport lottie from 'lottie-web';\r\n\r\nexport default {\r\n  name: 'AdminLogin',\r\n  components: {\r\n    User,\r\n    Lock,\r\n    ArrowLeftBold\r\n  },\r\n  setup() {\r\n    const router = useRouter();\r\n    const store = useStore();\r\n    const loginFormRef = ref(null);\r\n    const loading = ref(false);\r\n\r\n    const loginForm = reactive({\r\n      username: '',\r\n      password: '',\r\n    });\r\n\r\n    const loginRules = {\r\n      username: [{ required: true, message: '请输入管理员账号', trigger: 'blur' }],\r\n      password: [{ required: true, message: '请输入密码', trigger: 'blur' }],\r\n    };\r\n\r\n    const handleLogin = async () => {\r\n      if (!loginFormRef.value) return;\r\n      await loginFormRef.value.validate(async (valid) => {\r\n        if (valid) {\r\n          loading.value = true;\r\n          try {\r\n            await store.dispatch('user/adminLogin', { \r\n              username: loginForm.username, \r\n              password: loginForm.password \r\n            });\r\n            router.push('/admin/dashboard');\r\n            ElMessage.success('登录成功，欢迎回来');\r\n          } catch (err) {\r\n            ElMessage.error(err.message || '登录失败，请检查您的凭据');\r\n          } finally {\r\n            loading.value = false;\r\n          }\r\n        } else {\r\n          ElMessage.warning('请输入完整的登录信息');\r\n          return false;\r\n        }\r\n      });\r\n    };\r\n    \r\n    onMounted(() => {\r\n      const animContainer = document.getElementById('lottieAnimation');\r\n      if (animContainer) {\r\n        lottie.loadAnimation({\r\n          container: animContainer,\r\n          renderer: 'svg',\r\n          loop: true,\r\n          autoplay: true,\r\n          path: 'https://assets3.lottiefiles.com/packages/lf20_p9b319na.json' // 一个科技感的动画\r\n        });\r\n      }\r\n    });\r\n\r\n    return {\r\n      loginFormRef,\r\n      loginForm,\r\n      loginRules,\r\n      loading,\r\n      handleLogin,\r\n    };\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.admin-login-container {\r\n  font-family: 'Microsoft YaHei', sans-serif;\r\n  height: 100vh;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  background-color: #f0f2f5;\r\n  margin: 0;\r\n  padding: 0;\r\n}\r\n\r\n.main-container {\r\n  display: flex;\r\n  width: 90%;\r\n  max-width: 1000px;\r\n  min-height: 550px;\r\n  background: #ffffff;\r\n  border-radius: 12px;\r\n  box-shadow: 0 10px 40px rgba(0, 0, 0, 0.1);\r\n  overflow: hidden;\r\n}\r\n\r\n.left-section {\r\n  flex: 1;\r\n  background: linear-gradient(135deg, #2c3e50 0%, #4ca1af 100%);\r\n  padding: 40px;\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: center;\r\n  align-items: center;\r\n  color: white;\r\n}\r\n\r\n.animation-container {\r\n  width: 100%;\r\n  max-width: 350px;\r\n  height: 280px;\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.left-content {\r\n  text-align: center;\r\n}\r\n\r\n.left-content h2 {\r\n  font-size: 2rem;\r\n  font-weight: 600;\r\n  margin-bottom: 10px;\r\n}\r\n\r\n.left-content p {\r\n  font-size: 1rem;\r\n  opacity: 0.9;\r\n}\r\n\r\n.login-box {\r\n  flex: 1;\r\n  padding: 50px;\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: center;\r\n}\r\n\r\n.login-header {\r\n  margin-bottom: 2.5rem;\r\n  text-align: center;\r\n}\r\n\r\n.login-header i {\r\n  font-size: 2.5rem;\r\n  background: linear-gradient(45deg, #2c3e50, #4ca1af);\r\n  -webkit-background-clip: text;\r\n  color: transparent;\r\n  margin-bottom: 15px;\r\n  display: inline-block;\r\n}\r\n\r\n.login-header h4 {\r\n  font-size: 1.5rem;\r\n  font-weight: 600;\r\n  margin: 0;\r\n  color: #333;\r\n}\r\n\r\n.login-form {\r\n  width: 100%;\r\n}\r\n\r\n.login-form .el-form-item {\r\n  margin-bottom: 25px;\r\n}\r\n\r\n.login-form :deep(.el-input__wrapper) {\r\n  border-radius: 6px;\r\n  padding: 5px 15px;\r\n}\r\n\r\n.login-button {\r\n  width: 100%;\r\n  border-radius: 6px;\r\n  font-weight: 500;\r\n  letter-spacing: 1px;\r\n  transition: all 0.3s ease;\r\n}\r\n\r\n.login-button:hover {\r\n  transform: translateY(-2px);\r\n  box-shadow: 0 4px 10px rgba(0, 0, 0, 0.2);\r\n}\r\n\r\n.login-footer {\r\n  text-align: center;\r\n  margin-top: 2rem;\r\n}\r\n\r\n.back-link {\r\n  color: #909399;\r\n  text-decoration: none;\r\n  font-size: 14px;\r\n  display: inline-flex;\r\n  align-items: center;\r\n  transition: color 0.3s ease;\r\n}\r\n\r\n.back-link:hover {\r\n  color: #409eff;\r\n}\r\n\r\n.back-link .el-icon {\r\n  margin-right: 5px;\r\n}\r\n\r\n@media (max-width: 992px) {\r\n  .main-container {\r\n    flex-direction: column;\r\n    width: 90%;\r\n    max-width: 500px;\r\n    height: auto;\r\n  }\r\n  .left-section {\r\n    display: none; /* 在小屏幕上隐藏左侧，突出表单 */\r\n  }\r\n  .login-box {\r\n    padding: 40px 30px;\r\n  }\r\n}\r\n</style> "],"mappings":";AAuEA,SAASA,GAAG,EAAEC,QAAQ,EAAEC,SAAQ,QAAS,KAAK;AAC9C,SAASC,SAAQ,QAAS,YAAY;AACtC,SAASC,QAAO,QAAS,MAAM;AAC/B,SAASC,SAAQ,QAAS,cAAc;AACxC,SAASC,IAAI,EAAEC,IAAI,EAAEC,aAAY,QAAS,yBAAyB;AACnE,OAAOC,MAAK,MAAO,YAAY;AAE/B,eAAe;EACbC,IAAI,EAAE,YAAY;EAClBC,UAAU,EAAE;IACVL,IAAI;IACJC,IAAI;IACJC;EACF,CAAC;EACDI,KAAKA,CAAA,EAAG;IACN,MAAMC,MAAK,GAAIV,SAAS,CAAC,CAAC;IAC1B,MAAMW,KAAI,GAAIV,QAAQ,CAAC,CAAC;IACxB,MAAMW,YAAW,GAAIf,GAAG,CAAC,IAAI,CAAC;IAC9B,MAAMgB,OAAM,GAAIhB,GAAG,CAAC,KAAK,CAAC;IAE1B,MAAMiB,SAAQ,GAAIhB,QAAQ,CAAC;MACzBiB,QAAQ,EAAE,EAAE;MACZC,QAAQ,EAAE;IACZ,CAAC,CAAC;IAEF,MAAMC,UAAS,GAAI;MACjBF,QAAQ,EAAE,CAAC;QAAEG,QAAQ,EAAE,IAAI;QAAEC,OAAO,EAAE,UAAU;QAAEC,OAAO,EAAE;MAAO,CAAC,CAAC;MACpEJ,QAAQ,EAAE,CAAC;QAAEE,QAAQ,EAAE,IA