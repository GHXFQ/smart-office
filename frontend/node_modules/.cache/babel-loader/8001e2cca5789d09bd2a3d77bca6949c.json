lex\">\r\n              <span>投票问卷列表</span>\r\n              <el-button type=\"primary\" size=\"small\">\r\n                <i class=\"bi bi-plus\"></i> 新建\r\n              </el-button>\r\n            </div>\r\n          </template>\r\n          <div class=\"vote-list\">\r\n            <div class=\"search-box mb-3\">\r\n              <el-input \r\n                v-model=\"searchQuery\" \r\n                placeholder=\"搜索投票问卷...\"\r\n                clearable\r\n                prefix-icon=\"el-icon-search\"\r\n              ></el-input>\r\n            </div>\r\n            <div \r\n              v-for=\"(vote, index) in votes\" \r\n              :key=\"index\" \r\n              class=\"vote-item\" \r\n              :class=\"{'active': selectedVote === index}\"\r\n              @click=\"selectedVote = index\"\r\n            >\r\n              <div class=\"vote-title\">{{ vote.title }}</div>\r\n              <div class=\"vote-info\">\r\n                <span>{{ vote.creator }} · {{ vote.date }}</span>\r\n                <span class=\"vote-status\" :class=\"'status-' + vote.status\">{{ getStatusText(vote.status) }}</span>\r\n              </div>\r\n              <div class=\"vote-progress mt-2\" v-if=\"vote.status === 'active'\">\r\n                <el-progress :percentage=\"vote.completion\" :stroke-width=\"8\"></el-progress>\r\n                <div class=\"vote-percent\">\r\n                  <span>参与人数: {{ vote.participants }}/{{ vote.total }}</span>\r\n                  <span>{{ vote.completion }}%</span>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </el-card>\r\n      </div>\r\n\r\n      <!-- 右侧投票详情 -->\r\n      <div class=\"col-md-8\">\r\n        <el-card v-if=\"selectedVote !== null\">\r\n          <template #header>\r\n            <div class=\"card-header-flex\">\r\n              <span>{{ votes[selectedVote].title }}</span>\r\n              <div>\r\n                <el-button type=\"primary\" size=\"small\" v-if=\"votes[selectedVote].status === 'active'\">\r\n                  提交投票\r\n                </el-button>\r\n                <el-button size=\"small\" v-if=\"votes[selectedVote].status === 'active'\">\r\n                  放弃投票\r\n                </el-button>\r\n                <el-button size=\"small\" v-if=\"votes[selectedVote].status === 'draft'\">\r\n                  <i class=\"bi bi-pencil\"></i> 编辑\r\n                </el-button>\r\n                <el-button size=\"small\" v-if=\"votes[selectedVote].status === 'draft'\">\r\n                  <i class=\"bi bi-play-fill\"></i> 发布\r\n                </el-button>\r\n              </div>\r\n            </div>\r\n          </template>\r\n          \r\n          <div class=\"vote-detail\">\r\n            <div class=\"vote-description mb-4\">\r\n              {{ votes[selectedVote].description }}\r\n            </div>\r\n            \r\n            <div class=\"vote-meta mb-4\">\r\n              <div><strong>投票类型:</strong> {{ votes[selectedVote].type }}</div>\r\n              <div><strong>截止日期:</strong> {{ votes[selectedVote].deadline }}</div>\r\n              <div><strong>发起人:</strong> {{ votes[selectedVote].creator }}</div>\r\n              <div v-if=\"votes[selectedVote].tags && votes[selectedVote].tags.length\">\r\n                <strong>标签:</strong> \r\n                <span \r\n                  v-for=\"(tag, index) in votes[selectedVote].tags\" \r\n                  :key=\"index\" \r\n                  class=\"tag\"\r\n                >\r\n                  {{ tag }}\r\n                </span>\r\n              </div>\r\n            </div>\r\n            \r\n            <el-tabs v-model=\"activeTab\">\r\n              <el-tab-pane label=\"投票内容\" name=\"content\">\r\n                <div class=\"vote-questions\">\r\n                  <div v-for=\"(question, qIndex) in votes[selectedVote].questions\" :key=\"qIndex\" class=\"vote-question mb-4\">\r\n                    <div class=\"question-title mb-2\">\r\n                      <strong>{{ qIndex + 1 }}. {{ question.title }}</strong>\r\n                      <span v-if=\"question.required\" class=\"text-danger\">*</span>\r\n                    </div>\r\n                    <div class=\"question-options\">\r\n                      <template v-if=\"question.type === 'single'\">\r\n                        <el-radio-group v-model=\"question.answer\">\r\n                          <div v-for=\"(option, oIndex) in question.options\" :key=\"oIndex\" class=\"mb-2\">\r\n                            <el-radio :value=\"oIndex\">{{ option }}</el-radio>\r\n                          </div>\r\n                        </el-radio-group>\r\n                      </template>\r\n                      <template v-else-if=\"question.type === 'multiple'\">\r\n                        <el-checkbox-group v-model=\"question.answer\">\r\n                          <div v-for=\"(option, oIndex) in question.options\" :key=\"oIndex\" class=\"mb-2\">\r\n                            <el-checkbox :value=\"oIndex\">{{ option }}</el-checkbox>\r\n                          </div>\r\n                        </el-checkbox-group>\r\n                      </template>\r\n                      <template v-else-if=\"question.type === 'text'\">\r\n                        <el-input \r\n                          v-model=\"question.answer\" \r\n                          type=\"textarea\" \r\n                          :rows=\"3\"\r\n                          placeholder=\"请输入您的回答\"\r\n                        ></el-input>\r\n                      </template>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n              </el-tab-pane>\r\n              <el-tab-pane label=\"投票统计\" name=\"stats\">\r\n                <div class=\"vote-stats\">\r\n                  <div class=\"vote-participants mb-4\">\r\n                    <h5>参与情况</h5>\r\n                    <el-progress :percentage=\"votes[selectedVote].completion\" :stroke-width=\"