opagation>\r\n    <div class=\"row\">\r\n      <!-- 左侧分类导航 -->\r\n      <div class=\"col-md-2\">\r\n        <el-card class=\"category-nav-card\">\r\n          <div class=\"search-box mb-3\">\r\n            <i class=\"bi bi-search search-icon\"></i>\r\n            <el-input \r\n              v-model=\"searchQuery\" \r\n              placeholder=\"搜索办公用品...\"\r\n              clearable\r\n              class=\"search-input\"\r\n            ></el-input>\r\n          </div>\r\n          <div class=\"category-nav\">\r\n            <div \r\n              v-for=\"(category, index) in categories\" \r\n              :key=\"index\" \r\n              class=\"category-item\" \r\n              :class=\"{'active': selectedCategory === index}\"\r\n              @click.stop=\"selectCategory(index)\"\r\n            >\r\n              <i :class=\"category.icon\"></i>\r\n              <span>{{ category.name }}</span>\r\n              <span class=\"badge\">{{ category.count }}</span>\r\n            </div>\r\n          </div>\r\n        </el-card>\r\n      </div>\r\n\r\n      <!-- 中间物品列表 -->\r\n      <div class=\"col-md-4\">\r\n        <el-card class=\"supplies-list-card\">\r\n          <template #header>\r\n            <div class=\"card-header-flex\">\r\n              <span>{{ categories[selectedCategory].name }}</span>\r\n              <el-button type=\"primary\" size=\"small\" @click.stop>\r\n                <i class=\"bi bi-plus\"></i> 添加物品\r\n              </el-button>\r\n            </div>\r\n          </template>\r\n          <div class=\"supplies-list\">\r\n            <div \r\n              v-for=\"(supply, index) in supplies\" \r\n              :key=\"index\" \r\n              class=\"supply-item\" \r\n              :class=\"{'active': selectedSupply === index}\"\r\n              @click.stop=\"selectSupply(index)\"\r\n            >\r\n              <div class=\"supply-title\">\r\n                <span>{{ supply.name }}</span>\r\n                <span class=\"supply-status\" :class=\"'status-' + supply.status\">{{ getStatusText(supply.status) }}</span>\r\n              </div>\r\n              <div class=\"supply-info\">\r\n                <span>库存: {{ supply.stock }} {{ supply.unit }}</span>\r\n                <span>{{ supply.category }}</span>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </el-card>\r\n      </div>\r\n\r\n      <!-- 右侧物品详情 -->\r\n      <div class=\"col-md-6\">\r\n        <el-card v-if=\"selectedSupply !== null\">\r\n          <div class=\"detail-header\">\r\n            <div class=\"detail-image\">\r\n              <i class=\"bi bi-box-seam\" style=\"font-size: 48px; color: #6c757d;\"></i>\r\n            </div>\r\n            <div class=\"detail-info\">\r\n              <div class=\"detail-name\">{{ supplies[selectedSupply].name }}</div>\r\n              <div class=\"detail-category\">{{ supplies[selectedSupply].category }}</div>\r\n              <div class=\"detail-actions\">\r\n                <el-button type=\"primary\" size=\"small\">\r\n                  <i class=\"bi bi-cart-plus\"></i> 申请领用\r\n                </el-button>\r\n                <el-button size=\"small\">\r\n                  <i class=\"bi bi-pencil\"></i> 编辑信息\r\n                </el-button>\r\n                <el-button size=\"small\">\r\n                  <i class=\"bi bi-plus-circle\"></i> 更新库存\r\n                </el-button>\r\n              </div>\r\n            </div>\r\n          </div>\r\n\r\n          <el-tabs v-model=\"activeTab\">\r\n            <el-tab-pane label=\"基本信息\" name=\"info\">\r\n              <div class=\"detail-section\">\r\n                <div class=\"section-title\">库存信息</div>\r\n                <el-descriptions :column=\"2\" border>\r\n                  <el-descriptions-item label=\"当前库存\">{{ supplies[selectedSupply].stock }} {{ supplies[selectedSupply].unit }}</el-descriptions-item>\r\n                  <el-descriptions-item label=\"预警阈值\">{{ supplies[selectedSupply].threshold }} {{ supplies[selectedSupply].unit }}</el-descriptions-item>\r\n                  <el-descriptions-item label=\"状态\">\r\n                    <span class=\"supply-status\" :class=\"'status-' + supplies[selectedSupply].status\">{{ getStatusText(supplies[selectedSupply].status) }}</span>\r\n                  </el-descriptions-item>\r\n                  <el-descriptions-item label=\"供应商\">{{ supplies[selectedSupply].supplier }}</el-descriptions-item>\r\n                  <el-descriptions-item label=\"单价\">¥{{ supplies[selectedSupply].price }} / {{ supplies[selectedSupply].unit }}</el-descriptions-item>\r\n                  <el-descriptions-item label=\"规格\">{{ supplies[selectedSupply].specification }}</el-descriptions-item>\r\n                  <el-descriptions-item label=\"品牌\">{{ supplies[selectedSupply].brand }}</el-descriptions-item>\r\n                  <el-descriptions-item label=\"入库日期\">{{ supplies[selectedSupply].date }}</el-descriptions-item>\r\n                </el-descriptions>\r\n              </div>\r\n\r\n              <div class=\"detail-section\">\r\n                <div class=\"section-title\">物品描述</div>\r\n                <p>{{ supplies[selectedSupply].description || '暂无描述' }}</p>\r\n              </div>\r\n\r\n              <div class=\"detail-section\">\r\n                <div class=\"section-title\">使用记录</div>\r\n                <el-table :data=\"supplies[selectedSupply].records || []\" style=\"width: 100%\">\r\n                  <el-table-column prop=\"date\" label=\"日期\" width=\"180\"></el-table-column>\r\n                  <el-table-column prop=\"user\" label=\"领用人\"></el-table-column>\r\n                  <el-table-column prop=\"department\" label=\"部门\"></el-table-column>\r\n                  <el-table-column prop=\"quantity\" label=\"数量\"></el-table-column>\r\n                  <el-table-column prop=\"status\" label=\"状态\">\r\n                    <template #default=\"scope\">\r\n                      <el-tag \r\n                        :type=\"scope.row.status === '已审批' ? 'success' : scope.row.status === '待审批' ? 'warning' : 'info'\"\r\n                      >\r\n                        {{ scope.row.status }}\r\n                      </el-tag>\r\n                    </template>\r\n                  </el-table-column>\r\n                </el-table>\r\n              </div>\r\n            </el-tab-pane>\r\n            <el-tab-pane label=\"历史记录\" name=\"history\">\r\n              <div class=\"timeline-container\">\r\n                <el-timeline>\r\n                  <el-timeline-item\r\n                    v-for=\"(activity, index) in supplies[selectedSupply].activities || []\"\r\n                    :key=\"index\"\r\n                    :timestamp=\"activity.time\"\r\n                    :type=\"getTimelineType(activity.type)\"\r\n                  >\r\n                    {{ activity.content }}\r\n                  </el-timeline-item>\r\n                </el-timeline>\r\n              </div>\r\n            </el-tab-pane>\r\n          </el-tabs>\r\n        </el-card>\r\n        \r\n        <el-empty v-else description=\"请选择一个办公用品\"></el-empty>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'Supplies',\r\n  data() {\r\n    return {\r\n      searchQuery: '',\r\n      selectedCategory: 0,\r\n      selectedSupply: 0,\r\n      activeTab: 'info',\r\n      categories: [\r\n        { name: '全部分类', icon: 'bi bi-grid', count: 120 },\r\n        { name: '办公文具', icon: 'bi bi-pencil', count: 45 },\r\n        { name: '纸张纸板', icon: 'bi bi-file-earmark', count: 20 },\r\n        { name: '电脑配件', icon: 'bi bi-pc-display', count: 15 },\r\n        { name: '办公设备', icon: 'bi bi-printer', count: 12 },\r\n        { name: '清洁用品', icon: 'bi bi-droplet', count: 18 },\r\n        { name: '食品饮料', icon: 'bi bi-cup-hot', count: 10 }\r\n      ],\r\n      supplies: [\r\n        {\r\n          name: 'A4复印纸',\r\n          category: '纸张纸板',\r\n          stock: 48,\r\n          threshold: 20,\r\n      