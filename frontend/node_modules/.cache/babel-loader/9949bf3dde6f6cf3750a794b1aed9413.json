r\n        avatar: '',\r\n        type: 'company',\r\n        isVIP: true,\r\n        phone: '0755-88889999',\r\n        email: 'contact@sztech.com',\r\n        region: '深圳市南山区',\r\n        address: '科技园南区T3栋',\r\n        source: '合作伙伴引荐',\r\n        manager: '赵经理',\r\n        createTime: '2023-01-10',\r\n        level: 5,\r\n        industry: '硬件/电子',\r\n        scale: '1000人以上',\r\n        capital: '1亿',\r\n        remark: '大型企业客户，采购流程较长，但订单体量大。',\r\n        contacts: [\r\n          {\r\n            name: '钱总',\r\n            position: 'CTO',\r\n            phone: '13900002222',\r\n            email: 'qian@sztech.com',\r\n            isMain: true\r\n          },\r\n          {\r\n            name: '孙经理',\r\n            position: 'IT经理',\r\n            phone: '13700003333',\r\n            email: 'sun@sztech.com',\r\n            isMain: false\r\n          }\r\n        ],\r\n        followups: [\r\n          {\r\n            type: '现场拜访',\r\n            title: '需求调研',\r\n            time: '2023-05-18 09:30',\r\n            creator: '赵经理',\r\n            content: '前往客户公司进行了深入需求调研，了解到客户需要将系统与现有ERP进行集成。'\r\n          },\r\n          {\r\n            type: '电话沟通',\r\n            title: '技术细节确认',\r\n            time: '2023-05-08 16:15',\r\n            creator: '王工程师',\r\n            content: '与客户技术团队沟通了系统集成的技术细节，确定了API对接方案。'\r\n          }\r\n        ]\r\n      }\r\n    ])\r\n    \r\n    // 根据筛选条件过滤客户列表\r\n    const filteredCustomers = computed(() => {\r\n      let result = customers.value\r\n      \r\n      // 根据客户类型筛选\r\n      if (customerType.value !== 'all') {\r\n        result = result.filter(customer => customer.type === customerType.value)\r\n      }\r\n      \r\n      // 根据搜索关键词筛选\r\n      if (searchQuery.value) {\r\n        const query = searchQuery.value.toLowerCase()\r\n        result = result.filter(customer => \r\n          customer.name.toLowerCase().includes(query) || \r\n          customer.phone.includes(query) || \r\n          customer.manager.toLowerCase