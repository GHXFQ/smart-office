r\n  \r\n  // 设置字体大小\r\n  document.documentElement.style.setProperty('--font-size-base', `${savedFontSize}px`);\r\n  \r\n  // 更新meta主题色\r\n  const metaThemeColor = document.querySelector('meta[name=\"theme-color\"]');\r\n  if (metaThemeColor) {\r\n    metaThemeColor.setAttribute('content', savedTheme === 'dark' ? '#1a1a1a' : '#ffffff');\r\n  }\r\n}\r\n\r\n/**\r\n * 应用主题\r\n * @param {string} theme - 主题名称，'light'或'dark'\r\n */\r\nexport function applyTheme(theme) {\r\n  document.documentElement.setAttribute('data-theme', theme);\r\n  localStorage.setItem('theme', theme);\r\n  \r\n  // 更新meta主题色\r\n  const metaThemeColor = document.querySelector('meta[name=\"theme-color\"]');\r\n  if (metaThemeColor) {\r\n    metaThemeColor.setAttribute('content', theme === 'dark' ? '#1a1a1a' : '#ffffff');\r\n  }\r\n}\r\n\r\n/**\r\n * 检查当前路由是否应该应用深色模式\r\n * @param {string} path - 当前路由路径\r\n * @returns {boolean} - 是否应该应用深色模式\r\n */\r\nexport function shouldApplyDarkMode(path) {\r\n  // 用户端页面路径，这些页面应用深色模式\r\n  const userPaths = [\r\n    '/dashboard',\r\n    '/calendar',\r\n    '/knowledge',\r\n    '/chat',\r\n    '/smartdoc',\r\n    '/contract',\r\n    '/contact',\r\n    '/project',\r\n    '/company',\r\n    '/profile'\r\n  ];\r\n  \r\n  // 检查当前路径是否匹配用户端页面\r\n  return userPaths.some(userPath => path.startsWith(userPath));\r\n} "],"mappings":"AAAA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA,OAAO,SAASA,SAASA,CAAA,EAAG;EAC1B,MAAMC,UAAU,GAAGC,YAAY,CAACC,OAAO,CAAC,OAAO,CAAC,IAAI,OAAO;EAC3D,MAAMC,YAAY,GAAGF,YAAY,CAACC,OAAO,CAAC,SAAS,CAAC,IAAI,UAAU;EAClE,MAAME,aAAa,GAAGC,QAAQ,CAACJ,YAAY,CAACC,OAAO,CAAC,UAAU,CAAC,CAAC,IAAI,EAAE;;EAEtE;EACAI,QAAQ,CAACC,eAAe,CAACC,YAAY,CAAC,YAAY,EAAER,UAAU,CAAC;;EAE/D;EACAM,QAAQ,CAACG,IAAI,CAACD,YAAY,CAAC,cAAc,EAAEL,YAAY,CAAC;;EAExD;EACAG,QAAQ,CAACC,eAAe,CAACG,KAAK,CAACC,WAAW,CAAC,kBAAkB,EAAE,GAAGP,aAAa,IAAI,CAAC;;EAEpF;EACA,MAAMQ,cAAc,GAAGN,QAAQ,CAACO,aAAa,CAAC,0BAA0B,CAAC;EACzE,IAAID,cAAc,EAAE;IAClBA,cAAc,CAACJ,YAAY,CAAC,SAAS,EAAER,UAAU,KAAK,MAAM,GAAG,SAAS,GAAG,SAAS,CA