/span>\n            <el-radio-group v-model=\"settings.theme\" size=\"large\">\n              <el-radio-button label=\"light\">\n                <el-icon><Sunny /></el-icon> 浅色\n              </el-radio-button>\n              <el-radio-button label=\"dark\">\n                <el-icon><Moon /></el-icon> 深色\n              </el-radio-button>\n            </el-radio-group>\n          </div>\n\n          <h3>布局设置</h3>\n          \n          <div class=\"setting-item\">\n            <span class=\"setting-label\">内容密度</span>\n            <el-radio-group v-model=\"settings.density\" size=\"large\">\n              <el-radio-button label=\"compact\">紧凑</el-radio-button>\n              <el-radio-button label=\"standard\">标准</el-radio-button>\n              <el-radio-button label=\"comfortable\">宽松</el-radio-button>\n            </el-radio-group>\n          </div>\n\n          <div class=\"setting-item\">\n            <span class=\"setting-label\">字体大小</span>\n            <div class=\"font-size-slider\">\n              <el-slider \n                v-model=\"settings.fontSize\" \n                :min=\"12\" \n                :max=\"20\" \n                :step=\"1\" \n                show-input\n                :format-tooltip=\"value => `${value}px`\"\n              ></el-slider>\n            </div>\n          </div>\n        </div>\n\n        <!-- AI设置部分 (简化版) -->\n        <div v-if=\"activeSection === 'ai'\" class=\"settings-section\">\n          <h3>个性化AI设置</h3>\n          <el-empty description=\"暂未实现\"></el-empty>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { Refresh, Check, Setting, Brush, Sunny, Moon } from '@element-plus/icons-vue';\n\nexport default {\n  name: 'ThemeSettings',\n  components: {\n    Refresh, \n    Check, \n    Setting, \n    Brush, \n    Sunny, \n    Moon\n  },\n  data() {\n    return {\n      activeSection: 'ui',\n      settings: {\n        theme: localStorage.getItem('theme') || 'light',\n        density: localStorage.getItem('density') || 'standard',\n        fontSize: parseInt(localStorage.getItem('fontSize')) || 14\n      },\n      defaultSettings: {\n        theme: 'light',\n        density: 'standard',\n        fontSize: 14\n      }\n    }\n  },\n  watch: {\n    'settings.theme': {\n      handler(newVal) {\n        this.applyTheme(newVal);\n      },\n      immediate: true\n    },\n    'settings.fontSize': {\n      handler(newVal) {\n        document.documentElement.style.setProperty('--font-size-base', `${newVal}px`);\n        localStorage.setItem('fontSize', newVal);\n      },\n      immediate: true\n    },\n    'settings.density': {\n      handler(newVal) {\n        document.body.setAttribute('data-density', newVal);\n        localStorage.setItem('density', newVal);\n      },\n      immediate: true\n    }\n  },\n  methods: {\n    applyTheme(theme) {\n      document.documentElement.setAttribute('data-theme', theme);\n      localStorage.setItem('theme', theme);\n      \n      // 更新meta主题色\n      const metaThemeColor = document.querySelector('meta[name=\"theme-color\"]');\n      if (metaThemeColor) {\n        metaThemeColor.setAttribute('content', theme === 'dark' ? '#1a1a1a' : '#ffffff');\n      }\n    },\n    saveSettings() {\n      // 保存设置到localStorage\n      localStorage.setItem('theme', this.settings.theme);\n      localStorage.setItem('density', this.settings.density);\n      localStorage.setItem('fontSize', this.settings.fontSize);\n      \n      // 应用设置\n      this.applyTheme(this.settings.theme);\n      document.body.setAttribute('data-density', this.settings.density);\n      document.documentElement.style.setProperty('--font-size-base', `${this.settings.fontSize}px`);\n      \n      // 通知用户\n      this.$message({\n        message: '设置已保存',\n        type: 'success'\n      });\n      \n      // 关闭设置面板\n      this.$emit('close');\n    },\n    restoreDefaults() {\n      this.settings = JSON.parse(JSON.stringify(this.defaultSettings));\n    }\n  }\n}\n</script>\n\