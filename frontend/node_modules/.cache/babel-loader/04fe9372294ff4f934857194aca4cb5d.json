form-item label=\"AI助手名称\">\r\n        <el-input v-model=\"settings.aiAssistantName\" placeholder=\"例如，小智、AI助理\" style=\"width: 300px;\"></el-input>\r\n        <div class=\"setting-description\">为您的个人AI助手起一个独特的名字。</div>\r\n      </el-form-item>\r\n\r\n      <el-form-item label=\"AI助手角色\">\r\n        <el-select v-model=\"settings.aiAssistantRole\" placeholder=\"请选择角色定位\">\r\n          <el-option label=\"通用助手\" value=\"general\"></el-option>\r\n          <el-option label=\"编程助手\" value=\"developer\"></el-option>\r\n          <el-option label=\"写作助手\" value=\"writer\"></el-option>\r\n          <el-option label=\"数据分析师\" value=\"analyst\"></el-option>\r\n        </el-select>\r\n        <div class=\"setting-description\">选择AI助手的专业角色，以便在特定领域提供更精准的帮助。</div>\r\n      </el-form-item>\r\n      \r\n      <el-form-item label=\"AI交互语言\">\r\n        <el-radio-group v-model=\"settings.aiInteractionLanguage\">\r\n          <el-radio label=\"zh-CN\">中文</el-radio>\r\n          <el-radio label=\"en-US\">英文</el-radio>\r\n          <el-radio label=\"auto\">自动检测</el-radio>\r\n        </el-radio-group>\r\n        <div class=\"setting-description\">设置与AI交互时的首选语言。</div>\r\n      </el-form-item>\r\n\r\n      <el-form-item label=\"智能摘要默认长度\">\r\n        <el-slider\r\n          v-model=\"settings.summaryDefaultLength\"\r\n          :step=\"25\"\r\n          show-stops\r\n          style=\"width: 300px;\"\r\n        ></el-slider>\r\n        <div class=\"setting-description\">调整AI生成内容摘要的默认长度（0%为最简短，100%为最详细）。</div>\r\n      </el-form-item>\r\n      \r\n      <el-form-item label=\"开启主动建议\">\r\n        <el-switch v-model=\"settings.enableProactiveSuggestions\"></el-switch>\r\n        <div class=\"setting-description\">允许AI根据您的工作内容和习惯，主动提供相关建议和信息。</div>\r\n      </el-form-item>\r\n    </el-form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { ref, watch } from 'vue';\r\n\r\nexport default {\r\n  name: 'PersonalizedAISettings',\r\n  props: {\r\n    modelValue: {\r\n      type: Object,\r\n    