{"remainingRequest":"C:\\Users\\18083\\Desktop\\oo\\smart-office\\frontend\\node_modules\\babel-loader\\lib\\index.js!C:\\Users\\18083\\Desktop\\oo\\smart-office\\frontend\\node_modules\\cache-loader\\dist\\cjs.js??ref--1-0!C:\\Users\\18083\\Desktop\\oo\\smart-office\\frontend\\node_modules\\vue-loader-v16\\dist\\index.js??ref--1-1!C:\\Users\\18083\\Desktop\\oo\\smart-office\\frontend\\src\\views\\chat\\Index.vue?vue&type=script&lang=js","dependencies":[{"path":"C:\\Users\\18083\\Desktop\\oo\\smart-office\\frontend\\src\\views\\chat\\Index.vue","mtime":1746442326657},{"path":"C:\\Users\\18083\\Desktop\\oo\\smart-office\\frontend\\babel.config.js","mtime":1746431728272},{"path":"C:\\Users\\18083\\Desktop\\oo\\smart-office\\frontend\\node_modules\\cache-loader\\dist\\cjs.js","mtime":1746431218359},{"path":"C:\\Users\\18083\\Desktop\\oo\\smart-office\\frontend\\node_modules\\babel-loader\\lib\\index.js","mtime":1746431202324},{"path":"C:\\Users\\18083\\Desktop\\oo\\smart-office\\frontend\\node_modules\\cache-loader\\dist\\cjs.js","mtime":1746431218359},{"path":"C:\\Users\\18083\\Desktop\\oo\\smart-office\\frontend\\node_modules\\vue-loader-v16\\dist\\index.js","mtime":1746431218786}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"names":["ref","computed","nextTick","onMounted","Search","Phone","VideoCamera","InfoFilled","Document","PictureFilled","FolderOpened","Microphone","Position","ChatDotSquare","name","setup","searchQuery","activeTab","selectedContact","messageInput","messageContainer","tabs","contacts","initial","avatar","color","status","lastMessage","lastTime","unread","filteredContacts","value","filter","contact","toLowerCase","includes","messages","sender","content","type","time","size","sendMessage","trim","push","formatTime","Date","scrollTop","scrollHeight","setTimeout","date","hours","getHours","toString","padStart","minutes","getMinutes","getStatusText","statusMap"],"sources":["C:\\Users\\18083\\Desktop\\oo\\smart-office\\frontend\\src\\views\\chat\\Index.vue"],"sourcesContent":["<template>\r\n  <div class=\"chat-container\">\r\n    <div class=\"row h-100\">\r\n      <div class=\"col-12\">\r\n        <el-card class=\"chat-card\">\r\n          <div class=\"chat-layout\">\r\n            <!-- 左侧联系人列表 -->\r\n            <div class=\"chat-sidebar\">\r\n              <!-- 搜索框 -->\r\n              <div class=\"search-box\">\r\n                <el-input \r\n                  v-model=\"searchQuery\" \r\n                  placeholder=\"搜索联系人...\"\r\n                  clearable\r\n                  :prefix-icon=\"Search\"\r\n                ></el-input>\r\n              </div>\r\n              \r\n              <!-- 聊天标签页 -->\r\n              <div class=\"chat-tabs\">\r\n                <div \r\n                  v-for=\"(tab, index) in tabs\" \r\n                  :key=\"index\"\r\n                  class=\"chat-tab\"\r\n                  :class=\"{'active': activeTab === index}\"\r\n                  @click=\"activeTab = index\"\r\n                >\r\n                  {{ tab }}\r\n                </div>\r\n              </div>\r\n              \r\n              <!-- 联系人列表 -->\r\n              <div class=\"contacts-list\">\r\n                <div \r\n                  v-for=\"(contact, index) in filteredContacts\" \r\n                  :key=\"index\"\r\n                  class=\"contact-item\"\r\n                  :class=\"{'active': selectedContact === index}\"\r\n                  @click=\"selectedContact = index\"\r\n                >\r\n                  <div class=\"contact-avatar\">\r\n                    <div \r\n                      v-if=\"!contact.avatar\" \r\n                      class=\"avatar\" \r\n                      :style=\"{backgroundColor: contact.color}\"\r\n                    >\r\n                      <span class=\"avatar-initial\">{{ contact.initial }}</span>\r\n                    </div>\r\n                    <img v-else :src=\"contact.avatar\" alt=\"avatar\" class=\"avatar\" />\r\n                    <div \r\n                      class=\"contact-status\" \r\n                      :class=\"'status-' + contact.status\"\r\n                    ></div>\r\n                  </div>\r\n                  <div class=\"contact-info\">\r\n                    <div class=\"contact-name\">\r\n                      <span>{{ contact.name }}</span>\r\n                      <span class=\"time\">{{ contact.lastTime }}</span>\r\n                    </div>\r\n                    <div class=\"contact-message\">\r\n                      <span>{{ contact.lastMessage }}</span>\r\n                      <el-badge \r\n                        v-if=\"contact.unread\" \r\n                        :value=\"contact.unread\" \r\n                        class=\"message-badge\"\r\n                      />\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n            \r\n            <!-- 右侧聊天内容 -->\r\n            <div class=\"chat-content\">\r\n              <!-- 聊天头部 -->\r\n              <div class=\"chat-header\" v-if=\"selectedContact !== null\">\r\n                <div class=\"contact-avatar\">\r\n                  <div \r\n                    v-if=\"!contacts[selectedContact].avatar\" \r\n                    class=\"avatar\" \r\n                    :style=\"{backgroundColor: contacts[selectedContact].color}\"\r\n                  >\r\n                    <span class=\"avatar-initial\">{{ contacts[selectedContact].initial }}</span>\r\n                  </div>\r\n                  <img \r\n                    v-else \r\n                    :src=\"contacts[selectedContact].avatar\" \r\n                    alt=\"avatar\" \r\n                    class=\"avatar\" \r\n                  />\r\n                  <div \r\n                    class=\"contact-status\" \r\n                    :class=\"'status-' + contacts[selectedContact].status\"\r\n                  ></div>\r\n                </div>\r\n                <div class=\"contact-info\">\r\n                  <h6>{{ contacts[selectedContact].name }}</h6>\r\n                  <p>{{ getStatusText(contacts[selectedContact].status) }}</p>\r\n                </div>\r\n                <div class=\"chat-actions\">\r\n                  <el-tooltip content=\"语音通话\" placement=\"bottom\">\r\n                    <el-button circle>\r\n                      <el-icon><Phone /></el-icon>\r\n                    </el-button>\r\n                  </el-tooltip>\r\n                  <el-tooltip content=\"视频通话\" placement=\"bottom\">\r\n                    <el-button circle>\r\n                      <el-icon><VideoCamera /></el-icon>\r\n                    </el-button>\r\n                  </el-tooltip>\r\n                  <el-tooltip content=\"聊天信息\" placement=\"bottom\">\r\n                    <el-button circle>\r\n                      <el-icon><InfoFilled /></el-icon>\r\n                    </el-button>\r\n                  </el-tooltip>\r\n                </div>\r\n              </div>\r\n              \r\n              <!-- 聊天消息区域 -->\r\n              <div class=\"chat-messages\" v-if=\"selectedContact !== null\" ref=\"messageContainer\">\r\n                <div \r\n                  v-for=\"(message, index) in messages\" \r\n                  :key=\"index\"\r\n                  class=\"message\"\r\n                  :class=\"message.sender === 'me' ? 'message-sent' : 'message-received'\"\r\n                >\r\n                  <div class=\"message-container\">\r\n                    <div v-if=\"message.sender !== 'me'\" class=\"message-avatar\">\r\n                      <div \r\n                        class=\"avatar small\" \r\n                        :style=\"{backgroundColor: contacts[selectedContact].color}\"\r\n                      >\r\n                        <span class=\"avatar-initial\">{{ contacts[selectedContact].initial }}</span>\r\n                      </div>\r\n                    </div>\r\n                    <div class=\"message-content-wrapper\">\r\n                      <div \r\n                        class=\"message-content\"\r\n                        :class=\"message.sender === 'me' ? 'sent' : 'received'\"\r\n                      >\r\n                        <span v-if=\"message.type === 'text'\">{{ message.content }}</span>\r\n                        <div v-else-if=\"message.type === 'image'\" class=\"message-image\">\r\n                          <img :src=\"message.content\" alt=\"image\" />\r\n                        </div>\r\n                        <div v-else-if=\"message.type === 'file'\" class=\"message-file\">\r\n                          <el-icon><Document /></el-icon>\r\n                          <span>{{ message.content.name }}</span>\r\n                          <span class=\"file-size\">{{ message.content.size }}</span>\r\n                        </div>\r\n                      </div>\r\n                      <div class=\"message-time\">{{ message.time }}</div>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n              \r\n              <!-- 聊天输入框 -->\r\n              <div class=\"chat-input\" v-if=\"selectedContact !== null\">\r\n                <div class=\"chat-tools\">\r\n                  <el-button circle>\r\n                    <el-icon><PictureFilled /></el-icon>\r\n                  </el-button>\r\n                  <el-button circle>\r\n                    <el-icon><FolderOpened /></el-icon>\r\n                  </el-button>\r\n                  <el-button circle>\r\n                    <el-icon><VideoCamera /></el-icon>\r\n                  </el-button>\r\n                  <el-button circle>\r\n                    <el-icon><Microphone /></el-icon>\r\n                  </el-button>\r\n                </div>\r\n                <div class=\"input-area\">\r\n                  <el-input\r\n                    v-model=\"messageInput\"\r\n                    type=\"textarea\"\r\n                    :rows=\"3\"\r\n                    placeholder=\"输入消息...\"\r\n                    resize=\"none\"\r\n                    @keyup.enter.prevent=\"sendMessage\"\r\n                  ></el-input>\r\n                </div>\r\n                <div class=\"send-button\">\r\n                  <el-button type=\"primary\" @click=\"sendMessage\">\r\n                    <el-icon><Position /></el-icon>\r\n                    发送\r\n                  </el-button>\r\n                </div>\r\n              </div>\r\n              \r\n              <!-- 无选中联系人提示 -->\r\n              <div class=\"empty-chat\" v-if=\"selectedContact === null\">\r\n                <div class=\"empty-illustration\">\r\n                  <el-icon :size=\"64\"><ChatDotSquare /></el-icon>\r\n                </div>\r\n                <h4>选择一个联系人开始聊天</h4>\r\n                <p>或者使用搜索查找联系人</p>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </el-card>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { ref, computed, nextTick, onMounted } from 'vue'\r\nimport { \r\n  Search, Phone, VideoCamera, InfoFilled, Document, PictureFilled, \r\n  FolderOpened, Microphone, Position, ChatDotSquare\r\n} from '@element-plus/icons-vue'\r\n\r\nexport default {\r\n  name: 'Chat',\r\n  setup() {\r\n    const searchQuery = ref('')\r\n    const activeTab = ref(0)\r\n    const selectedContact = ref(0)\r\n    const messageInput = ref('')\r\n    const messageContainer = ref(null)\r\n    \r\n    // 标签页\r\n    const tabs = ref(['最近聊天', '同事', '群组'])\r\n    \r\n    // 联系人列表\r\n    const contacts = ref([\r\n      {\r\n        name: '张经理',\r\n        initial: '张',\r\n        avatar: '',\r\n        color: '#007bff',\r\n        status: 'online',\r\n        lastMessage: '好的，我稍后会把文件发给你',\r\n        lastTime: '12:30',\r\n        unread: 2\r\n      },\r\n      {\r\n        name: '技术部群',\r\n        initial: '技',\r\n        avatar: '',\r\n        color: '#28a745',\r\n        status: 'online',\r\n        lastMessage: '李工：项目进度如何了？',\r\n        lastTime: '11:45',\r\n        unread: 5\r\n      },\r\n      {\r\n        name: '王总',\r\n        initial: '王',\r\n        avatar: '',\r\n        color: '#dc3545',\r\n        status: 'busy',\r\n        lastMessage: '下午3点开会，记得准备材料',\r\n        lastTime: '上午',\r\n        unread: 0\r\n      },\r\n      {\r\n        name: '李工',\r\n        initial: '李',\r\n        avatar: '',\r\n        color: '#fd7e14',\r\n        status: 'online',\r\n        lastMessage: '我已经检查过了，没问题',\r\n        lastTime: '上午',\r\n        unread: 0\r\n      },\r\n      {\r\n        name: '市场部群',\r\n        initial: '市',\r\n        avatar: '',\r\n        color: '#6f42c1',\r\n        status: 'online',\r\n        lastMessage: '赵经理：本月销售情况统计出来了吗？',\r\n        lastTime: '昨天',\r\n        unread: 0\r\n      },\r\n      {\r\n        name: '赵经理',\r\n        initial: '赵',\r\n        avatar: '',\r\n        color: '#20c997',\r\n        status: 'away',\r\n        lastMessage: '这个客户的意向很强，可以跟进一下',\r\n        lastTime: '昨天',\r\n        unread: 0\r\n      },\r\n      {\r\n        name: '钱工',\r\n        initial: '钱',\r\n        avatar: '',\r\n        color: '#17a2b8',\r\n        status: 'offline',\r\n        lastMessage: '好的，我了解了，谢谢',\r\n        lastTime: '星期三',\r\n        unread: 0\r\n      }\r\n    ])\r\n    \r\n    // 根据搜索过滤联系人\r\n    const filteredContacts = computed(() => {\r\n      if (!searchQuery.value) return contacts.value\r\n      return contacts.value.filter(contact =>\r\n        contact.name.toLowerCase().includes(searchQuery.value.toLowerCase())\r\n      )\r\n    })\r\n    \r\n    // 模拟聊天消息\r\n    const messages = ref([\r\n      {\r\n        sender: 'other',\r\n        content: '你好，张工，那个智能办公系统的需求文档完成了吗？',\r\n        type: 'text',\r\n        time: '上午 11:30'\r\n      },\r\n      {\r\n        sender: 'me',\r\n        content: '已经完成了，我正在做最后的审核',\r\n        type: 'text',\r\n        time: '上午 11:35'\r\n      },\r\n      {\r\n        sender: 'other',\r\n        content: '太好了，什么时候能发过来看看？',\r\n        type: 'text',\r\n        time: '上午 11:36'\r\n      },\r\n      {\r\n        sender: 'me',\r\n        content: '稍等，我下午应该可以发给你',\r\n        type: 'text',\r\n        time: '上午 11:40'\r\n      },\r\n      {\r\n        sender: 'me',\r\n        content: '同时我还准备了一些界面原型',\r\n        type: 'text',\r\n        time: '上午 11:40'\r\n      },\r\n      {\r\n        sender: 'other',\r\n        content: '好的，那我等你的消息',\r\n        type: 'text',\r\n        time: '上午 11:42'\r\n      },\r\n      {\r\n        sender: 'other',\r\n        content: {\r\n          name: '智能办公系统-界面原型.zip',\r\n          size: '5.4MB'\r\n        },\r\n        type: 'file',\r\n        time: '上午 11:45'\r\n      },\r\n      {\r\n        sender: 'me',\r\n        content: '好的，我稍后会把文件发给你',\r\n        type: 'text',\r\n        time: '上午 12:30'\r\n      }\r\n    ])\r\n    \r\n    // 发送消息\r\n    const sendMessage = () => {\r\n      if (!messageInput.value.trim()) return\r\n      \r\n      messages.value.push({\r\n        sender: 'me',\r\n        content: messageInput.value,\r\n        type: 'text',\r\n        time: formatTime(new Date())\r\n      })\r\n      \r\n      messageInput.value = ''\r\n      \r\n      // 滚动到底部\r\n      nextTick(() => {\r\n        if (messageContainer.value) {\r\n          messageContainer.value.scrollTop = messageContainer.value.scrollHeight\r\n        }\r\n      })\r\n      \r\n      // 模拟回复\r\n      setTimeout(() => {\r\n        messages.value.push({\r\n          sender: 'other',\r\n          content: '好的，我收到了',\r\n          type: 'text',\r\n          time: formatTime(new Date())\r\n        })\r\n        \r\n        // 滚动到底部\r\n        nextTick(() => {\r\n          if (messageContainer.value) {\r\n            messageContainer.value.scrollTop = messageContainer.value.scrollHeight\r\n          }\r\n        })\r\n      }, 1000)\r\n    }\r\n    \r\n    // 格式化时间\r\n    const formatTime = (date) => {\r\n      const hours = date.getHours().toString().padStart(2, '0')\r\n      const minutes = date.getMinutes().toString().padStart(2, '0')\r\n      return `${hours}:${minutes}`\r\n    }\r\n    \r\n    // 获取状态文字\r\n    const getStatusText = (status) => {\r\n      const statusMap = {\r\n        'online': '在线',\r\n        'offline': '离线',\r\n        'busy': '忙碌',\r\n        'away': '离开'\r\n      }\r\n      return statusMap[status] || status\r\n    }\r\n    \r\n    // 初始化时滚动到底部\r\n    onMounted(() => {\r\n      nextTick(() => {\r\n        if (messageContainer.value) {\r\n          messageContainer.value.scrollTop = messageContainer.value.scrollHeight\r\n        }\r\n      })\r\n    })\r\n    \r\n    return {\r\n      searchQuery,\r\n      activeTab,\r\n      selectedContact,\r\n      tabs,\r\n      contacts,\r\n      filteredContacts,\r\n      messages,\r\n      messageInput,\r\n      messageContainer,\r\n      // 图标\r\n      Search,\r\n      Phone,\r\n      VideoCamera,\r\n      InfoFilled,\r\n      Document,\r\n      PictureFilled,\r\n      FolderOpened,\r\n      Microphone,\r\n      Position,\r\n      ChatDotSquare,\r\n      // 方法\r\n      sendMessage,\r\n      getStatusText\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.chat-container {\r\n  padding: 20px;\r\n  height: 100%;\r\n}\r\n\r\n.row.h-100 {\r\n  height: 100%;\r\n}\r\n\r\n.chat-card {\r\n  height: calc(100vh - 100px);\r\n  display: flex;\r\n  flex-direction: column;\r\n}\r\n\r\n.chat-layout {\r\n  display: flex;\r\n  height: 100%;\r\n}\r\n\r\n/* 侧边栏样式 */\r\n.chat-sidebar {\r\n  width: 300px;\r\n  border-right: 1px solid #f0f0f0;\r\n  display: flex;\r\n  flex-direction: column;\r\n  background-color: #fff;\r\n}\r\n\r\n.search-box {\r\n  padding: 15px;\r\n  border-bottom: 1px solid #f0f0f0;\r\n}\r\n\r\n.chat-tabs {\r\n  display: flex;\r\n  border-bottom: 1px solid #f0f0f0;\r\n}\r\n\r\n.chat-tab {\r\n  flex: 1;\r\n  text-align: center;\r\n  padding: 15px;\r\n  cursor: pointer;\r\n  color: #6c757d;\r\n  font-weight: 500;\r\n  position: relative;\r\n  transition: all 0.2s;\r\n}\r\n\r\n.chat-tab.active {\r\n  color: #007bff;\r\n}\r\n\r\n.chat-tab.active::after {\r\n  content: '';\r\n  position: absolute;\r\n  bottom: -1px;\r\n  left: 0;\r\n  right: 0;\r\n  height: 2px;\r\n  background-color: #007bff;\r\n}\r\n\r\n.contacts-list {\r\n  overflow-y: auto;\r\n  flex: 1;\r\n}\r\n\r\n.contact-item {\r\n  display: flex;\r\n  align-items: center;\r\n  padding: 15px;\r\n  border-bottom: 1px solid #f0f0f0;\r\n  cursor: pointer;\r\n  transition: all 0.2s;\r\n}\r\n\r\n.contact-item:hover, .contact-item.active {\r\n  background-color: #f8f9fa;\r\n}\r\n\r\n.contact-avatar {\r\n  width: 40px;\r\n  height: 40px;\r\n  border-radius: 50%;\r\n  overflow: hidden;\r\n  margin-right: 15px;\r\n  position: relative;\r\n  flex-shrink: 0;\r\n}\r\n\r\n.avatar {\r\n  width: 40px;\r\n  height: 40px;\r\n  border-radius: 50%;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  font-weight: 500;\r\n  font-size: 1rem;\r\n  overflow: hidden;\r\n}\r\n\r\n.avatar.small {\r\n  width: 30px;\r\n  height: 30px;\r\n  font-size: 0.8rem;\r\n}\r\n\r\n.avatar img {\r\n  width: 100%;\r\n  height: 100%;\r\n  object-fit: cover;\r\n}\r\n\r\n.avatar-initial {\r\n  color: #fff;\r\n}\r\n\r\n.contact-status {\r\n  position: absolute;\r\n  bottom: 0;\r\n  right: 0;\r\n  width: 10px;\r\n  height: 10px;\r\n  border-radius: 50%;\r\n  border: 2px solid #fff;\r\n}\r\n\r\n.status-online {\r\n  background-color: #28a745;\r\n}\r\n\r\n.status-offline {\r\n  background-color: #6c757d;\r\n}\r\n\r\n.status-busy {\r\n  background-color: #dc3545;\r\n}\r\n\r\n.status-away {\r\n  background-color: #ffc107;\r\n}\r\n\r\n.contact-info {\r\n  flex: 1;\r\n  overflow: hidden;\r\n}\r\n\r\n.contact-name {\r\n  font-weight: 500;\r\n  color: #343a40;\r\n  margin-bottom: 3px;\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n}\r\n\r\n.contact-name .time {\r\n  font-size: 12px;\r\n  color: #adb5bd;\r\n  font-weight: normal;\r\n}\r\n\r\n.contact-message {\r\n  font-size: 13px;\r\n  color: #6c757d;\r\n  margin: 0;\r\n  white-space: nowrap;\r\n  overflow: hidden;\r\n  text-overflow: ellipsis;\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n}\r\n\r\n/* 聊天内容区域样式 */\r\n.chat-content {\r\n  flex: 1;\r\n  display: flex;\r\n  flex-direction: column;\r\n  background-color: #fff;\r\n  position: relative;\r\n}\r\n\r\n.chat-header {\r\n  display: flex;\r\n  align-items: center;\r\n  padding: 15px;\r\n  border-bottom: 1px solid #f0f0f0;\r\n}\r\n\r\n.chat-header .contact-info {\r\n  margin-left: 0;\r\n}\r\n\r\n.chat-header .contact-info h6 {\r\n  margin: 0;\r\n  font-weight: 600;\r\n}\r\n\r\n.chat-header .contact-info p {\r\n  margin: 0;\r\n  font-size: 13px;\r\n  color: #6c757d;\r\n}\r\n\r\n.chat-header .chat-actions {\r\n  margin-left: auto;\r\n  display: flex;\r\n  gap: 10px;\r\n}\r\n\r\n.chat-messages {\r\n  flex: 1;\r\n  overflow-y: auto;\r\n  padding: 20px;\r\n  background-color: #f5f7fa;\r\n}\r\n\r\n.message {\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.message-container {\r\n  display: flex;\r\n  align-items: flex-start;\r\n}\r\n\r\n.message-avatar {\r\n  margin-right: 10px;\r\n}\r\n\r\n.message-content-wrapper {\r\n  display: flex;\r\n  flex-direction: column;\r\n}\r\n\r\n.message-content {\r\n  padding: 10px 15px;\r\n  border-radius: 18px;\r\n  max-width: 70%;\r\n  margin-bottom: 5px;\r\n  word-break: break-word;\r\n}\r\n\r\n.message-content.received {\r\n  background-color: #fff;\r\n  color: #343a40;\r\n  border-bottom-left-radius: 4px;\r\n}\r\n\r\n.message-content.sent {\r\n  background-color: #007bff;\r\n  color: #fff;\r\n  border-bottom-right-radius: 4px;\r\n}\r\n\r\n.message-sent {\r\n  align-items: flex-end;\r\n}\r\n\r\n.message-sent .message-container {\r\n  flex-direction: row-reverse;\r\n}\r\n\r\n.message-time {\r\n  font-size: 12px;\r\n  color: #adb5bd;\r\n  margin-top: 2px;\r\n}\r\n\r\n.message-image img {\r