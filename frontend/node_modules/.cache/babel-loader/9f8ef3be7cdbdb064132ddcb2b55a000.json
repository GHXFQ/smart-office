ws\\auth\\Register.vue"],"sourcesContent":["<template>\n  <div class=\"register-container\">\n    <div class=\"register-box\">\n      <div class=\"register-header\">\n        <img src=\"https://cdn-icons-png.flaticon.com/512/2891/2891491.png\" alt=\"Logo\">\n        <h4>创建您的账户</h4>\n        <p>欢迎加入智能办公系统</p>\n      </div>\n\n      <el-form ref=\"registerFormRef\" :model=\"registerForm\" :rules=\"registerRules\" class=\"register-form\">\n        <el-form-item prop=\"account\">\n          <el-input \n            v-model=\"registerForm.account\" \n            placeholder=\"请输入您的账号 (例如：user123)\"\n          >\n            <template #prefix>\n              <el-icon><User /></el-icon>\n            </template>\n          </el-input>\n        </el-form-item>\n        \n        <el-form-item prop=\"email\">\n          <el-input \n            v-model=\"registerForm.email\" \n            placeholder=\"请输入您的邮箱地址\"\n          >\n            <template #prefix>\n              <el-icon><Message /></el-icon>\n            </template>\n          </el-input>\n          <div class=\"form-text\">我们将发送验证邮件到此邮箱，也可用于密码找回。</div>\n        </el-form-item>\n\n        <el-form-item prop=\"password\">\n          <el-input \n            v-model=\"registerForm.password\" \n            type=\"password\" \n            placeholder=\"请输入密码 (至少8位)\"\n            show-password\n          >\n            <template #prefix>\n              <el-icon><Lock /></el-icon>\n            </template>\n          </el-input>\n        </el-form-item>\n\n        <el-form-item prop=\"confirmPassword\">\n          <el-input \n            v-model=\"registerForm.confirmPassword\" \n            type=\"password\" \n            placeholder=\"请再次输入密码\"\n            show-password\n          >\n            <template #prefix>\n              <el-icon><Lock /></el-icon>\n            </template>\n          </el-input>\n        </el-form-item>\n\n        <el-divider />\n\n        <div class=\"security-section\">\n          <h5>设置安全问题 (用于密码找回)</h5>\n          \n          <el-form-item prop=\"securityQuestion\">\n            <el-select\n              v-model=\"registerForm.securityQuestion\"\n              placeholder=\"-- 请选择 --\"\n              class=\"security-select\"\n              @change=\"handleQuestionChange\"\n            >\n              <el-option\n                v-for=\"item in securityQuestions\"\n                :key=\"item.value\"\n                :label=\"item.label\"\n                :value=\"item.value\"\n              />\n            </el-select>\n          </el-form-item>\n\n          <el-form-item v-if=\"showCustomQuestion\" prop=\"customQuestion\">\n            <el-input \n              v-model=\"registerForm.customQuestion\" \n              placeholder=\"请输入您的自定义问题\"\n            />\n          </el-form-item>\n\n          <el-form-item prop=\"securityAnswer\">\n            <el-input \n              v-model=\"registerForm.securityAnswer\" \n              placeholder=\"请输入安全问题的答案\"\n            />\n            <div class=\"form-text\">请牢记您的问题和答案。</div>\n          </el-form-item>\n        </div>\n\n        <el-button type=\"primary\" class=\"register-button\" @click=\"handleRegister\" :loading=\"loading\">\n          注册\n        </el-button>\n      </el-form>\n\n      <div class=\"login-link\">\n        <p>已有账号？<router-link to=\"/login\">立即登录</router-link></p>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { ref, reactive } from 'vue'\nimport { useRouter } from 'vue-router'\nimport { ElMessage } from 'element-plus'\nimport { User, Lock, Message } from '@element-plus/icons-vue'\nimport { register } from '@/api/auth'\n\nexport default {\n  name: 'Register',\n  setup() {\n    const router = useRouter();\n    const registerFormRef = ref(null);\n    const loading = ref(false);\n    const showCustomQuestion = ref(false);\n\n    // 安全问题选项\n    const securityQuestions = [\n      { value: 'birth_city', label: '您出生的城市是？' },\n      { value: 'first_pet', label: '您的第一只宠物的名字是？' },\n      { value: 'mother_maiden_name', label: '您母亲的娘家姓是？' },\n      { value: 'favorite_color', label: '您最喜欢的颜色是？' },\n      { value: 'custom', label: '自定义问题...' }\n    ];\n\n    // 注册表单数据\n    const registerForm = reactive({\n      account: '',\n      email: '',\n      password: '',\n      confirmPassword: '',\n      securityQuestion: '',\n      customQuestion: '',\n      securityAnswer: ''\n    });\n\n    // 注册表单验证规则\n    const registerRules = {\n      account: [\n        { required: true, message: '请输入账号', trigger: 'blur' },\n        { min: 4, message: '账号长度至少4个字符', trigger: 'blur' }\n      ],\n      email: [\n        { required: true, message: '请输入邮箱', trigger: 'blur' },\n        { type: 'email', message: '请输入正确的邮箱格式', trigger: 'blur' }\n      ],\n      password: [\n        { required: true, message: '请输入密码', trigger: 'blur' },\n        { min: 8, message: '密码长度至少8个字符', trigger: 'blur' }\n      ],\n      confirmPassword: [\n        { required: true, message: '请确认密码', trigger: 'blur' },\n        { \n          validator: (rule, value, callback) => {\n            if (value !== registerForm.password) {\n              callback(new Error('两次输入密码不一致'));\n            } else {\n              callback();\n            }\n          }, \n          trigger: 'blur' \n        }\n      ],\n      securityQuestion: [\n        { required: true, message: '请选择安全问题', trigger: 'change' }\n      ],\n      customQuestion: [\n        { \n          validator: (rule, value, callback) => {\n            if (registerForm.securityQuestion === 'custom' && !value) {\n              callback(new Error('请输入自定义安全问题'));\n            } else {\n              callback();\n            }\n          }, \n          trigger: 'blur' \n        }\n      ],\n      securityAnswer: [\n        { required: true, message: '请输入安全问题答案', trigger: 'blur' }\n      ]\n    };\n\n    // 处理安全问题选择变化\n    const handleQuestionChange = (value) => {\n      showCustomQuestion.value = value === 'custom';\n      if (value !== 'custom') {\n        registerForm.customQuestion = '';\n      }\n    };\n\n    // 处理注册\n    const handleRegister = async () => {\n      if (!registerFormRef.value) return;\n      \n      try {\n        await registerFormRef.value.validate();\n      } catch (error) {\n        return;\n      }\n      \n      loading.value = true;\n      \n      try {\n        // 创建注册请求数据\n        const registerData = {\n          username: registerForm.account,\n          email: registerForm.email,\n          password: registerForm.password,\n          confirmPassword: registerForm.confirmPassword,\n          security_question: registerForm.securityQuestion === 'custom' \n            ? registerForm.customQuestion \n            : registerForm.securityQuestion,\n          security_answer: registerForm.securityAnswer\n        };\n        \n        // 调用注册API\n        const response = await register(registerData);\n        \n        if (response.success) {\n          ElMessage.success(response.message || '注册成功！');\n          router.push('/login');\n        } else {\n          ElMessage.error(response.message || '注册失败');\n        }\n      } catch (error) {\n        ElMessage.error('注册失败：' + (error.message || '未知错误'));\n      } finally {\n        loading.value = false;\n      }\n    };\n\n    return {\n      registerFormRef,\n      registerForm,\n      registerRules,\n      loading,\n      securityQuestions,\n      showCustomQuestion,\n      handleQuestionChange,\n      handleRegister,\n      User,\n      Lock,\n      Message\n    };\n  }\n}\n</script>\n\n<style scoped>\n.register-container {\n  height: 100vh;\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  background: linear-gradient(135deg, #667eea 0%, #764ba2 100%);\n}\n\n.register-box {\n  width: 450px;\n  background-color: #ffffff;\n  border-radius: 10px;\n  box-shadow: 0 0 20px rgba(0, 0, 0, 0.1);\n  padding: 40px;\n}\n\n.register-header {\n  text-align: center;\n  margin-bottom: 30px;\n}\n\n.register-header img {\n  width: 60px;\n  margin-bottom: 15px;\n}\n\n.register-header h4 {\n  font-weight: 600;\n  color: #333;\n  margin-bottom: 8px;\n  font-size: 20px;\n}\n\n.register-header p {\n  color: #6c757d;\n  font-size: 14px;\n}\n\n.register-form {\n  margin-bottom: 20px;\n}\n\n.register-button {\n  width: 100%;\n  margin-top: 20px;\n  height: 40px;\n  font-size: 16px;\n}\n\n.login-link {\n  text-align: center;\n  margin-top: 20px;\n  font-size: 14px;\n  color: #6c757d;\n}\n\n.login-link a {\n  color: #1890ff;\n  text-decoration: none;\n}\n\n.login-link a:hover {\n  text-decoration: underline;\n}\n\n.form-text {\n  font-size: 12px;\n  color: #6c757d;\n  margin-top: 4px;\n  margin-bottom: 12px;\n}\n\n.security-section {\n  margin-bottom: 20px;\n}\n\n.security-section h5 {\n  font-size: 16px;\n  font-weight: 500;\n  margin-bottom: 20px;\n  color: #333;\n}\n\n.security-select {\n  width: 100%;\n}\n\n:deep(.el-divider--horizontal) {\n  margin: 24px 0;\n}\n</style> "],"mappings":";;AA6GA,SAASA,GAAG,EAAEC,QAAO,QAAS,KAAI;AAClC,SAASC,SAAQ,QAAS,YAAW;AACrC,SAASC,SAAQ,QAAS,cAAa;AACvC,SAASC,IAAI,EAAEC,IAAI,EAAEC,OAAM,QAAS,yBAAwB;AAC5D,SAASC,QAAO,QAAS,YAAW;AAEpC,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,KAAKA,CAAA,EAAG;IACN,MAAMC,MAAK,GAAIR,SAAS,CAAC,CAAC;IAC1B,MAAMS,eAAc,GAAIX,GAAG,CAAC,IAAI,CAAC;IACjC,MAAMY,OAAM,GAAIZ,GAAG,CAAC,KAAK,CAAC;IAC1B,MAAMa,kBAAiB,GAAIb,GAAG,CAAC,KAAK,CAAC;;IAErC;IACA,MAAMc,iBAAgB,GAAI,CACxB;MAAEC,KAAK,EAAE,YAAY;MAAEC,KAAK,EAAE;IAAW,CAAC,EAC1C;MAAED,KAAK,EAAE,WAAW;MAAEC,KAAK,EAAE;IAAe,CAAC,EAC7C;MAAED,KAAK,EAAE,oBAAoB;MAAEC,KAAK,EAAE;IAAY,CAAC,EACnD;MAAED,KAAK,EAAE,gBAAgB;MAAEC,KAAK,EAAE;IAAY,CAAC,EAC/C;MAAED,KAAK,EAAE,QAAQ;MAAEC,KAAK,EAAE;IAAW,EACtC;;IAED;IACA,MAAMC,YAAW,GAAIhB,QAAQ,CAAC;MAC5BiB,OAAO,EAAE,EAAE;MACXC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE,EAAE;MACZC,eAAe,EAAE,EAAE;MACnBC,gBAAgB,EAAE,EAAE;MACpBC,cAAc,EAAE,EAAE;MAClBC,cAAc,EAAE;IAClB,CAAC,CAAC;;IAEF;IACA,MAAMC,aAAY,GAAI;MACpBP,OAAO,EAAE,CACP;QAAEQ,QAAQ,EAAE,IAAI;QAAEC,OAAO,EAAE,OAAO;QAAEC,OAAO,EAAE;MAAO,CAAC