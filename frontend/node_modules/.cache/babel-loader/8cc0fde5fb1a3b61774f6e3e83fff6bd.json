ateElement","innerHTML","body","appendChild","errorModal","addEventListener","removeChild"],"sources":["C:\\Users\\18083\\Desktop\\CCIT\\office\\o\\smart-office\\frontend\\src\\views\\admin\\ContractTemplateManagement.vue"],"sourcesContent":["<template>\n  <div class=\"contract-template-management\">\n    <h1 class=\"page-title\">合同模板管理</h1>\n\n    <div class=\"actions\">\n      <router-link :to=\"{ name: 'AdminContractTemplateNew' }\" class=\"btn btn-primary\">\n        <i class=\"bi bi-plus-lg\"></i> 新建模板\n      </router-link>\n    </div>\n\n    <div class=\"template-list\">\n      <div v-if=\"loading\" class=\"loading-spinner\">\n        <div class=\"spinner-border text-primary\" role=\"status\">\n          <span class=\"visually-hidden\">加载中...</span>\n        </div>\n      </div>\n      <div v-else-if=\"templates.length === 0\" class=\"empty-state\">\n        <i class=\"bi bi-file-earmark-text-fill\"></i>\n        <p>暂无合同模板</p>\n        <span>点击\"新建模板\"按钮来创建一个吧</span>\n      </div>\n      <div v-else class=\"table-responsive\">\n        <table class=\"table table-hover align-middle\">\n          <thead>\n            <tr>\n              <th>模板名称</th>\n              <th>合同类型</th>\n              <th>行业领域</th>\n              <th>创建人</th>\n              <th>创建时间</th>\n              <th>状态</th>\n              <th>操作</th>\n            </tr>\n          </thead>\n          <tbody>\n            <tr v-for=\"template in filteredTemplates\" :key=\"template.id\">\n              <td>{{ template.name }}</td>\n              <td><span class=\"badge bg-secondary\">{{ template.contract_type }}</span></td>\n              <td>{{ template.industry || '-' }}</td>\n              <td>{{ template.created_by_name }}</td>\n              <td>{{ new Date(template.created_at).toLocaleDateString() }}</td>\n              <td>\n                <div class=\"form-check form-switch d-flex align-items-center\">\n                  <input class=\"form-check-input\" type=\"checkbox\" role=\"switch\" :id=\"'switch-' + template.id\" v-model=\"template.is_active\" @change=\"toggleStatus(template)\">\n                  <label class=\"form-check-label ms-2\" :for=\"'switch-' + template.id\">{{ template.is_active ? '已启用' : '已禁用' }}</label>\n                </div>\n              </td>\n              <td>\n                <router-link :to=\"{ name: 'AdminContractTemplateEdit', params: { id: template.id } }\" class=\"btn btn-sm btn-outline-primary me-2\">\n                  <i class=\"bi bi-pencil-square\"></i> 编辑\n                </router-link>\n                <button @click=\"confirmDelete(template)\" class=\"btn btn-sm btn-outline-danger\">\n                  <i class=\"bi bi-trash\"></i> 删除\n                </button>\n              </td>\n            </tr>\n          </tbody>\n        </table>\n      </div>\n    </div>\n\n    <!-- 删除确认弹窗 -->\n    <div class=\"modal fade\" id=\"deleteConfirmModal\" tabindex=\"-1\" aria-hidden=\"true\">\n      <div class=\"modal-dialog modal-dialog-centered\">\n        <div class=\"modal-content\">\n          <div class=\"modal-header bg-danger text-white\">\n            <h5 class=\"modal-title\"><i class=\"bi bi-exclamation-triangle me-2\"></i>删除确认</h5>\n            <button type=\"button\" class=\"btn-close btn-close-white\" data-bs-dismiss=\"modal\" aria-label=\"Close\"></button>\n          </div>\n          <div class=\"modal-body text-center py-4\">\n            <h4>确定要删除此模板吗？</h4>\n            <p class=\"mb-1\">模板名称: <strong>{{ templateToDelete?.name }}</strong></p>\n            <p class=\"mb-0 text-danger\">此操作无法撤销，请确认您的选择。</p>\n          </div>\n          <div class=\"modal-footer\">\n            <button type=\"button\" class=\"btn btn-secondary\" data-bs-dismiss=\"modal\">取消</button>\n            <button type=\"button\" class=\"btn btn-danger\" @click=\"executeDelete\" :disabled=\"deleting\">\n              <span v-if=\"deleting\" class=\"spinner-border spinner-border-sm me-2\" role=\"status\" aria-hidden=\"true\"></span>\n              {{ deleting ? '删除中...' : '确认删除' }}\n            </button>\n          </div>\n        </div>\n      </div>\n    </div>\n\n    <!-- 删除成功弹窗 -->\n    <div class=\"modal fade\" id=\"deleteSuccessModal\" tabindex=\"-1\" aria-hidden=\"true\">\n      <div class=\"modal-dialog modal-dialog-centered\">\n        <div class=\"modal-content\">\n          <div class=\"modal-header bg-success text-white\">\n            <h5 class=\"modal-title\"><i class=\"bi bi-check-circle me-2\"></i>操作成功</h5>\n            <button type=\"button\" class=\"btn-close btn-close-white\" data-bs-dismiss=\"modal\" aria-label=\"Close\"></button>\n          </div>\n          <div class=\"modal-body text-center py-4\">\n            <h4>模板已成功删除</h4>\n          </div>\n          <div class=\"modal-footer justify-content-center\">\n            <button type=\"button\" class=\"btn btn-success px-4\" data-bs-dismiss=\"modal\">确定</button>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport api from '@/api/admin';\nimport { Modal } from 'bootstrap';\n\nexport default {\n  name: 'ContractTemplateManagement',\n  data() {\n    return {\n      templates: [],\n      loading: true,\n      templateToDelete: null,\n      deleteConfirmModal: null,\n      deleteSuccessModal: null,\n      deleting: false,\n    };\n  },\n  computed: {\n    filteredTemplates() {\n      return this.templates.filter(template => template && template.id);\n    }\n  },\n  mounted() {\n    this.fetchTemplates();\n    this.deleteConfirmModal = new Modal(document.getElementById('deleteConfirmModal'));\n    this.deleteSuccessModal = new Modal(document.getElementById('deleteSuccessModal'));\n  },\n  methods: {\n    async fetchTemplates() {\n      this.loading = true;\n      try {\n        const response = await api.getContractTemplates();\n        console.log('API响应:', response); // 调试输出\n\n        if (response && response.data) {\n          // 检查并处理分页响应\n          const templatesData = Array.isArray(response.data.results) ? response.data.results :\n                                (Array.isArray(response.data) ? response.data : []);\n          this.templates = templatesData.filter(item => item !== null);\n        } else {\n          console.error('获取合同模板返回了不正确的数据格式:', response);\n          this.templates = [];\n        }\n      } catch (error) {\n        console.error('获取合同模板失败:', error);\n        this.showErrorModal('获取模板列表失败，请稍后重试。');\n        this.templates = [];\n      } finally {\n        this.loading = false;\n      }\n    },\n    async toggleStatus(template) {\n      const originalStatus = !template.is_active;\n      try {\n        await api.updateContractTemplate(template.id, { is_active: template.is_active });\n        // 可以在这里添加一个成功提示\n      } catch (error) {\n        console.error('更新模板状态失败:', error);\n        // 失败时恢复开关状态\n        template.is_active = originalStatus;\n        this.showErrorModal('更新模板状态失败，请稍后重试。');\n      }\n    },\n    confirmDelete(template) {\n      this.templateToDelete = template;\n      this.deleteConfirmModal.show();\n    },\n    async executeDelete() {\n      if (!this.templateToDelete) return;\n      \n      this.deleting = true;\n      try {\n        await api.deleteContractTemplate(this.templateToDelete.id);\n        this.deleteConfirmModal.hide();\n        await this.fetchTemplates();\n        this.deleteSuccessModal.show();\n      } catch (error) {\n        console.error('删除模板失败:', error);\n        this.deleteConfirmModal.hide();\n        this.showErrorModal('删除模板失败，请稍后重试。');\n      } finally {\n        this.deleting = false;\n        this.templateToDelete = null;\n      }\n    },\n    showErrorModal(message) {\n      // 创建临时的错误模态框元素\n      const modalDiv = document.createElement('div');\n      modalDiv.innerHTML = `\n        <div class=\"modal fade\" id=\"tempErrorModal\" tabindex=\"-1\" aria-hidden=\"true\">\n          <div class=\"modal-dialog modal-dialog-centered\">\n            <div class=\"modal-content\">\n              <div class=\"modal-header bg-danger text-white\">\n                <h5 class=\"modal-title\"><i class=\"bi bi-exclamation-triangle me-2\"></i>操作失败</h5>\n                <button type=\"button\" class=\"btn-close btn-close-white\" data-bs-dismiss=\"modal\" aria-label=\"Close\"></button>\n              </div>\n              <div class=\"modal-body text-center py-4\">\n                <p class=\"mb-0\">${message}</p>\n              </div>\n              <div class=\"modal-footer justify-content-center\">\n                <button type=\"button\" class=\"btn btn-secondary px-4\" data-bs-dismiss=\"modal\">确定</button>\n              </div>\n            </div>\n          </div>\n        </div>`;\n      document.body.appendChild(modalDiv);\n      \n      const errorModal = new Modal(document.getElementById('tempErrorModal'));\n      errorModal.show();\n      \n      // 模态框关闭后移除元素\n      document.getElementById('tempErrorModal').addEventListener('hidden.bs.modal', function () {\n        document.body.removeChild(modalDiv);\n      });\n    }\n  },\n};\n</script>\n\n<style scoped>\n.contract-template-management {\n  padding: 2rem;\n  background-color: #f8f9fa;\n}\n\n.page-title {\n  font-weight: 300;\n  color: #343a40;\n  margin-bottom: 1.5rem;\n}\n\n.actions {\n  margin-bottom: 1.5rem;\n}\n\n.loading-spinner {\n    display: flex;\n    justify-content: center;\n    align-items: center;\n    height: 200px;\n}\n\n.empty-state {\n    text-align: center;\n    padding: 3rem;\n    border: 2px dashed #dee2e6;\n    border-radius: .5rem;\n    background-color: #fff;\n}\n\n.empty-state i {\n    font-size: 3rem;\n    color: #6c757d;\n}\n\n.empty-state p {\n    font-size: 1.2rem;\n    font-weight: 500;\n    margin-top: 1rem;\n}\n\n.empty-state span {\n    color: #6c757d;\n}\n\n.table {\n    background-color: #fff;\n    border-radius: .5rem;\n    box-shadow: 0 0.125rem 0.25rem rgba(0,0,0,.075);\n}\n\n.table th {\n    font-weight: 500;\n}\n\n.badge {\n  font-size: 0.8rem;\n}\n\n.form-check.form-switch {\n  padding-left: 3.5em; /* 调整以适应标签 */\n}\n.form-check-input {\n  cursor: pointer;\n}\n</style> "],"mappings":";;AA0GA,OAAOA,GAAE,MAAO,aAAa;AAC7B,SAASC,KAAI,QAAS,WAAW;AAEjC,eAAe;EACbC,IAAI,EAAE,4BAA4B;EAClCC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE,EAAE;MACbC,OAAO,EAAE,IAAI;MACbC,gBAAgB,EAAE,IAAI;MACtBC,kBAAkB,EAAE,IAAI;MACxBC,kBAAkB,EAAE,IAAI;MACxBC,QAAQ,EAAE;IACZ,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,iBAAiBA,CAAA,EAAG;MAClB,OAAO,IAAI,CAACP,SAAS,CAACQ,MAAM,CAACC,QAAO,IAAKA,QAAO,IAAKA,QAAQ,CAACC,EAAE,CAAC;IACnE;EACF,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,cAAc,CAAC,CAAC;IACrB,IAAI,CAACT,kBAAiB,GAAI,IAAIN,KAAK,CAACgB,QAAQ,CAACC,cAAc,CAAC,oBAAoB,CAAC,CAAC;IAClF,IAAI,CAACV,kBAAiB,GAAI,IAAIP,KAAK,CAACgB,QAAQ,CAACC,cAAc,CAAC,oBAAoB,CAAC,CAAC;EACpF,CAAC;EACDC,OAAO,EAAE;IACP,MAAMH,cAAcA,CAAA,EAAG;MACrB,IAAI,CAACX,OAAM,GAAI,IAAI;MACnB,IAAI;QACF,MAAMe,QAAO,GAAI,MAAMpB,GAAG,CAACqB,oBAAoB,CAAC,CAAC;QACjDC,OAAO,CAACC,GAAG,CAAC,QAAQ,EAAEH,QAAQ,CAAC,EAAE;;QAEjC,IAAIA,QAAO,IAAKA,QAAQ,CAACjB,IAAI,EAAE;UAC7B;UACA,MAAMqB,aAAY,GAAIC,KAAK,CAACC,OAAO,CAACN,QAAQ,CAACjB,IAAI,CAACwB,OAAO,IAAIP,QAAQ,CAACjB,IAAI,CAACwB,OAAM,GAC1DF,KAAK,CAACC,OAAO,CAACN,QAAQ,CAACjB,IAAI,IAAIiB,QAAQ,CAACjB,IAAG,GAAI,EAAG;UACzE,IAAI,CAACC,SAAQ,GAAIoB,aAAa,CAACZ,MAAM,CAACgB,IAAG,IAAKA,IAAG,KAAM,IAAI,CAAC;QAC9D,OAAO;UACLN,OAAO,CAACO,KAAK,CAAC,oBAAoB,EAAET,QAAQ,CAAC;UAC7C,IAAI,CAAChB,SAAQ,GAAI,EAAE;QACrB;MACF,EAAE,OAAOyB,KAAK,EAAE;QACdP,OAAO,CAACO,KAAK,CAAC,WAAW,EAAEA,KAAK,CAAC;QACjC,IAAI,CAACC,cAAc,CAAC,iBAAiB,CAAC;QACtC,IAAI,CAAC1B,SAAQ,GAAI,EAAE;MACrB,UAAU;QACR,IAAI,CAACC,OAAM,GAAI,KAAK;MACtB;IACF,CAAC;IACD,MAAM0B,YAAYA,CAAClB,QAAQ,EAAE;MAC3B,MAAMmB,cAAa,GAAI,CAACnB,QAAQ,CAACoB,SAAS;MAC1C,IAAI;QACF,MAAMjC,GAAG,CAACkC,sBAAsB,CAACrB,QAAQ,CAACC,EAAE,EAAE;UAAEmB,SAAS,EAAEpB,QAAQ,CAACoB;QAAU,CAAC,CAAC;QAChF;MACF,EAAE,OAAOJ,KAAK,EAAE;QACdP,OAAO,CAACO,KAAK,CAAC,WAAW,EAAEA,KAAK,CAAC;QACjC;QACAhB,QAAQ,CAACoB,SAAQ,GAAID,cAAc;QACnC,IAAI,CAACF,cAAc,CAAC,iBAAiB,CAAC;MACxC;IACF,CAAC;IACDK,aAAaA,