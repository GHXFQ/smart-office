ntent bottom-content\"></div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { ref, onMounted, onBeforeUnmount, computed } from 'vue'\r\n\r\nexport default {\r\n  name: 'SimpleOffice3DAvatar',\r\n  props: {\r\n    size: {\r\n      type: Number,\r\n      default: 60\r\n    }\r\n  },\r\n  emits: ['click'],\r\n  setup(props) {\r\n    const container = ref(null)\r\n    const cube = ref(null)\r\n    let animationId = null\r\n    let isMouseOver = false\r\n    let rotationX = 0\r\n    let rotationY = 0\r\n    let autoRotateSpeed = 0.5\r\n    \r\n    // 计算立方体深度\r\n    const cubeDepth = computed(() => props.size * 0.4)\r\n    \r\n    // 自动旋转\r\n    const animate = () => {\r\n      if (!isMouseOver) {\r\n        rotationY += autoRotateSpeed\r\n        // 添加轻微上下浮动\r\n        rotationX = Math.sin(Date.now() * 0.001) * 5\r\n      }\r\n      \r\n      if (cube.value) {\r\n        cube.value.style.transform = \r\n          `rotateX(${rotationX}deg) rotateY(${rotationY}deg)`\r\n      }\r\n      \r\n      animationId = requestAnimationFrame(animate)\r\n    }\r\n    \r\n    // 鼠标交互\r\n    const handleMouseMove = (e) => {\r\n      if (!container.value) return\r\n      \r\n      const rect = container.value.getBoundingClientRect()\r\n      const centerX = rect.left + rect.width / 2\r\n      const centerY = rect.top + rect.height / 2\r\n      \r\n      // 计算鼠标位置相对于中心点的偏移\r\n      const mouseX = e.clientX - centerX\r\n      const mouseY = e.clientY - centerY\r\n      \r\n      // 将偏移转换为旋转角度\r\n      rotationX = -mouseY * 0.2\r\n      rotationY = mouseX * 0.2\r\n    }\r\n    \r\n    // 鼠标悬停处理\r\n    const handleMouseEnter = () => {\r\n      isMouseOver = true\r\n    }\r\n    \r\n    const handleMouseLeave = () => {\r\n      isMouseOver = false\r\n    }\r\n    \r\n    // 随机眨眼动画\r\n    const blinkEyes = () => {\r\n      if (document) {\r\n        const leftEye = document.querySelector('.left-eye')\r\n        const rightEye = document.querySelector('.right-eye')\r\n        \r\n        if (leftEye && rightEye) {\r\n          leftEye.classList.add('blink')\r\n          rightEye.classList.add('blink')\r\n          \r\n          setTimeout(() => {\r\n            leftEye.classList.remove('blink')\r\n            rightEye.classList.remove('blink')\r\n          }, 200)\r\n          \r\n          // 随机间隔眨眼\r\n          setTimeout(blinkEyes, 2000 + Math.random() * 3000)\r\n        }\r\n      }\r\n    }\r\n    \r\n    // 添加嘴巴动画\r\n    const animateMouth = () => {\r\n      if (document) {\r\n        const mouth = document.querySelector('.mouth')\r\n        if (mouth) {\r\n          mouth.classList.add('talk')\r\n          setTimeout(() => {\r\n            mouth.classList.remove('talk')\r\n          }, 300 + Math.random() * 200)\r\n          \r\n          setTimeout(animateMouth, 1000 + Math.random() * 2000)\r\n        }\r\n      }\r\n    }\r\n    \r\n    onMounted(() => {\r\n      // 设置初始旋转\r\n      if (cube.value) {\r\n        cube.value.style.transform = `rotateX(0deg) rotateY(0deg)`\r\n      }\r\n      \r\n      // 启动动画\r\n      setTimeout(() => {\r\n        animate()\r\n      }, 100)\r\n      \r\n      if (container.value) {\r\n        container.value.addEventListener('mousemove', handleMouseMove)\r\n        container.value.addEventListener('mouseenter', handleMouseEnter)\r\n        container.value.addEventListener('mouseleave', handleMouseLeave)\r\n      }\r\n      \r\n      // 启动眨眼动画\r\n      setTimeout(blinkEyes, 1000)\r\n      // 启动嘴巴动画\r\n      setTimeout(animateMouth, 2000)\r\n    })\r\n    \r\n    onBeforeUnmount(() => {\r\n      if (animationId) {\r\n        cancelAnimationFrame(animationId)\r\n      }\r\n      \r\n      if (container.value) {\r\n        container.value.removeEventListener('mousemove', handleMouseMove)\r\n        container.value.removeEventListener('mouseenter', handleMouseEnter)\r\n        container.value.removeEventListener('mouseleave', handleMouseLeave)\r\n      }\r\n    })\r\n    \r\n    return {\r\n      container,\r\n      cube,\r\n      cubeDepth\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.avatar-container {\r\n  position: relative;\r\n  width: v-bind('size + \"px\"');\r\n  height: v-bind('size + \"px\"');\r\n  perspective: 600px;\r\n  cursor: pointer;\r\n  border-radius: 0;\r\n  overflow: visible;\r\n  transition: transform 0.3s;\r\n  z-index: 10;\r\n  \r\n  &:hover {\r\n    transform: scale(1.1);\r\n  }\r\n}\r\n\r\n.scene {\r\n  width: 100%;\r\n  height: 100%;\r\n  position: relative;\r\n  transform-style: preserve-3d;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n}\r\n\r\n.cube {\r\n  width: 80%;\r\n  height: 80%;\r\n  position: relative;\r\n  transform-style: preserve-3d;\r\n  transition: transform 0.1s ease;\r\n  transform: rotateX(0deg) rotateY(0deg);\r\n}\r\n\r\n.cube-face {\r\n  position: absolute;\r\n  width: 100%;\r\n  height: 100%;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  border-radius: 10px;\r\n  backface-visibility: hidden;\r\n  top: 0;\r\n  left: 0;\r\n}\r\n\r\n.face-content {\r\n  width: 100%;\r\n  height: 100%;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  border-radius: 10px;\r\n}\r\n\r\n.cube-face-front {\r\n  transform: translateZ(v-bind('cubeDepth + \"px\"'));\r\n  background: #5B8FF9;\r\n}\r\n\r\n.cube-face-back {\r\n  transform: rotateY(180deg) translateZ(v-bind('cubeDepth + \"px\"'));\r\n  background: #2F54EB;\r\n}\r\n\r\n.cube-face-right {\r\n  transform: rotateY(90deg) translateZ(v-bind('cubeDepth + \"px\"'));\r\n  background: #1890FF;\r\n}\r\n\r\n.cube-face-left {\r\n  transform: rotateY(-90deg) translateZ(v-bind('cubeDepth + \"px\"'));\r\n  background: #36CFC9;\r\n}\r\n\r\n.cube-face-top {\r\n  transform: rotateX(90deg) translateZ(v-bind('cubeDepth + \"px\"'));\r\n  background: #73D13D;\r\n}\r\n\r\n.cube-face-bottom {\r\n  transform: rotateX(-90deg) translateZ(v-bind('cubeDepth + \"px\"'));\r\n  background: #FAAD14;\r\n}\r\n\r\n.avatar-face {\r\n  width: 80%;\r\n  height: 80%;\r\n  position: relative;\r\n}\r\n\r\n.eyes {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  width: 70%;\r\n  margin: 30% auto 0;\r\n}\r\n\r\n.eye {\r\n  width: 20%;\r\n  height: 20%;\r\n  background: white;\r\n  border-radius: 50%;\r\n  position: relative;\r\n  \r\n  &::after {\r\n    content: '';\r\n    position: absolute;\r\n    width: 50%;\r\n    height: 50%;\r\n    background: black;\r\n    border-radius: 50%;\r\n    top: 25%;\r\n    left: 25%;\r\n  }\r\n  \r\n  &.blink {\r\n    height: 2px;\r\n    margin-top: 10%;\r\n    \r\n    &::after {\r\n      opacity: 0;\r\n    }\r\n  }\r\n}\r\n\r\n.mouth {\r\n  width: 40%;\r\n  height: 15%;\r\n  background: #FF5500;\r\n  border-radius: 0 0 100px 100px;\r\n  margin: 15% auto 0;\r\n  position: relative;\r\n  transition: height 0.2s, width 0.2s;\r\n  \r\n  &.talk {\r\n    height: 20%;\r\n    width: 30%;\r\n    border-radius: 50%;\r\n  }\r\n}\r\n\r\n.side-content {\r\n  background: linear-gradient(to bottom, rgba(255,255,255,0.3), rgba(255,255,255,0));\r\n}\r\n\r\n.back-content {\r\n  background: linear-gradient(to bottom, rgba(0,0,0,0.2), rgba(0,0,0,0));\r\n}\r\n\r\n.top-content {\r\n  background: radial-gradient(circle, rgba(255,255,255,0.5), rgba(255,255,255,0));\r\n}\r\n\r\n.bottom-content {\r\n  background: radial-gradient(circle, rgba(0,0,0,0.2), rgba(0,0,0,0));\r\n}\r\n</style> "],"mappings":"AAoCA,SAASA,GAAG,EAAEC,SAAS,EAAEC,eAAe,EAAEC,QAAO,QAAS,KAAI;AAE9D,MAAKC,WAAU;EACbC,IAAI,EAAE,sBAAsB;EAC5BC,KAAK,EAAE;IACLC,IAAI,EAAE;MACJC,IAAI,EAAEC,MAAM;MACZC,OAAO,EAAE;IACX;EACF,CAAC;EACDC,KAAK,EAAE,CAAC,OAAO,CAAC;EAChBC,KAAKA,CAACN,KAAK,EAAE;IACX,MAAMO,SAAQ,GAAIb,GAAG,CAAC,IAAI;IAC1B,MAAMc,IAAG,GAAId,GAAG,CAAC,IAAI;IACrB,IAAIe,WAAU,GAAI,IAAG;IACrB,IAAIC,WAAU,GAAI,KAAI;IACtB,IAAIC,SAAQ,GAAI;IAChB,IAAIC,SAAQ,GAAI;IAChB,IAAIC,eAAc,GAAI,GAAE;;IAExB;IACA,MAAMC,SAAQ,GAAIjB,QAAQ,CAAC,MAAMG,KAAK,CAACC,IAAG,GAAI,GA