{"remainingRequest":"C:\\Users\\18083\\Desktop\\CCIT\\office\\smart-office\\frontend\\node_modules\\babel-loader\\lib\\index.js!C:\\Users\\18083\\Desktop\\CCIT\\office\\smart-office\\frontend\\node_modules\\cache-loader\\dist\\cjs.js??ref--1-0!C:\\Users\\18083\\Desktop\\CCIT\\office\\smart-office\\frontend\\node_modules\\vue-loader-v16\\dist\\index.js??ref--1-1!C:\\Users\\18083\\Desktop\\CCIT\\office\\smart-office\\frontend\\src\\components\\ai\\OpenWebUI.vue?vue&type=script&lang=js","dependencies":[{"path":"C:\\Users\\18083\\Desktop\\CCIT\\office\\smart-office\\frontend\\src\\components\\ai\\OpenWebUI.vue","mtime":1748693702434},{"path":"C:\\Users\\18083\\Desktop\\CCIT\\office\\smart-office\\frontend\\babel.config.js","mtime":1748077811200},{"path":"C:\\Users\\18083\\Desktop\\CCIT\\office\\smart-office\\frontend\\node_modules\\cache-loader\\dist\\cjs.js","mtime":1746431218000},{"path":"C:\\Users\\18083\\Desktop\\CCIT\\office\\smart-office\\frontend\\node_modules\\babel-loader\\lib\\index.js","mtime":1746431202000},{"path":"C:\\Users\\18083\\Desktop\\CCIT\\office\\smart-office\\frontend\\node_modules\\cache-loader\\dist\\cjs.js","mtime":1746431218000},{"path":"C:\\Users\\18083\\Desktop\\CCIT\\office\\smart-office\\frontend\\node_modules\\vue-loader-v16\\dist\\index.js","mtime":1746431218000}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"names":["ref","reactive","onMounted","nextTick","computed","marked","hljs","name","setup","inputMessage","messages","loading","messagesContainer","inputField","settingsVisible","settings","model","temperature","showTimestamp","codeHighlight","suggestions","sendMessage","text","messageContent","value","trim","push","role","content","time","Date","scrollToBottom","setTimeout","response","generateResponse","then","error","console","message","includes","clearChat","toggleSettings","formatMessage","replace","setOptions","highlight","code","lang","getLanguage","language","highlightAuto","breaks","formatTime","date","toLocaleTimeString","hour","minute","scrollTop","scrollHeight","focus"],"sources":["C:\\Users\\18083\\Desktop\\CCIT\\office\\smart-office\\frontend\\src\\components\\ai\\OpenWebUI.vue"],"sourcesContent":["<template>\r\n  <div class=\"open-webui-container\">\r\n    <div class=\"chat-window\">\r\n      <div class=\"chat-header\">\r\n        <div class=\"model-info\">\r\n          <i class=\"bi bi-cpu\"></i>\r\n          <span>智能助手</span>\r\n        </div>\r\n        <div class=\"header-actions\">\r\n          <el-button type=\"text\" size=\"small\" @click=\"clearChat\">\r\n            <i class=\"bi bi-trash\"></i>\r\n          </el-button>\r\n          <el-button type=\"text\" size=\"small\" @click=\"toggleSettings\">\r\n            <i class=\"bi bi-gear\"></i>\r\n          </el-button>\r\n        </div>\r\n      </div>\r\n      \r\n      <div class=\"chat-messages\" ref=\"messagesContainer\">\r\n        <div v-if=\"messages.length === 0\" class=\"welcome-message\">\r\n          <div class=\"welcome-header\">\r\n            <i class=\"bi bi-robot\"></i>\r\n            <h2>智能办公助手</h2>\r\n          </div>\r\n          <p>我可以帮助您:</p>\r\n          <div class=\"suggestion-chips\">\r\n            <div v-for=\"(suggestion, index) in suggestions\" :key=\"index\" \r\n                 class=\"suggestion-chip\" @click=\"sendMessage(suggestion)\">\r\n              {{ suggestion }}\r\n            </div>\r\n          </div>\r\n        </div>\r\n        \r\n        <template v-else>\r\n          <div v-for=\"(message, index) in messages\" :key=\"index\" \r\n               :class=\"['message', message.role === 'user' ? 'user-message' : 'assistant-message']\">\r\n            <div class=\"message-avatar\">\r\n              <i :class=\"message.role === 'user' ? 'bi bi-person-circle' : 'bi bi-robot'\"></i>\r\n            </div>\r\n            <div class=\"message-content\">\r\n              <div class=\"message-header\">\r\n                <span class=\"message-sender\">{{ message.role === 'user' ? '您' : '智能助手' }}</span>\r\n                <span class=\"message-time\">{{ formatTime(message.time) }}</span>\r\n              </div>\r\n              <div class=\"message-text\" v-html=\"formatMessage(message.content)\"></div>\r\n            </div>\r\n          </div>\r\n          <div v-if=\"loading\" class=\"message assistant-message\">\r\n            <div class=\"message-avatar\">\r\n              <i class=\"bi bi-robot\"></i>\r\n            </div>\r\n            <div class=\"message-content\">\r\n              <div class=\"message-header\">\r\n                <span class=\"message-sender\">智能助手</span>\r\n              </div>\r\n              <div class=\"typing-indicator\">\r\n                <span></span><span></span><span></span>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </template>\r\n      </div>\r\n      \r\n      <div class=\"chat-input\">\r\n        <el-input\r\n          v-model=\"inputMessage\"\r\n          type=\"textarea\"\r\n          :rows=\"1\"\r\n          placeholder=\"输入您的问题...\"\r\n          resize=\"none\"\r\n          @keydown.enter.prevent=\"sendMessage\"\r\n          ref=\"inputField\"\r\n          :disabled=\"loading\"\r\n        />\r\n        <div class=\"input-actions\">\r\n          <el-button type=\"primary\" icon=\"el-icon-s-promotion\" :loading=\"loading\" @click=\"sendMessage\">\r\n            <i class=\"bi bi-send\"></i>\r\n          </el-button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    \r\n    <el-drawer\r\n      title=\"设置\"\r\n      v-model=\"settingsVisible\"\r\n      direction=\"rtl\"\r\n      size=\"300px\"\r\n    >\r\n      <div class=\"settings-content\">\r\n        <div class=\"setting-section\">\r\n          <h3>模型设置</h3>\r\n          <el-form label-position=\"top\">\r\n            <el-form-item label=\"AI模型\">\r\n              <el-select v-model=\"settings.model\" placeholder=\"选择模型\">\r\n                <el-option label=\"Claude-4-Sonnet\" value=\"claude-4-sonnet\" />\r\n                <el-option label=\"GPT-4\" value=\"gpt-4\" />\r\n                <el-option label=\"GPT-3.5 Turbo\" value=\"gpt-3.5-turbo\" />\r\n              </el-select>\r\n            </el-form-item>\r\n            <el-form-item label=\"温度\">\r\n              <el-slider v-model=\"settings.temperature\" :min=\"0\" :max=\"1\" :step=\"0.1\" show-stops />\r\n            </el-form-item>\r\n          </el-form>\r\n        </div>\r\n        \r\n        <div class=\"setting-section\">\r\n          <h3>界面设置</h3>\r\n          <el-form label-position=\"top\">\r\n            <el-form-item label=\"显示时间戳\">\r\n              <el-switch v-model=\"settings.showTimestamp\" />\r\n            </el-form-item>\r\n            <el-form-item label=\"显示代码高亮\">\r\n              <el-switch v-model=\"settings.codeHighlight\" />\r\n            </el-form-item>\r\n          </el-form>\r\n        </div>\r\n      </div>\r\n    </el-drawer>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { ref, reactive, onMounted, nextTick, computed } from 'vue'\r\nimport { marked } from 'marked'\r\nimport hljs from 'highlight.js'\r\n\r\nexport default {\r\n  name: 'OpenWebUI',\r\n  setup() {\r\n    const inputMessage = ref('')\r\n    const messages = ref([])\r\n    const loading = ref(false)\r\n    const messagesContainer = ref(null)\r\n    const inputField = ref(null)\r\n    const settingsVisible = ref(false)\r\n    \r\n    // 设置\r\n    const settings = reactive({\r\n      model: 'claude-4-sonnet',\r\n      temperature: 0.7,\r\n      showTimestamp: true,\r\n      codeHighlight: true\r\n    })\r\n    \r\n    // 建议问题\r\n    const suggestions = [\r\n      '如何创建一个新项目?',\r\n      '帮我写一封请假邮件',\r\n      '如何使用文档管理功能?',\r\n      '为下周的会议生成议程'\r\n    ]\r\n    \r\n    // 发送消息\r\n    const sendMessage = async (text = null) => {\r\n      const messageContent = text || inputMessage.value.trim()\r\n      if (!messageContent || loading.value) return\r\n      \r\n      // 添加用户消息到聊天\r\n      messages.value.push({\r\n        role: 'user',\r\n        content: messageContent,\r\n        time: new Date()\r\n      })\r\n      \r\n      // 清空输入框\r\n      inputMessage.value = ''\r\n      \r\n      // 滚动到底部\r\n      await nextTick()\r\n      scrollToBottom()\r\n      \r\n      // 设置加载状态\r\n      loading.value = true\r\n      \r\n      try {\r\n        // 这里实际会调用后端API，为演示简单模拟响应\r\n        setTimeout(() => {\r\n          // 模拟AI响应\r\n          const response = generateResponse(messageContent)\r\n          \r\n          // 添加AI响应到聊天\r\n          messages.value.push({\r\n            role: 'assistant',\r\n            content: response,\r\n            time: new Date()\r\n          })\r\n          \r\n          // 滚动到底部\r\n          nextTick().then(scrollToBottom)\r\n          \r\n          // 结束加载状态\r\n          loading.value = false\r\n        }, 1000)\r\n      } catch (error) {\r\n        console.error('消息发送失败:', error)\r\n        loading.value = false\r\n      }\r\n    }\r\n    \r\n    // 生成响应（模拟）\r\n    const generateResponse = (message) => {\r\n      // 简单的模拟响应逻辑\r\n      if (message.includes('项目') || message.includes('创建')) {\r\n        return '要创建新项目，请点击导航栏中的\"项目\"菜单，然后选择\"新建项目\"。填写项目名称、描述和成员后即可创建。\\n\\n```javascript\\n// 项目创建示例代码\\nconst project = {\\n  name: \"新项目\",\\n  description: \"项目描述\",\\n  members: [\"用户1\", \"用户2\"]\\n};\\n```'\r\n      } else if (message.includes('邮件') || message.includes('请假')) {\r\n        return '以下是一个请假邮件模板：\\n\\n**主题**：请假申请\\n\\n尊敬的[主管姓名]：\\n\\n我需要请假[天数]，从[开始日期]至[结束日期]。请假原因是[原因]。\\n\\n我已经安排好了工作交接，[同事姓名]将负责我的工作。\\n\\n谢谢您的理解。\\n\\n此致,\\n[您的姓名]'\r\n      } else if (message.includes('文档') || message.includes('管理')) {\r\n        return '使用文档管理功能的步骤：\\n\\n1. 点击左侧菜单的\"文档管理\"\\n2. 选择目标文件夹或创建新文件夹\\n3. 上传文档或创建新文档\\n4. 设置权限和标签\\n\\n您还可以使用搜索功能快速找到需要的文档。'\r\n      } else if (message.includes('会议') || message.includes('议程')) {\r\n        return '以下是会议议程模板：\\n\\n# [会议名称] 议程\\n## 日期：[日期]\\n## 时间：[开始时间]-[结束时间]\\n## 地点：[地点]\\n\\n### 议程项目：\\n1. 开场白和介绍（5分钟）\\n2. 上次会议记录回顾（10分钟）\\n3. [议题1]（15分钟）\\n4. [议题2]（15分钟）\\n5. 问答环节（10分钟）\\n6. 下一步行动计划（5分钟）\\n\\n### 与会者：\\n- [人员1]\\n- [人员2]\\n- [人员3]'\r\n      } else {\r\n        return '我是您的智能办公助手，可以帮助您解决工作中的各种问题。请告诉我您需要什么帮助？'\r\n      }\r\n    }\r\n    \r\n    // 清空聊天记录\r\n    const clearChat = () => {\r\n      messages.value = []\r\n    }\r\n    \r\n    // 切换设置面板\r\n    const toggleSettings = () => {\r\n      settingsVisible.value = !settingsVisible.value\r\n    }\r\n    \r\n    // 格式化消息（支持Markdown和代码高亮）\r\n    const formatMessage = (text) => {\r\n      if (!settings.codeHighlight) return text.replace(/\\n/g, '<br>')\r\n      \r\n      // 配置marked使用highlight.js进行代码高亮\r\n      marked.setOptions({\r\n        highlight: function(code, lang) {\r\n          if (lang && hljs.getLanguage(lang)) {\r\n            return hljs.highlight(code, { language: lang }).value\r\n          }\r\n          return hljs.highlightAuto(code).value\r\n        },\r\n        breaks: true\r\n      })\r\n      \r\n      return marked(text)\r\n    }\r\n    \r\n    // 格式化时间\r\n    const formatTime = (date) => {\r\n      if (!settings.showTimestamp) return ''\r\n      \r\n      return new Date(date).toLocaleTimeString([], {\r\n        hour: '2-digit',\r\n        minute: '2-digit'\r\n      })\r\n    }\r\n    \r\n    // 滚动到底部\r\n    const scrollToBottom = () => {\r\n      if (messagesContainer.value) {\r\n        messagesContainer.value.scrollTop = messagesContainer.value.scrollHeight\r\n      }\r\n    }\r\n    \r\n    // 组件挂载后设置焦点\r\n    onMounted(() => {\r\n      if (inputField.value) {\r\n        inputField.value.focus()\r\n      }\r\n    })\r\n    \r\n    return {\r\n      inputMessage,\r\n      messages,\r\n      loading,\r\n      suggestions,\r\n      settings,\r\n      settingsVisible,\r\n      messagesContainer,\r\n      inputField,\r\n      sendMessage,\r\n      clearChat,\r\n      toggleSettings,\r\n      formatMessage,\r\n      formatTime,\r\n      scrollToBottom\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.open-webui-container {\r\n  width: 100%;\r\n  height: 100%;\r\n  display: flex;\r\n  flex-direction: column;\r\n  background-color: var(--bg-color, #f5f7fa);\r\n}\r\n\r\n.chat-window {\r\n  display: flex;\r\n  flex-direction: column;\r\n  width: 100%;\r\n  height: 100%;\r\n  overflow: hidden;\r\n  border-radius: 8px;\r\n  background-color: var(--bg-color-secondary, #ffffff);\r\n  box-shadow: 0 2px 12px var(--shadow-color, rgba(0, 0, 0, 0.1));\r\n}\r\n\r\n.chat-header {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n  padding: 16px 20px;\r\n  border-bottom: 1px solid var(--border-color, #ebeef5);\r\n  background-color: var(--bg-color-secondary, #ffffff);\r\n}\r\n\r\n.model-info {\r\n  display: flex;\r\n  align-items: center;\r\n  gap: 8px;\r\n  \r\n  i {\r\n    font-size: 20px;\r\n    color: var(--primary-color, #3b82f6);\r\n  }\r\n  \r\n  span {\r\n    font-weight: 600;\r\n    font-size: 16px;\r\n  }\r\n}\r\n\r\n.header-actions {\r\n  display: flex;\r\n  gap: 10px;\r\n  \r\n  .el-button {\r\n    padding: 6px;\r\n    \r\n    i {\r\n      font-size: 18px;\r\n    }\r\n  }\r\n}\r\n\r\n.chat-messages {\r\n  flex: 1;\r\n  overflow-y: auto;\r\n  padding: 20px;\r\n  display: flex;\r\n  flex-direction: column;\r\n  gap: 16px;\r\n  background-color: var(--bg-color, #f5f7fa);\r\n}\r\n\r\n.welcome-message {\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n  text-align: center;\r\n  margin: 60px auto;\r\n  max-width: 600px;\r\n  \r\n  .welcome-header {\r\n    display: flex;\r\n    align-items: center;\r\n    gap: 10px;\r\n    margin-bottom: 20px;\r\n    \r\n    i {\r\n      font-size: 36px;\r\n      color: var(--primary-color, #3b82f6);\r\n    }\r\n    \r\n    h2 {\r\n      font-size: 28px;\r\n      font-weight: 600;\r\n      margin: 0;\r\n    }\r\n  }\r\n  \r\n  p {\r\n    font-size: 16px;\r\n    color: var(--text-color-secondary, #666);\r\n    margin-bottom: 20px;\r\n  