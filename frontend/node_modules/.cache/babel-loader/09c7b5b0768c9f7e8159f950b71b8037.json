{"remainingRequest":"C:\\Users\\18083\\Desktop\\offic_ai\\office\\smart-office\\frontend\\node_modules\\babel-loader\\lib\\index.js!C:\\Users\\18083\\Desktop\\offic_ai\\office\\smart-office\\frontend\\node_modules\\cache-loader\\dist\\cjs.js??ref--1-0!C:\\Users\\18083\\Desktop\\offic_ai\\office\\smart-office\\frontend\\node_modules\\vue-loader-v16\\dist\\index.js??ref--1-1!C:\\Users\\18083\\Desktop\\offic_ai\\office\\smart-office\\frontend\\src\\views\\project\\Index.vue?vue&type=script&lang=js","dependencies":[{"path":"C:\\Users\\18083\\Desktop\\offic_ai\\office\\smart-office\\frontend\\src\\views\\project\\Index.vue","mtime":1746443091281},{"path":"C:\\Users\\18083\\Desktop\\offic_ai\\office\\smart-office\\frontend\\babel.config.js","mtime":1746431728272},{"path":"C:\\Users\\18083\\Desktop\\offic_ai\\office\\smart-office\\frontend\\node_modules\\cache-loader\\dist\\cjs.js","mtime":1746431218359},{"path":"C:\\Users\\18083\\Desktop\\offic_ai\\office\\smart-office\\frontend\\node_modules\\babel-loader\\lib\\index.js","mtime":1746431202324},{"path":"C:\\Users\\18083\\Desktop\\offic_ai\\office\\smart-office\\frontend\\node_modules\\cache-loader\\dist\\cjs.js","mtime":1746431218359},{"path":"C:\\Users\\18083\\Desktop\\offic_ai\\office\\smart-office\\frontend\\node_modules\\vue-loader-v16\\dist\\index.js","mtime":1746431218786}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"names":["ref","Operation","Plus","Calendar","User","Setting","ElMessage","name","setup","activeProject","activeTab","taskDetailVisible","currentTask","projects","tasks","date","manager","status","kanbanColumns","color","title","description","tags","type","assignees","initial","getStatusType","typeMap","getProgressColor","percentage","getTotalTasks","value","reduce","total","column","length","getCompletedTasks","completedColumn","find","col","getInProgressTasks","inProgressColumn","getCompletePercentage","completed","Math","round","createProject","success","addTask","columnIndex","openTaskDetail","task"],"sources":["C:\\Users\\18083\\Desktop\\offic_ai\\office\\smart-office\\frontend\\src\\views\\project\\Index.vue"],"sourcesContent":["<template>\r\n  <div class=\"project-container\">\r\n    <div class=\"row\">\r\n      <!-- 左侧项目导航 -->\r\n      <div class=\"col-md-3\">\r\n        <el-card class=\"project-nav-card\">\r\n          <template #header>\r\n            <div class=\"card-header-flex\">\r\n              <span>我的项目</span>\r\n              <el-button type=\"primary\" size=\"small\" @click=\"createProject\">\r\n                <el-icon><Plus /></el-icon> 新建\r\n              </el-button>\r\n            </div>\r\n          </template>\r\n          \r\n          <div class=\"project-nav\">\r\n            <div \r\n              v-for=\"(project, index) in projects\" \r\n              :key=\"index\"\r\n              class=\"project-item\"\r\n              :class=\"{'active': activeProject === index}\"\r\n              @click=\"activeProject = index\"\r\n            >\r\n              <el-icon><Operation /></el-icon>\r\n              <span class=\"project-name\">{{ project.name }}</span>\r\n              <el-badge :value=\"project.tasks\" class=\"project-badge\" />\r\n            </div>\r\n          </div>\r\n        </el-card>\r\n      </div>\r\n      \r\n      <!-- 右侧项目内容 -->\r\n      <div class=\"col-md-9\">\r\n        <el-card v-if=\"activeProject !== null\">\r\n          <template #header>\r\n            <div class=\"card-header-flex\">\r\n              <div class=\"project-header-title\">\r\n                <h5>{{ projects[activeProject].name }}</h5>\r\n                <div class=\"project-meta\">\r\n                  <span><el-icon><Calendar /></el-icon> {{ projects[activeProject].date }}</span>\r\n                  <span><el-icon><User /></el-icon> {{ projects[activeProject].manager }}</span>\r\n                  <el-tag size=\"small\" :type=\"getStatusType(projects[activeProject].status)\">\r\n                    {{ projects[activeProject].status }}\r\n                  </el-tag>\r\n                </div>\r\n              </div>\r\n              <div class=\"project-actions\">\r\n                <el-button-group>\r\n                  <el-button size=\"small\">\r\n                    <el-icon><User /></el-icon> 成员\r\n                  </el-button>\r\n                  <el-button size=\"small\">\r\n                    <el-icon><Setting /></el-icon> 设置\r\n                  </el-button>\r\n                </el-button-group>\r\n              </div>\r\n            </div>\r\n          </template>\r\n          \r\n          <!-- 项目内容选项卡 -->\r\n          <el-tabs v-model=\"activeTab\" class=\"project-tabs\">\r\n            <el-tab-pane label=\"看板\" name=\"kanban\">\r\n              <div class=\"kanban-container\">\r\n                <div \r\n                  v-for=\"(column, colIndex) in kanbanColumns\" \r\n                  :key=\"colIndex\"\r\n                  class=\"kanban-column\"\r\n                >\r\n                  <div class=\"kanban-column-header\" :style=\"{backgroundColor: column.color}\">\r\n                    <span>{{ column.name }}</span>\r\n                    <span class=\"column-count\">{{ column.tasks.length }}</span>\r\n                  </div>\r\n                  <div class=\"kanban-column-content\">\r\n                    <div \r\n                      v-for=\"(task, taskIndex) in column.tasks\" \r\n                      :key=\"taskIndex\"\r\n                      class=\"kanban-item\"\r\n                      @click=\"openTaskDetail(task)\"\r\n                    >\r\n                      <div class=\"kanban-item-title\">{{ task.title }}</div>\r\n                      <div class=\"kanban-item-desc\">{{ task.description }}</div>\r\n                      <div class=\"kanban-item-footer\">\r\n                        <div class=\"kanban-item-tags\">\r\n                          <el-tag \r\n                            v-for=\"(tag, tagIndex) in task.tags\" \r\n                            :key=\"tagIndex\"\r\n                            size=\"small\"\r\n                            :type=\"tag.type\"\r\n                          >\r\n                            {{ tag.name }}\r\n                          </el-tag>\r\n                        </div>\r\n                        <div class=\"kanban-item-members\">\r\n                          <div \r\n                            v-for=\"(member, memberIndex) in task.assignees\" \r\n                            :key=\"memberIndex\"\r\n                            class=\"kanban-member\"\r\n                            :style=\"{backgroundColor: member.color}\"\r\n                            :title=\"member.name\"\r\n                          >\r\n                            {{ member.initial }}\r\n                          </div>\r\n                        </div>\r\n                      </div>\r\n                    </div>\r\n                    \r\n                    <div class=\"add-task\" @click=\"addTask(colIndex)\">\r\n                      <el-icon><Plus /></el-icon> 添加任务\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </el-tab-pane>\r\n            \r\n            <el-tab-pane label=\"统计\" name=\"stats\">\r\n              <div class=\"stats-container\">\r\n                <div class=\"row\">\r\n                  <div class=\"col-md-6\">\r\n                    <el-card class=\"stats-card\">\r\n                      <template #header>\r\n                        <div>任务完成情况</div>\r\n                      </template>\r\n                      <div class=\"stats-content\">\r\n                        <el-progress \r\n                          type=\"dashboard\" \r\n                          :percentage=\"getCompletePercentage()\" \r\n                          :color=\"getProgressColor\"\r\n                        />\r\n                        <div class=\"stats-summary\">\r\n                          <div>总任务: {{ getTotalTasks() }}</div>\r\n                          <div>已完成: {{ getCompletedTasks() }}</div>\r\n                          <div>进行中: {{ getInProgressTasks() }}</div>\r\n                        </div>\r\n                      </div>\r\n                    </el-card>\r\n                  </div>\r\n                  <div class=\"col-md-6\">\r\n                    <el-card class=\"stats-card\">\r\n                      <template #header>\r\n                        <div>任务分布</div>\r\n                      </template>\r\n                      <div class=\"task-distribution\">\r\n                        <div \r\n                          v-for=\"(column, index) in kanbanColumns\" \r\n                          :key=\"index\"\r\n                          class=\"distribution-item\"\r\n                        >\r\n                          <div class=\"distribution-label\">\r\n                            <div class=\"color-dot\" :style=\"{backgroundColor: column.color}\"></div>\r\n                            <span>{{ column.name }}</span>\r\n                          </div>\r\n                          <div class=\"distribution-value\">\r\n                            <div class=\"distribution-bar\">\r\n                              <div \r\n                                class=\"distribution-progress\" \r\n                                :style=\"{\r\n                                  width: (column.tasks.length / getTotalTasks() * 100) + '%',\r\n                                  backgroundColor: column.color\r\n                                }\"\r\n                              ></div>\r\n                            </div>\r\n                            <span>{{ column.tasks.length }}</span>\r\n                          </div>\r\n                        </div>\r\n                      </div>\r\n                    </el-card>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </el-tab-pane>\r\n          </el-tabs>\r\n        </el-card>\r\n        \r\n        <el-empty \r\n          v-else \r\n          description=\"请选择一个项目\" \r\n          class=\"project-empty\"\r\n        ></el-empty>\r\n      </div>\r\n    </div>\r\n    \r\n    <!-- 任务详情对话框 -->\r\n    <el-dialog \r\n      v-model=\"taskDetailVisible\" \r\n      title=\"任务详情\" \r\n      width=\"600px\"\r\n    >\r\n      <div v-if=\"currentTask\" class=\"task-detail\">\r\n        <h5 class=\"task-detail-title\">{{ currentTask.title }}</h5>\r\n        <div class=\"task-detail-meta\">\r\n          <el-tag \r\n            v-for=\"(tag, index) in currentTask.tags\" \r\n            :key=\"index\"\r\n            :type=\"tag.type\"\r\n            class=\"mr-5\"\r\n          >\r\n            {{ tag.name }}\r\n          </el-tag>\r\n        </div>\r\n        <div class=\"task-detail-section\">\r\n          <div class=\"section-title\">描述</div>\r\n          <div class=\"section-content\">{{ currentTask.description }}</div>\r\n        </div>\r\n        <div class=\"task-detail-section\">\r\n          <div class=\"section-title\">负责人</div>\r\n          <div class=\"section-content assignees\">\r\n            <div \r\n              v-for=\"(member, index) in currentTask.assignees\" \r\n              :key=\"index\"\r\n              class=\"assignee-item\"\r\n            >\r\n              <div \r\n                class=\"kanban-member\"\r\n                :style=\"{backgroundColor: member.color}\"\r\n              >\r\n                {{ member.initial }}\r\n              </div>\r\n              <span>{{ member.name }}</span>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { ref } from 'vue'\r\nimport { \r\n  Operation, Plus, Calendar, User, Setting\r\n} from '@element-plus/icons-vue'\r\nimport { ElMessage } from 'element-plus'\r\n\r\nexport default {\r\n  name: 'Project',\r\n  setup() {\r\n    const activeProject = ref(0)\r\n    const activeTab = ref('kanban')\r\n    const taskDetailVisible = ref(false)\r\n    const currentTask = ref(null)\r\n    \r\n    // 项目列表\r\n    const projects = ref([\r\n      {\r\n        name: '智能办公系统开发',\r\n        tasks: 12,\r\n        date: '2023/05/01 - 2023/07/31',\r\n        manager: '张经理',\r\n        status: '进行中'\r\n      },\r\n      {\r\n        name: '市场推广方案',\r\n        tasks: 8,\r\n        date: '2023/05/15 - 2023/06/30',\r\n        manager: '李经理',\r\n        status: '计划中'\r\n      },\r\n      {\r\n        name: '客户管理系统升级',\r\n        tasks: 6,\r\n        date: '2023/04/10 - 2023/05/20',\r\n        manager: '王工程师',\r\n        status: '已完成'\r\n      },\r\n      {\r\n        name: '年度财务报表',\r\n        tasks: 5,\r\n        date: '2023/06/01 - 2023/06/15',\r\n        manager: '赵财务',\r\n        status: '待启动'\r\n      }\r\n    ])\r\n    \r\n    // 看板列\r\n    const kanbanColumns = ref([\r\n      {\r\n        name: '待处理',\r\n        color: '#909399',\r\n        tasks: [\r\n          {\r\n            title: '需求分析文档编写',\r\n            description: '编写系统需求分析文档，包括功能需求和非功能需求',\r\n            tags: [\r\n              { name: '文档', type: 'info' },\r\n              { name: '高优先级', type: 'danger' }\r\n            ],\r\n            assignees: [\r\n              { name: '张经理', initial: '张', color: '#409EFF' }\r\n            ]\r\n          },\r\n          {\r\n            title: '数据库设计',\r\n            description: '设计系统数据库架构，包括表结构和关系',\r\n            tags: [\r\n              { name: '设计', type: 'primary' },\r\n              { name: '中优先级', type: 'warning' }\r\n            ],\r\n            assignees: [\r\n              { name: '王工程师', initial: '王', color: '#67C23A' }\r\n            ]\r\n          }\r\n        ]\r\n      },\r\n      {\r\n        name: '进行中',\r\n        color: '#E6A23C',\r\n        tasks: [\r\n          {\r\n            title: '前端界面开发',\r\n            description: '使用Vue框架开发前端界面，实现各功能模块',\r\n            tags: [\r\n              { name: '开发', type: 'success' },\r\n              { name: '高优先级', type: 'danger' }\r\n            ],\r\n            assignees: [\r\n              { name: '李开发', initial: '李', color: '#F56C6C' },\r\n              { name: '钱开发', initial: '钱', color: '#909399' }\r\n            ]\r\n          },\r\n          {\r\n            title: 'API接口开发',\r\n            description: '实现后端API接口，包括用户管理、文档管理等功能',\r\n            tags: [\r\n              { name: '开发', type: 'success' },\r\n              { name: '高优先级', type: 'danger' }\r\n            ],\r\n            assignees: [\r\n              { name: '孙开发', initial: '孙', color: '#E6A23C' }\r\n            ]\r\n          }\r\n        ]\r\n      },\r\n      {\r\n        name: '已完成',\r\n        color: '#67C23A',\r\n        tasks: [\r\n          {\r\n            title: '项目计划制定',\r\n            description: '制定项目开发计划，包括时间节点和人员分工',\r\n            tags: [\r\n              { name: '计划', type: 'info' },\r\n              { name: '已完成', type: 'success' }\r\n            ],\r\n            assignees: [\r\n              { name: '张经理', initial: '张', color: '#409EFF' }\r\n            ]\r\n          },\r\n          {\r\n            title: '技术选型',\r\n            description: '选择适合的技术栈，包括前端框架、后端语言和数据库',\r\n            tags: [\r\n              { name: '设计', type: 'primary' },\r\n              { name: '已完成', type: 'success' }\r\n            ],\r\n            assignees: [\r\n              { name: '张经理', initial: '张', color: '#409EFF' },\r\n              { name: '王工程师', initial: '王', color: '#67C23A' }\r\n            ]\r\n          },\r\n          {\r\n            title: '原型设计',\r\n            description: '设计系统原型，明确各功能模块的界面和交互',\r\n            tags: [\r\n              { name: '设计', type: 'primary' },\r\n              { name: '已完成', type: 'success' }\r\n            ],\r\n            assignees: [\r\n              { name: '周设计', initial: '周', color: '#F56C6C' }\r\n            ]\r\n          }\r\n        ]\r\n      }\r\n    ])\r\n    \r\n    // 获取状态类型\r\n    const getStatusType = (status) => {\r\n      const typeMap = {\r\n        '进行中': 'primary',\r\n        '计划中': 'warning',\r\n        '已完成': 'success',\r\n        '待启动': 'info'\r\n      }\r\n      return typeMap[status] || 'info'\r\n    }\r\n    \r\n    // 获取进度颜色\r\n    const getProgressColor = (percentage) => {\r\n      if (percentage < 30) {\r\n        return '#F56C6C'\r\n      } else if (percentage < 70) {\r\n        return '#E6A23C'\r\n      } else {\r\n        return '#67C23A'\r\n      }\r\n    }\r\n    \r\n    // 获取任务总数\r\n    const getTotalTasks = () => {\r\n      return kanbanColumns.value.reduce((total, column) => {\r\n        return total + column.tasks.length\r\n      }, 0)\r\n    }\r\n    \r\n    // 获取已完成任务数\r\n    const getCompletedTasks = () => {\r\n      const completedColumn = kanbanColumns.value.find(col => col.name === '已完成')\r\n      return completedColumn ? completedColumn.tasks.length : 0\r\n    }\r\n    \r\n    // 获取进行中任务数\r\n    const getInProgressTasks = () => {\r\n      const inProgressColumn = kanbanColumns.value.find(col => col.name === '进行中')\r\n      return inProgressColumn ? inProgressColumn.tasks.length : 0\r\n    }\r\n    \r\n    // 计算完成百分比\r\n    const getCompletePercentage = () => {\r\n      const total = getTotalTasks()\r\n      const completed = getCompletedTasks()\r\n      return total > 0 ? Math.round((completed / total) * 100) : 0\r\n    }\r\n    \r\n    // 创建项目\r\n    const createProject = () => {\r\n      ElMessage.success('创建新项目')\r\n    }\r\n    \r\n    // 添加任务\r\n    const addTask = (columnIndex) => {\r\n      ElMessage.success(`向 ${kanbanColumns.value[columnIndex].name} 添加新任务`)\r\n    }\r\n    \r\n    // 打开任务详情\r\n    const openTaskDetail = (task) => {\r\n      currentTask.value = task\r\n      taskDetailVisible.value = true\r\n    }\r\n    \r\n    return {\r\n      activeProject,\r\n      activeTab,\r\n      projects,\r\n      kanbanColumns,\r\n      taskDetailVisible,\r\n      currentTask,\r\n      // 图标\r\n      Operation,\r\n      Plus,\r\n      Calendar,\r\n      Use