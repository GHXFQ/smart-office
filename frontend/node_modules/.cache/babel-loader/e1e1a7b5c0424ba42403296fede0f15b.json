p图标类名转换为Element Plus图标组件\r\n    const iconComponent = (icon) => {\r\n      // 如果是bootstrap图标类，转换为Element Plus图标\r\n      if (icon && icon.includes('bi-')) {\r\n        // 移除bi-前缀，转换为驼峰命名\r\n        const name = icon.replace('bi-', '')\r\n          .split('-')\r\n          .map((word, index) => \r\n            index === 0 ? word : word.charAt(0).toUpperCase() + word.slice(1)\r\n          )\r\n          .join('')\r\n        \r\n        // 检查是否存在对应的Element Plus图标\r\n        const iconMap = {\r\n          'home': 'House',\r\n          'grid': 'Grid',\r\n          'calendar': 'Calendar',\r\n          'book': 'Collection',\r\n          'chat': 'ChatDotRound',\r\n          'camera': 'VideoCamera',\r\n          'file': 'Document',\r\n          'file-earmark-text': 'Document',\r\n          'building': 'OfficeBuilding',\r\n          'envelope': 'Message',\r\n          'list': 'List',\r\n          'person': 'User',\r\n          'people': 'User',\r\n          'cart': 'ShoppingCart',\r\n          'gear': 'Setting',\r\n          'box': 'Box',\r\n          'graph-up': 'TrendCharts',\r\n          'code-slash': 'Terminal',\r\n          'headset': 'Service',\r\n          'send': 'Position',\r\n          'checkAll': 'CheckboxButton',\r\n          'clock': 'Clock',\r\n          'trash': 'Delete'\r\n        };\r\n        \r\n        return iconMap[name] || 'Document';\r\n      }\r\n      return 'Document'\r\n    }\r\n\r\n    return {\r\n      sidebarOpened,\r\n      routes,\r\n      activeMenu,\r\n      iconComponent\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.sidebar {\r\n  width: 220px;\r\n  height: 100%;\r\n  background-color: #fff;\r\n  border-right: 1px solid #e9ecef;\r\n  box-shadow: 0 0 10px rgba(0, 0, 0, 0.05);\r\n  z-index: 1000;\r\n  transition: width 0.3s;\r\n}\r\n\r\n.sidebar.is-collapsed {\r\n  width: 64px;\r\n}\r\n\r\n.sidebar-header {\r\n  padding: 20px;\r\n  display: flex;\r\n  align-items: center;\r\n  border-bottom: 1px solid #e9ecef;\r\n  height: 60px;\r\n}\r\n\r\n.sidebar-header img {\r\n  width: 24px;\r\n  height: 24px;\r\n}\r\n\r\n.sidebar-header h5 {\r\n  margin: 0 0 0 12px;\r\n  font-size: 16px;\r\n  font-weight: 600;\r\n  color: #333;\r\n  white-space: nowrap;\r\n}\r\n\r\n.sidebar-menu-container {\r\n  height: calc(100% - 60px);\r\n}\r\n\r\n.el-menu {\r\n  border-right: none;\r\n}\r\n\r\n.el-menu-item {\r\n  height: 50px;\r\n  line-height: 50px;\r\n}\r\n\r\n.el-menu-item .el-icon {\r\n  margin-right: 10px;\r\n  width: 24px;\r\n  text-align: center;\r\n  font-size: 18px;\r\n}\r\n</style> "],"mappings":";;EAGSA,KAAK,EAAC;AAAgB;;;;;;;;;uBAF7BC,mBAAA,CA0BM;IA1BDD,KAAK,EAAAE,eAAA,EAAC,SAAS;MAAA,iBAA4BC,MAAA,CAAAC;IAAa;MAC3DC,mBAAA,WAAc,EACdC,mBAAA,CAGM,OAHNC,UAGM,G,0BAFJD,mBAAA,CAA8E;IAAzEE,GAAG,EAAC,yDAAyD;IAACC,GAAG,EAAC;+BAC7DN,MAAA,CAAAC,aAAa,I,cAAvBH,mBAAA,CAAoC,MAAAS,UAAA,EAAX,QAAM,K,qCAGjCL,mBAAA,WAAc,EACdM,YAAA,CAiBeC,uBAAA;IAjBDZ,KAAK,EAAC;EAAwB;sBAC1C,MAeU,CAfVW,YAAA,CAeUE,kBAAA;MAdP,gBAAc,EAAEV,MAAA,CAAAW,UAAU;MAC1BC,QAAQ,GAAGZ,MAAA,CAAAC,aAAa;MACzB,kBAAgB,EAAC,MAAM;MACvB,YAAU,EAAC,SAAS;MACpB,mBAAiB,EAAC,SAAS;MAC1B,eAAa,EAAE,IAAI;MACpBY,MAAM,EAAN;;wBAEc,MAAuB,E,kBAArCf,mBAAA,CAKegB,SAAA,QAAAC,WAAA,CALef,MAAA,CAAAgB,MAAM,EAAfC,KAAK;6BAA1BC,YAAA,CAKeC,uBAAA;UALwBC,GAAG,EAAEH,KAAK,CAACI,IAAI;UAAGC,KAAK,EAAEL,KAAK,CAACI;;UAIzDE,KAAK,EAAAC,QAAA,CAAC,MAAsB,C,kCAAnBP,KAAK,CAACQ,IAAI,CAACF,KAAK,iB;4BAHpC,MAEU,CAFVf,YAAA,CAEUkB,kBAAA;8BADR,MAA4D,E,cAA5DR,YAAA,CAA4DS,wBAAA,CAA5C3B,MAAA,CAAA4B,aAAa,CAACX,KAAK,CAACQ,IAAI,CAACI,IAAI,K","ignoreList":[]}]}