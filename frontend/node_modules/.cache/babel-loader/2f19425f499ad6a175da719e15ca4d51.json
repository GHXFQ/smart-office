{"remainingRequest":"C:\\Users\\18083\\Desktop\\新建文件夹 (3)\\smart-office\\frontend\\node_modules\\babel-loader\\lib\\index.js!C:\\Users\\18083\\Desktop\\新建文件夹 (3)\\smart-office\\frontend\\node_modules\\cache-loader\\dist\\cjs.js??ref--1-0!C:\\Users\\18083\\Desktop\\新建文件夹 (3)\\smart-office\\frontend\\node_modules\\vue-loader-v16\\dist\\index.js??ref--1-1!C:\\Users\\18083\\Desktop\\新建文件夹 (3)\\smart-office\\frontend\\src\\views\\document\\Index.vue?vue&type=script&lang=js","dependencies":[{"path":"C:\\Users\\18083\\Desktop\\新建文件夹 (3)\\smart-office\\frontend\\src\\views\\document\\Index.vue","mtime":1746442848940},{"path":"C:\\Users\\18083\\Desktop\\新建文件夹 (3)\\smart-office\\frontend\\babel.config.js","mtime":1746431728272},{"path":"C:\\Users\\18083\\Desktop\\新建文件夹 (3)\\smart-office\\frontend\\node_modules\\cache-loader\\dist\\cjs.js","mtime":1746431218359},{"path":"C:\\Users\\18083\\Desktop\\新建文件夹 (3)\\smart-office\\frontend\\node_modules\\babel-loader\\lib\\index.js","mtime":1746431202324},{"path":"C:\\Users\\18083\\Desktop\\新建文件夹 (3)\\smart-office\\frontend\\node_modules\\cache-loader\\dist\\cjs.js","mtime":1746431218359},{"path":"C:\\Users\\18083\\Desktop\\新建文件夹 (3)\\smart-office\\frontend\\node_modules\\vue-loader-v16\\dist\\index.js","mtime":1746431218786}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"names":["ref","computed","Plus","Search","Document","DocumentIcon","Grid","Present","More","Edit","Share","Download","EditPen","Delete","ArrowDown","Bold","Italic","Underline","AlignLeft","AlignCenter","AlignRight","List","Menu","Picture","Link","ElMessage","name","setup","searchQuery","filterType","selectedDocument","shareDialogVisible","sharePermission","shareLink","selectedCollaborators","currentCollaborators","editorContent","editorFont","editorSize","editorBold","editorItalic","editorUnderline","editorAlign","documents","title","type","owner","lastModified","status","collaborators","initial","color","collaboratorOptions","value","label","filteredDocuments","result","filter","doc","some","c","query","toLowerCase","includes","getStatusText","statusMap","getStatusType","typeMap","createDocument","typeNames","unshift","success","editDocument","console","log","shareDocument","map","permission","downloadDocument","renameDocument","deleteDocument","copyShareLink","navigator","clipboard","writeText","removeCollaborator","index","splice","confirmShare","SlidingFull"],"sources":["C:\\Users\\18083\\Desktop\\新建文件夹 (3)\\smart-office\\frontend\\src\\views\\document\\Index.vue"],"sourcesContent":["<template>\r\n  <div class=\"document-container\">\r\n    <div class=\"row\">\r\n      <!-- 左侧文档列表 -->\r\n      <div class=\"col-md-4\">\r\n        <el-card class=\"document-list-card\">\r\n          <template #header>\r\n            <div class=\"card-header-flex\">\r\n              <span>我的文档</span>\r\n              <div>\r\n                <el-dropdown>\r\n                  <el-button type=\"primary\" size=\"small\">\r\n                    <el-icon class=\"mr-1\"><Plus /></el-icon>\r\n                    创建文档 <el-icon><ArrowDown /></el-icon>\r\n                  </el-button>\r\n                  <template #dropdown>\r\n                    <el-dropdown-menu>\r\n                      <el-dropdown-item @click=\"createDocument('doc')\">\r\n                        <el-icon><Document /></el-icon> 文本文档\r\n                      </el-dropdown-item>\r\n                      <el-dropdown-item @click=\"createDocument('sheet')\">\r\n                        <el-icon><Grid /></el-icon> 电子表格\r\n                      </el-dropdown-item>\r\n                      <el-dropdown-item @click=\"createDocument('slide')\">\r\n                        <el-icon><Present /></el-icon> 演示文稿\r\n                      </el-dropdown-item>\r\n                    </el-dropdown-menu>\r\n                  </template>\r\n                </el-dropdown>\r\n              </div>\r\n            </div>\r\n          </template>\r\n          \r\n          <div class=\"document-filter\">\r\n            <el-input\r\n              v-model=\"searchQuery\"\r\n              placeholder=\"搜索文档...\"\r\n              clearable\r\n              prefix-icon=\"Search\"\r\n            ></el-input>\r\n            \r\n            <div class=\"filter-tabs\">\r\n              <el-radio-group v-model=\"filterType\" size=\"small\">\r\n                <el-radio-button label=\"all\">全部</el-radio-button>\r\n                <el-radio-button label=\"owned\">我创建的</el-radio-button>\r\n                <el-radio-button label=\"shared\">共享给我的</el-radio-button>\r\n              </el-radio-group>\r\n            </div>\r\n          </div>\r\n          \r\n          <div class=\"document-list\">\r\n            <div\r\n              v-for=\"(doc, index) in filteredDocuments\"\r\n              :key=\"index\"\r\n              class=\"document-item\"\r\n              :class=\"{'active': selectedDocument === index}\"\r\n              @click=\"selectedDocument = index\"\r\n            >\r\n              <div class=\"document-icon\" :class=\"'icon-' + doc.type\">\r\n                <el-icon v-if=\"doc.type === 'doc'\"><Document /></el-icon>\r\n                <el-icon v-else-if=\"doc.type === 'sheet'\"><Grid /></el-icon>\r\n                <el-icon v-else-if=\"doc.type === 'slide'\"><SlidingFull /></el-icon>\r\n              </div>\r\n              <div class=\"document-content\">\r\n                <div class=\"document-title\">\r\n                  {{ doc.title }}\r\n                  <span \r\n                    v-if=\"doc.status\" \r\n                    class=\"status-badge\" \r\n                    :class=\"'status-' + doc.status\"\r\n                  >\r\n                    {{ getStatusText(doc.status) }}\r\n                  </span>\r\n                </div>\r\n                <div class=\"document-info\">\r\n                  <div>{{ doc.owner }} · {{ doc.lastModified }}</div>\r\n                  <div class=\"document-collaborators\">\r\n                    <div \r\n                      v-for=\"(collab, i) in doc.collaborators\" \r\n                      :key=\"i\"\r\n                      class=\"collaborator\"\r\n                      :style=\"{ backgroundColor: collab.color }\"\r\n                      :title=\"collab.name\"\r\n                    >\r\n                      {{ collab.initial }}\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n              <div class=\"document-actions\">\r\n                <el-dropdown trigger=\"click\" @click.stop>\r\n                  <el-button circle size=\"small\">\r\n                    <el-icon><More /></el-icon>\r\n                  </el-button>\r\n                  <template #dropdown>\r\n                    <el-dropdown-menu>\r\n                      <el-dropdown-item @click=\"editDocument(doc)\">\r\n                        <el-icon><Edit /></el-icon> 编辑\r\n                      </el-dropdown-item>\r\n                      <el-dropdown-item @click=\"shareDocument(doc)\">\r\n                        <el-icon><Share /></el-icon> 共享\r\n                      </el-dropdown-item>\r\n                      <el-dropdown-item @click=\"downloadDocument(doc)\">\r\n                        <el-icon><Download /></el-icon> 下载\r\n                      </el-dropdown-item>\r\n                      <el-dropdown-item @click=\"renameDocument(doc)\">\r\n                        <el-icon><EditPen /></el-icon> 重命名\r\n                      </el-dropdown-item>\r\n                      <el-dropdown-item divided @click=\"deleteDocument(doc)\">\r\n                        <el-icon><Delete /></el-icon> 删除\r\n                      </el-dropdown-item>\r\n                    </el-dropdown-menu>\r\n                  </template>\r\n                </el-dropdown>\r\n              </div>\r\n            </div>\r\n            \r\n            <el-empty \r\n              v-if=\"filteredDocuments.length === 0\" \r\n              description=\"暂无文档\"\r\n            ></el-empty>\r\n          </div>\r\n        </el-card>\r\n      </div>\r\n      \r\n      <!-- 右侧文档编辑器 -->\r\n      <div class=\"col-md-8\">\r\n        <el-card v-if=\"selectedDocument !== null\" class=\"editor-card\">\r\n          <template #header>\r\n            <div class=\"card-header-flex\">\r\n              <div class=\"editor-title\">\r\n                <span>{{ documents[selectedDocument].title }}</span>\r\n                <el-tag size=\"small\" class=\"ml-2\" :type=\"getStatusType(documents[selectedDocument].status)\">\r\n                  {{ getStatusText(documents[selectedDocument].status) }}\r\n                </el-tag>\r\n              </div>\r\n              <div class=\"editor-actions\">\r\n                <div class=\"collaborators\">\r\n                  <div \r\n                    v-for=\"(collab, i) in documents[selectedDocument].collaborators\" \r\n                    :key=\"i\"\r\n                    class=\"collaborator\"\r\n                    :style=\"{ backgroundColor: collab.color }\"\r\n                    :title=\"collab.name\"\r\n                  >\r\n                    {{ collab.initial }}\r\n                  </div>\r\n                  <el-button circle size=\"small\">\r\n                    <el-icon><Plus /></el-icon>\r\n                  </el-button>\r\n                </div>\r\n                <el-button type=\"primary\" size=\"small\">\r\n                  <el-icon><Share /></el-icon> 共享\r\n                </el-button>\r\n              </div>\r\n            </div>\r\n          </template>\r\n          \r\n          <div class=\"document-editor\">\r\n            <!-- 编辑器工具栏 -->\r\n            <div class=\"editor-toolbar\">\r\n              <div class=\"toolbar-group\">\r\n                <el-select v-model=\"editorFont\" size=\"small\" class=\"mr-2\">\r\n                  <el-option label=\"默认字体\" value=\"default\"></el-option>\r\n                  <el-option label=\"宋体\" value=\"SimSun\"></el-option>\r\n                  <el-option label=\"微软雅黑\" value=\"Microsoft YaHei\"></el-option>\r\n                  <el-option label=\"Arial\" value=\"Arial\"></el-option>\r\n                </el-select>\r\n                <el-select v-model=\"editorSize\" size=\"small\">\r\n                  <el-option label=\"12\" value=\"12\"></el-option>\r\n                  <el-option label=\"14\" value=\"14\"></el-option>\r\n                  <el-option label=\"16\" value=\"16\"></el-option>\r\n                  <el-option label=\"18\" value=\"18\"></el-option>\r\n                </el-select>\r\n              </div>\r\n              \r\n              <div class=\"toolbar-group\">\r\n                <el-button-group>\r\n                  <el-tooltip content=\"粗体\" placement=\"top\">\r\n                    <el-button text :class=\"{'is-active': editorBold}\">\r\n                      <el-icon><Bold /></el-icon>\r\n                    </el-button>\r\n                  </el-tooltip>\r\n                  <el-tooltip content=\"斜体\" placement=\"top\">\r\n                    <el-button text :class=\"{'is-active': editorItalic}\">\r\n                      <el-icon><Italic /></el-icon>\r\n                    </el-button>\r\n                  </el-tooltip>\r\n                  <el-tooltip content=\"下划线\" placement=\"top\">\r\n                    <el-button text :class=\"{'is-active': editorUnderline}\">\r\n                      <el-icon><Underline /></el-icon>\r\n                    </el-button>\r\n                  </el-tooltip>\r\n                </el-button-group>\r\n              </div>\r\n              \r\n              <div class=\"toolbar-group\">\r\n                <el-button-group>\r\n                  <el-tooltip content=\"左对齐\" placement=\"top\">\r\n                    <el-button text :class=\"{'is-active': editorAlign === 'left'}\">\r\n                      <el-icon><AlignLeft /></el-icon>\r\n                    </el-button>\r\n                  </el-tooltip>\r\n                  <el-tooltip content=\"居中\" placement=\"top\">\r\n                    <el-button text :class=\"{'is-active': editorAlign === 'center'}\">\r\n                      <el-icon><AlignCenter /></el-icon>\r\n                    </el-button>\r\n                  </el-tooltip>\r\n                  <el-tooltip content=\"右对齐\" placement=\"top\">\r\n                    <el-button text :class=\"{'is-active': editorAlign === 'right'}\">\r\n                      <el-icon><AlignRight /></el-icon>\r\n                    </el-button>\r\n                  </el-tooltip>\r\n                </el-button-group>\r\n              </div>\r\n              \r\n              <div class=\"toolbar-group\">\r\n                <el-button-group>\r\n                  <el-tooltip content=\"项目符号\" placement=\"top\">\r\n                    <el-button text>\r\n                      <el-icon><List /></el-icon>\r\n                    </el-button>\r\n                  </el-tooltip>\r\n                  <el-tooltip content=\"编号列表\" placement=\"top\">\r\n                    <el-button text>\r\n                      <el-icon><Menu /></el-icon>\r\n                    </el-button>\r\n                  </el-tooltip>\r\n                </el-button-group>\r\n              </div>\r\n              \r\n              <div class=\"toolbar-group\">\r\n                <el-button-group>\r\n                  <el-tooltip content=\"插入图片\" placement=\"top\">\r\n                    <el-button text>\r\n                      <el-icon><Picture /></el-icon>\r\n                    </el-button>\r\n                  </el-tooltip>\r\n                  <el-tooltip content=\"插入表格\" placement=\"top\">\r\n                    <el-button text>\r\n                      <el-icon><Grid /></el-icon>\r\n                    </el-button>\r\n                  </el-tooltip>\r\n                  <el-tooltip content=\"插入链接\" placement=\"top\">\r\n                    <el-button text>\r\n                      <el-icon><Link /></el-icon>\r\n                    </el-button>\r\n                  </el-tooltip>\r\n                </el-button-group>\r\n              </div>\r\n            </div>\r\n            \r\n            <!-- 编辑器内容区 -->\r\n            <div class=\"editor-content\" contenteditable=\"true\" ref=\"editorContent\">\r\n              <h1>智能办公系统产品设计方案</h1>\r\n              <p>本文档旨在详细描述智能办公系统的产品设计方案，包括功能模块、技术架构和实施计划。</p>\r\n              \r\n              <h2>1. 产品概述</h2>\r\n              <p>智能办公系统是一款集成了办公所需各类功能的一站式解决方案，旨在提高工作效率、促进协作和简化办公流程。</p>\r\n              \r\n              <h3>1.1 产品定位</h3>\r\n              <p>为中小型企业提供的一站式智能办公平台，满足日常办公、沟通协作、文档管理、任务管理等需求。</p>\r\n              \r\n              <h3>1.2 目标用户</h3>\r\n              <ul>\r\n                <li>企业管理层：需要全局掌控企业运营情况</li>\r\n                <li>部门主管：需要管理团队任务和项目进度</li>\r\n                <li>普通职员：需要高效完成日常工作任务</li>\r\n              </ul>\r\n              \r\n              <h2>2. 功能模块</h2>\r\n              <p>系统包含以下核心功能模块：</p>\r\n              \r\n              <h3>2.1 即时通讯</h3>\r\n              <p>提供文字聊天、语音通话、视频会议等功能，支持一对一和群组沟通。</p>\r\n              \r\n              <h3>2.2 文档协作</h3>\r\n              <p>支持在线创建、编辑和共享文档，实现多人实时协作。</p>\r\n            </div>\r\n          </div>\r\n        </el-card>\r\n        \r\n        <el-empty \r\n          v-else \r\n          description=\"请选择一个文档进行编辑\"\r\n          class=\"editor-empty\"\r\n        ></el-empty>\r\n      </div>\r\n    </div>\r\n    \r\n    <!-- 共享文档对话框 -->\r\n    <el-dialog\r\n      v-model=\"shareDialogVisible\"\r\n      title=\"共享文档\"\r\n      width=\"500px\"\r\n    >\r\n      <div class=\"share-dialog-content\">\r\n        <div class=\"share-settings\">\r\n          <div class=\"share-setting-item\">\r\n            <span>权限设置</span>\r\n            <el-radio-group v-model=\"sharePermission\">\r\n              <el-radio label=\"view\">只能查看</el-radio>\r\n              <el-radio label=\"comment\">可以评论</el-radio>\r\n              <el-radio label=\"edit\">可以编辑</el-radio>\r\n            </el-radio-group>\r\n          </div>\r\n          \r\n          <div class=\"share-setting-item\">\r\n            <span>共享链接</span>\r\n            <div class=\"share-link-input\">\r\n              <el-input v-model=\"shareLink\" readonly>\r\n                <template #append>\r\n                  <el-button @click=\"copyShareLink\">复制</el-button>\r\n                </template>\r\n              </el-input>\r\n            </div>\r\n          </div>\r\n          \r\n          <div class=\"share-setting-item\">\r\n            <span>添加协作者</span>\r\n            <el-select\r\n              v-model=\"selectedCollaborators\"\r\n              multiple\r\n              filterable\r\n              placeholder=\"搜索用户\"\r\n              style=\"width: 100%\"\r\n            >\r\n              <el-option\r\n                v-for=\"item in collaboratorOptions\"\r\n                :key=\"item.value\"\r\n                :label=\"item.label\"\r\n                :value=\"item.value\"\r\n              >\r\n                <div class=\"collaborator-option\">\r\n                  <div \r\n                    class=\"collaborator\"\r\n                    :style=\"{ backgroundColor: item.color }\"\r\n                  >\r\n                    {{ item.initial }}\r\n                  </div>\r\n                  <span>{{ item.label }}</span>\r\n                </div>\r\n              </el-option>\r\n            </el-select>\r\n          </div>\r\n          \r\n          <div class=\"shared-users-list\">\r\n            <div class=\"shared-users-title\">当前协作者</div>\r\n            <div \r\n              v-for=\"(user, index) in currentCollaborators\" \r\n              :key=\"index\"\r\n              class=\"shared-user-item\"\r\n            >\r\n              <div class=\"shared-user-info\">\r\n                <div \r\n                  class=\"collaborator\"\r\n                  :style=\"{ backgroundColor: user.color }\"\r\n                >\r\n                  {{ user.initial }}\r\n                </div>\r\n                <span>{{ user.name }}</span>\r\n              </div>\r\n              <div class=\"shared-user-permission\">\r\n                <el-select v-model=\"user.permission\" size=\"small\">\r\n                  <el-option label=\"只能查看\" value=\"view\"></el-option>\r\n                  <el-option label=\"可以评论\" value=\"comment\"></el-option>\r\n                  <el-option label=\"可以编辑\" value=\"edit\"></el-option>\r\n                </el-select>\r\n                <el-button\r\n                  type=\"danger\"\r\n                  size=\"small\"\r\n                  circle\r\n                  @click=\"removeCollaborator(index)\"\r\n                >\r\n                  <el-icon><Delete /></el-icon>\r\n                </el-button>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <template #footer>\r\n        <span class=\"dialog-footer\">\r\n          <el-button @click=\"shareDialogVisible = false\">取消</el-button>\r\n          <el-button type=\"primary\" @click=\"confirmShare\">\r\n            确定\r\n          </el-button>\r\n        </span>\r\n      </template>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { ref, computed } from 'vue'\r\nimport { \r\n  Plus, Search, Document as DocumentIcon, Grid, Present, More, Edit, \r\n  Share, Download, EditPen, Delete, ArrowDown, Bold, \r\n  Italic, Underline, AlignLeft, AlignCenter, AlignRight, \r\n  List, Menu, Picture, Link\r\n} from '@element-plus/icons-vue'\r\nimport { ElMessage } from 'element-plus'\r\n\r\nexport default {\r\n  name: 'Document',\r\n  setup() {\r\n    const searchQuery = ref('')\r\n    const filterType = ref('all')\r\n    const selectedDocument = ref(0)\r\n    const shareDialogVisible = ref(false)\r\n    const sharePermission = ref('view')\r\n    const shareLink = ref('https://office.example.com/doc/abc123')\r\n    const selectedCollaborators = ref([])\r\n    const currentCollaborators = ref([])\r\n    const editorContent = ref(null)\r\n    \r\n    // 编辑器状态\r\n    const editorFont = ref('default')\r\n    const editorSize = ref('14')\r\n    const editorBold = ref(false)\r\n    const editorItalic = ref(false)\r\n    const editorUnderline = ref(false)\r\n    const editorAlign = ref('left')\r\n    \r\n    // 文档列表\r\n    const documents = ref([\r\n      {\r\n        title: '智能办公系统产品设计方案',\r\n        type: 'doc',\r\n        owner: '张经理',\r\n        lastModified: '今天 14:30',\r\n        status: 'editing',\r\n        collaborators: [\r\n          { name: '张经理', initial: '张', color: '#007bff' },\r\n          { name: '李工', initial: '李', color: '#28a745' }\r\n        ]\r\n      },\r\n      {\r\n        title: '2023年Q2销售业绩统计',\r\n        type: 'sheet',\r\n        owner: '王总',\r\n        lastModified: '昨天 10:15',\r\n        status: 'shared',\r\n        collaborators: [\r\n          { name: '王总', initial: '王', color: '#dc3545' },\r\n          { name: '赵经理', initial: '赵', color: '#fd7e14' },\r\n          { name: '张经理', initial: '张', color: '#007bff' }\r\n        ]\r\n      },\r\n      {\r\n        title: '新功能发布会演示PPT',\r\n        type: 'slide',\r\n        owner: '李工',\r\n        lastModified: '05-12 09:30',\r\n        status: null,\r\n        collaborators: [\r\n          { name: '李工', initial: '李', color: '#28a745' }\r\n        ]\r\n      },\r\n      {\r\n        title: '项目进度报告',\r\n        type: 'doc',\r\n        owner: '张经理',\r\n        lastModified: '05-10 16:45',\r\n        status: 'locked',\r\n        collaborators: [\r\n          { name: '张经理', initial: '张', color: '#007bff' }\r\n        ]\r\n      }\r\n    ])\r\n    \r\n    // 协作者选项\r\n    const collaboratorOptions = ref([\r\n      { value: '1', label: '王总', initial: '王', color: '#dc3545' },\r\n      { value: '2', label: '李工', initial: '李', color: '#28a745' },\r\n      { value: '3', label: '赵经理', initial: '赵', color: '#fd7e14' },\r\n      { value: '4', label: '钱主管', initial: '钱', color: '#6610f2' },\r\n      { value: '5', label: 