:percentage=\"80\" status=\"success\" style=\"margin-bottom:8px;\" />\r\n            <p class=\"text-muted small\">项目整体运行良好，进度符合预期</p>\r\n          </div>\r\n        </div>\r\n        <div class=\"col-md-4\">\r\n          <div class=\"ai-analysis-card\">\r\n            <h6 class=\"text-primary\">资源使用情况</h6>\r\n            <div class=\"resource-usage\">\r\n              <div class=\"usage-item\">\r\n                <el-icon><UserFilled /></el-icon>\r\n                人力资源利用率：85%\r\n              </div>\r\n              <div class=\"usage-item\">\r\n                <el-icon><Clock /></el-icon>\r\n                时间利用率：78%\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <div class=\"col-md-4\">\r\n          <div class=\"ai-analysis-card\">\r\n            <h6 class=\"text-primary\">风险预警</h6>\r\n            <div class=\"risk-warnings\">\r\n              <div class=\"warning-item text-warning\">\r\n                <el-icon><WarningFilled /></el-icon>\r\n                3个任务可能延期\r\n              </div>\r\n              <div class=\"warning-item text-success\">\r\n                <el-icon><Check /></el-icon>\r\n                资源分配合理\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <!-- 报表内容 -->\r\n    <div class=\"reports-content\">\r\n      <div class=\"row\">\r\n        <div class=\"col-md-6\">\r\n          <div class=\"card\">\r\n            <div class=\"card-body\">\r\n              <h6 class=\"card-title\">项目进度</h6>\r\n              <div ref=\"progressChart\" style=\"width:100%;height:300px;\"></div>\r\n              <div class=\"ai-chart-analysis mt-2\">\r\n                <small class=\"text-muted\">AI分析：进度符合预期，建议关注高风险任务</small>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <div class=\"col-md-6\">\r\n          <div class=\"card\">\r\n            <div class=\"card-body\">\r\n              <h6 class=\"