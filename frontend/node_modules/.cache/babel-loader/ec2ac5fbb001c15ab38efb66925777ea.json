tar\"\r\n                      >\r\n                        {{ ['ZS', 'LM', 'WL', 'ZX', 'CJ'][i-1] }}\r\n                      </div>\r\n                      <div v-if=\"votes[selectedVote].participants > 5\" class=\"more-participants\">\r\n                        +{{ votes[selectedVote].participants - 5 }}\r\n                      </div>\r\n                    </div>\r\n                  </div>\r\n                  \r\n                  <div v-for=\"(question, qIndex) in votes[selectedVote].questions\" :key=\"qIndex\" class=\"question-stats mb-4\">\r\n                    <h5>{{ qIndex + 1 }}. {{ question.title }}</h5>\r\n                    <div v-if=\"question.type !== 'text'\" class=\"question-options-stats\">\r\n                      <div v-for=\"(option, oIndex) in question.options\" :key=\"oIndex\" class=\"option-stat mb-2\">\r\n                        <div class=\"option-name mb-1\">{{ option }}</div>\r\n                        <el-progress \r\n                          :percentage=\"question.results ? question.results[oIndex] : Math.floor(Math.random() * 100)\" \r\n                          :stroke-width=\"8\"\r\n                        ></el-progress>\r\n                      </div>\r\n                    </div>\r\n                    <div v-else class=\"text-muted\">\r\n                      文本回答不提供统计图表\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n              </el-tab-pane>\r\n            </el-tabs>\r\n          </div>\r\n        </el-card>\r\n        \r\n        <el-empty v-else description=\"请选择一个投票问卷\"></el-empty>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'Vote',\r\n  data() {\r\n    return {\r\n      searchQuery: '',\r\n      selectedVote: 0,\r\n      activeTab: 'content',\r\n      votes: [\r\n        {\r\n          title: '公司年度旅游目的地投票',\r\n          description: '为了更好地安排公司年度旅游活动，请大家投票选择心仪的旅游目的地。本次投票结果将作为最终决策的重要参考依据。',\r\n          creator: '人事部',