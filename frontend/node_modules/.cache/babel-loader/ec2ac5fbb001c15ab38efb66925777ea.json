     class=\"vote-item\" \r\n              :class=\"{'active': selectedVote === index}\"\r\n              @click=\"selectedVote = index\"\r\n            >\r\n              <div class=\"vote-title\">{{ vote.title }}</div>\r\n              <div class=\"vote-info\">\r\n                <span>{{ vote.creator }} · {{ vote.date }}</span>\r\n                <span class=\"vote-status\" :class=\"'status-' + vote.status\">{{ getStatusText(vote.status) }}</span>\r\n              </div>\r\n              <div class=\"vote-progress mt-2\" v-if=\"vote.status === 'active'\">\r\n                <el-progress :percentage=\"vote.completion\" :stroke-width=\"8\"></el-progress>\r\n                <div class=\"vote-percent\">\r\n                  <span>参与人数: {{ vote.participants }}/{{ vote.total }}</span>\r\n                  <span>{{ vote.completion }}%</span>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </el-card>\r\n      </div>\r\n\r\n      <!-- 右侧投票详情 -->\r\n      <div class=\"col-md-8\">\r\n        <el-card v-if=\"selectedVote !== null\">\r\n          <template #header>\r\n            <div class=\"card-header-flex\">\r\n              <span>{{ votes[selectedVote].title }}</span>\r\n              <div>\r\n                <el-button type=\"primary\" size=\"small\" v-if=\"votes[selectedVote].status === 'active'\">\r\n                  提交投票\r\n                </el-button>\r\n                <el-button size=\"small\" v-if=\"votes[selectedVote].status === 'active'\">\r\n                  放弃投票\r\n                </el-button>\r\n                <el-button size=\"small\" v-if=\"votes[selectedVote].status === 'draft'\">\r\n                  <i class=\"bi bi-pencil\"></i> 编辑\r\n                </el-button>\r\n                <el-button size=\"small\" v-if=\"votes[selectedVote].status === 'draft'\">\r\n                  <i class=\"bi bi-play-fill\"></i> 发布\r\n                </el-button>\r\n              </div>\r\n            </div>\r\n   