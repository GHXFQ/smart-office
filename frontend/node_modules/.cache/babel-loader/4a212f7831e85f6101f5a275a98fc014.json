resolve(response.data);\n      } else {\n        commit('REQUEST_ERROR', response.message || '获取仪表盘数据失败');\n        return Promise.reject(response);\n      }\n    } catch (error) {\n      const errorMsg = error.response?.data?.message || '获取仪表盘数据失败';\n      commit('REQUEST_ERROR', errorMsg);\n      return Promise.reject(error);\n    }\n  },\n  \n  // 获取未读消息数量\n  async fetchUnreadCount({ commit }) {\n    commit('REQUEST_START');\n    try {\n      const response = await dashboardApi.getUnreadCount();\n      \n      if (response.success) {\n        commit('SET_UNREAD', response.data);\n        return Promise.resolve(response.data);\n      } else {\n        commit('REQUEST_ERROR', response.message || '获取未读消息数量失败');\n        return Promise.reject(response);\n      }\n    } catch (error) {\n      const errorMsg = error.response?.data?.message || '获取未读消息数量失败';\n      commit('REQUEST_ERROR', errorMsg);\n      return Promise.reject(error);\n    }\n  },\n  \n  // 获取最近任务\n  async fetchRecentTasks({ commit }) {\n    commit('REQUEST_START');\n    try {\n      const response = await dashboardApi.getRecentTasks();\n      \n      if (response.success) {\n        commit('SET_RECENT_TASKS', response.data);\n        return Promise.resolve(response.data);\n      } else {\n        commit('REQUEST_ERROR', response.message || '获取最近任务失败');\n        return Promise.reject(response);\n      }\n    } catch (error) {\n      const errorMsg = error.response?.data?.message || '获取最近任务失败';\n      commit('REQUEST_ERROR', errorMsg);\n      return Promise.reject(error);\n    }\n  },\n  \n  // 获取通知消息\n  async fetchNotifications({ commit }) {\n    commit('REQUEST_START');\n    try {\n      const response = await dashboardApi.getNotifications();\n      \n      if (response.success) {\n        commit('SET_NOTIFICATIONS', response.data);\n        return Promise.resolve(response.data);\n      } else {\n        commit('REQUEST_ERROR', response.message || '获取通知消息失败');\n        return Promise.reject(response);\n      }\n    } catch (error) {\n      const errorMsg = error.response?.data?.message || '获取通知消息失败';\n      commit('REQUEST_ERROR', errorMsg);\n      return Promise.reject(error);\n    }\n  },\n  \n  // 获取日程安排\n  async fetchCalendarEvents({ commit }) {\n    commit('REQUEST_START');\n    try {\n      const response = await dashboardApi.getCalendarEvents();\n      \n      if (response.success) {\n        commit('SET_CALENDAR_EVENTS', response.data);\n        return Promise.resolve(response.data);\n      } else {\n        commit('REQUEST_ERROR', response.message || '获取日程安排失败');\n        return Promise.reject(response);\n      }\n    } catch (error) {\n      const errorMsg = error.response?.data?.message || '获取日程安排失败';\n      commit('REQUEST_ERROR', errorMsg);\n      return Promise.reject(error);\n    }\n  },\n  \n  // 获取团队动态\n  async fetchTeamActivities({ commit }) {\n    commit('REQUEST_START');\n    try {\n      const response = await dashboardApi.getTeamActivities();\n      \n      if (response.success) {\n        commit('SET_ACTIVITIES', response.data);\n        return Promise.resolve(response.data);\n      } else {\n        commit('REQUEST_ERROR', response.message || '获取团队动态失败');\n        return Promise.reject(response);\n      }\n    } catch (error) {\n      const errorMsg = error.response?.data?.message || '获取团队动态失败';\n      commit('REQUEST_ERROR', errorMsg);\n      return Promise.reject(error);\n    }\n  }\n};\n\nconst getters = {\n  unread: state => state.unread,\n  recentTasks: state => state.recentTasks,\n  notifications: state => state.notifications,\n  calendarEvents: state => state.calendarEvents,\n  activities: state => state.activities,\n  loading: state => state.loading,\n  error: state => state.error\n};\n\nexport default {\n  namespaced: true,\n  state,\n  mutations,\n  actions,\n  getters\n}; "],"mappings":"AAAA,OAAOA,YAAY,MAAM,iBAAiB;AAE1C,MAAMC,KAAK,GAAG;EACZC,OAAO,EAAE,KAAK;EACdC,KAAK,EAAE,IAAI;EACXC,MAAM,EAAE;IACNC,QAAQ,EAAE,CAAC;IACXC,KAAK,EAAE