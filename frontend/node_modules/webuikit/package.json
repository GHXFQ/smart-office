{
  "name": "webuikit",
  "version": "0.0.15",
  "description": "Basic UI React Component Kit for Zomato",
  "repository": "git@github.com:Zomato/WebUIKit.git",
  "keywords": [
    "uikit",
    "zomato",
    "design-system",
    "react",
    "components"
  ],
  "author": "Zomato",
  "license": "ISC",
  "main": "./index.js",
  "bugs": {
    "url": "https://github.com/Zomato/WebUIKit/issues"
  },
  "homepage": "https://github.com/Zomato/WebUIKit#readme",
  "scripts": {
    "generate-icons": "node ./buildHelpers/generateSVG.js",
    "start": "npm run generate-icons && webpack-dev-server --config ./prod.config.js --watch",
    "format": "prettier --write \"src/**/*.{js,jsx,css,html}\" ",
    "format:check": "prettier --list-different \"src/**/*.{js,jsx,css,html}\" ",
    "build:webpack": "npm run generate-icons && webpack --config ./prod.config.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "storybook": "npm run generate-icons && start-storybook -s ./public",
    "serve": "npm run generate-icons && npm run storybook -- -p 80",
    "build-storybook": "npm run generate-icons && build-storybook -c .storybook -o .out",
    "lint": "eslint --fix \"src/**/*.{js,jsx}\" && echo 'Lint complete.'",
    "copy:build": "shx cp package.json dist/ && shx cp README.md dist/",
    "prepublish:npm": "rm -rvf ./dist/* && yarn run build && yarn run copy:build && cd ./dist/ && npm pack && npm addUser",
    "build:es2015": "NODE_ENV=production babel ./src --out-dir ./dist --ignore **.test.js,**/__tests__/**,**/e2e.js,template-component/**",
    "build:es2015modules": "NODE_ENV=production BABEL_ENV=modules babel ./src --out-dir ./dist/esm --ignore **.test.js,**/__tests__/**,**/e2e.js,template-component/**",
    "build:es": "NODE_ENV=production BABEL_ENV=es babel ./src --out-dir ./dist/es --ignore **.test.js,**/__tests__/**,**/e2e.js,template-component/**",
    "build": "yarn run generate-icons && yarn run build:es2015 && yarn run build:es2015modules && yarn run build:es",
    "publish:npm": "cd ./dist/ && npm publish",
    "postpublish:npm": "PACKAGE_VERSION=$(cat package.json | grep \\\"version\\\" | head -1 | awk -F: '{ print $2 }' | sed 's/[\",]//g' | tr -d '[[:space:]]') && git tag -a v$PACKAGE_VERSION && git push zomato_origin --tags"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-stage-2": "^7.0.0",
    "@babel/runtime": "^7.2.0",
    "@pickra/copy-code-block": "^1.1.2",
    "@storybook/addon-a11y": "^5.0.6",
    "@storybook/addon-actions": "^5.0.6",
    "@storybook/addon-backgrounds": "^5.0.6",
    "@storybook/addon-centered": "^5.0.6",
    "@storybook/addon-info": "^5.0.6",
    "@storybook/addon-knobs": "^5.0.6",
    "@storybook/addon-viewport": "^5.0.11",
    "@storybook/addons": "^5.0.5",
    "@storybook/react": "^5.0.5",
    "@storybook/theming": "^5.0.8",
    "autoprefixer": "^9.5.1",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-dynamic-import-webpack": "^1.1.0",
    "babel-plugin-styled-components": "^1.10.0",
    "babel-plugin-uglify": "^1.0.2",
    "classnames": "^2.2.6",
    "clean-webpack-plugin": "^1.0.0",
    "compression-webpack-plugin": "^2.0.0",
    "copy-webpack-plugin": "^4.6.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^1.6.0",
    "glob": "^7.1.3",
    "glob-fs": "^0.1.7",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "hygen": "^4.0.2",
    "lint-staged": "^8.1.5",
    "lodash-es": "^4.17.11",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "4.11.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.17.1",
    "prettier-eslint": "^8.8.2",
    "react": "^16.8.5",
    "react-dom": "^16.8.5",
    "react-storybook-addon-chapters": "^3.1.3",
    "shx": "^0.3.2",
    "storybook-addon-jsx": "^7.1.0",
    "storybook-readme": "^5.0.2",
    "style-loader": "^0.23.1",
    "styled-components": "^4.2.0",
    "styled-normalize": "^8.0.6",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.1",
    "webpack-combine-loaders": "^2.0.4",
    "yarn": "^1.15.2"
  },
  "dependencies": {
    "babel-preset-minify": "^0.5.0",
    "cuid": "^2.1.6",
    "json-loader": "^0.5.7",
    "parse5": "^5.1.0",
    "prop-types": "^15.7.2",
    "react-date-range": "^1.0.0-beta",
    "react-highlight": "^0.12.0",
    "react-input-range": "^1.3.0",
    "react-lazy-load-image-component": "^1.3.2",
    "react-onclickoutside": "^6.8.0",
    "react-slick": "^0.24.0",
    "slick-carousel": "^1.8.1"
  },
  "peerDependencies": {
    "lodash-es": ">=  4",
    "react": "> 16.8.0",
    "react-dom": "> 16.8.0",
    "styled-components": ">= 4"
  }
}
